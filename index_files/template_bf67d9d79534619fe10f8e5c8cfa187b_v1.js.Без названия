; /* Start:"a:4:{s:4:"full";s:65:"/local/templates/v22/assets/html_css/index.bundle.js?163412600676";s:6:"source";s:52:"/local/templates/v22/assets/html_css/index.bundle.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
(window.webpackJsonp=window.webpackJsonp||[]).push([[1],[],[["ROtW",2,0]]]);
/* End */
