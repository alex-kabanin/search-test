                        for (var i = 1; i <= digits; i++)
                            isFinite(buffer[radixPosition + i]) ||
                                (buffer[radixPosition + i] = "0");
                    }
                    return (
                        negationBack && buffer.push(opts.negationSymbol.back),
                        buffer
                    );
                }
                function findValidator(symbol, maskset) {
                    var posNdx = 0;
                    if ("+" === symbol) {
                        for (posNdx in maskset.validPositions);
                        posNdx = parseInt(posNdx);
                    }
                    for (var tstNdx in maskset.tests)
                        if (((tstNdx = parseInt(tstNdx)), posNdx <= tstNdx))
                            for (
                                var ndx = 0,
                                    ndxl = maskset.tests[tstNdx].length;
                                ndx < ndxl;
                                ndx++
                            )
                                if (
                                    (void 0 ===
                                        maskset.validPositions[tstNdx] ||
                                        "-" === symbol) &&
