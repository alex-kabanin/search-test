                                      analyseMask(
                                          maskedValue,
                                          opts.inputFormat,
                                          opts
                                      ),
                                      opts,
                                      !0
                                  )
                                : unmaskedValue;
                        },
                        casing: function casing(
                            elem,
                            test,
                            pos,
                            validPositions
                        ) {
                            return 0 == test.nativeDef.indexOf("[ap]")
                                ? elem.toLowerCase()
                                : 0 == test.nativeDef.indexOf("[AP]")
                                ? elem.toUpperCase()
                                : elem;
                        },
                        onBeforeMask: function onBeforeMask(
                            initialValue,
                            opts
                        ) {
                            return (
