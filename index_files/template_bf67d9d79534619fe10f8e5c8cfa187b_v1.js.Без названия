                                          maskToken.matches[match]
                                      ));
                            }
                        return maskToken;
                    }
                    function groupify(matches) {
                        var groupToken = new MaskToken(!0);
                        return (
                            (groupToken.openGroup = !1),
                            (groupToken.matches = matches),
                            groupToken
                        );
                    }
                    function closeGroup() {
                        if (
                            ((openingToken = openenings.pop()),
                            (openingToken.openGroup = !1),
                            void 0 !== openingToken)
                        )
                            if (0 < openenings.length) {
                                if (
