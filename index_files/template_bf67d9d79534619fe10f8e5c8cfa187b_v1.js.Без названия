                    function reverseTokens(maskToken) {
                        function reverseStatic(st) {
                            return (
                                st === opts.optionalmarker[0]
                                    ? (st = opts.optionalmarker[1])
                                    : st === opts.optionalmarker[1]
                                    ? (st = opts.optionalmarker[0])
                                    : st === opts.groupmarker[0]
                                    ? (st = opts.groupmarker[1])
                                    : st === opts.groupmarker[1] &&
                                      (st = opts.groupmarker[0]),
