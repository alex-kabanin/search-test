                    }
                    function groupQuantifier(matches) {
                        var lastMatch = matches.pop();
                        return (
                            lastMatch.isQuantifier &&
                                (lastMatch = groupify([
                                    matches.pop(),
                                    lastMatch,
                                ])),
                            lastMatch
                        );
