                    function verifyGroupMarker(maskToken) {
                        maskToken &&
                            maskToken.matches &&
                            maskToken.matches.forEach(function (token, ndx) {
                                var nextToken = maskToken.matches[ndx + 1];
                                (void 0 === nextToken ||
