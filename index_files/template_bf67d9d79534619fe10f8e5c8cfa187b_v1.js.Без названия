                        opts.alternatormarker === txt.charAt(i)
                            ? (escapedTxt += "\\" + txt.charAt(i))
                            : (escapedTxt += txt.charAt(i));
                    return escapedTxt;
                }
                function alignDigits(buffer, digits, opts, force) {
                    if (
                        0 < buffer.length &&
                        0 < digits &&
                        (!opts.digitsOptional || force)
                    ) {
                        var radixPosition = buffer.indexOf(opts.radixPoint),
                            negationBack = !1;
