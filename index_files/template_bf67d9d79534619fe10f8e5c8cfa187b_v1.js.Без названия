window.sp_global.v22.scriptForPage = {

    exec: function(page) {
        // window.sp_global.v22.scriptForPage.exec('index');

        var that = this;

        $(document).ready(function(){
            that[ page ]();
        });

    }, // function

    personal_orders__btn_remove_click: function(obj) {

        function fLog(msg, label) {
            window.sp_global.fLog(msg, label);
        }

        var v22 = window.sp_global.v22;

        if (!v22.fSetProcessing(true)) {
            return;
        }

        var hrefSuccess = obj.data('href-success'),
            data = {
                oper: 'removeOrder',
                id:   obj.data('id'),
            };

        fLog(data, 'data');

        v22.fAjax({
            url: '/local/bitrix_ajax/v22.php',
            data: data,
            fSuccess: function(response) {
                //fLog(response);

                v22.fSetProcessing(false);

                if (!hrefSuccess) {
                    // personal_orders_list
                    window.location.reload();
                } else {
                    // personal_orders_detail
                    window.location.assign(hrefSuccess);
                }
            }
        });

    }, // function

    personal_orders__btn_renewal_click: function(obj, params={}) {

        function fLog(msg, label) {
            window.sp_global.fLog(msg, label);
        }

        var v22 = window.sp_global.v22;

        if (obj.hasClass('disabled') || !v22.fSetProcessing(true)) {
            return;
        }

        var objMsg = (!params.flagDetail) ? obj.parent().find('.global-msg') : obj.find('.global-msg'),
            data = {
                format:   'json',
                ORDER_ID: obj.data('id'),
            };

        fLog(data, 'data');

        v22.fAjax({
            url: '/local/bitrix_ajax/order_renewal.php',
            data: data,
            fSuccess: function(response) {
                fLog(response);

                v22.fSetProcessing(false);

                obj.addClass('disabled');

                if (params.flagDetail) {
                    params.objMain.find('.msg-storage-date').addClass('m-hidden');
                    params.objMain.find('.msg-storage-date-renewed').removeClass('m-hidden');
                }

                v22.fShowGlobalMsg(objMsg);
            }
        });

    }, // function

    catalog_section: function() {

        function fLog(msg, label) {
            window.sp_global.v22.fLog(msg, label);
        }

        var v22     = window.sp_global.v22,
            that    = this,
            store   = this.catalog_section__getStore(),
            storeId = store.id;

        //fLog(storeId, 'storeId');
        //fLog(v22.dataMain.basketProductIdAndQuantity, 'basketProductIdAndQuantity');

        $('.js-catalog-section .js-catalog-item').each(function(){
            that.catalog_section__itemBindHandlers({
                objItem:      $(this),
                storeId:      store.id,
                storeName:    store.name,
                flagSppStore: true,
            });
        });
    
        // # Фильтр, сортировка (
        if (!storeId) {
            $('.js-filter_store').closest('.medicine-control__checkbox-list-item').hide();
        }

        $('.js-filter_store_24h, .js-filter_can_buy, .js-filter_store').change(function(){
            var url = ($(this).prop('checked')) ? $(this).data('ref-on') : $(this).data('ref-off');
            window.location.assign(url);
        });

        $('.js-sort').change(function(){
            window.location.assign( $(this).val() );
        });
        // # )

        $('#itemCategoriesSm').html( $('#itemCategories').html() );

        // # Объединенные товары (
        var obj = $('.block-union-products');
        if (obj.length) {
            var objScroll = obj.find('.button-radio__radio'),
                scroll    = objScroll.width() - 100;

            obj.find('.radio_btn.left').click(function(){
                objScroll.animate({scrollLeft: ('-=' + scroll)}, 500);
            });
            obj.find('.radio_btn.right').click(function(){
                objScroll.animate({scrollLeft: ('+=' + scroll)}, 500);
            });
        }
        // # )

        //$('.js-catalog-section .js-btn-catalog-stores').first().click();

    }, // function

    catalog_section__getStore: function() {
        // Выбранный склад
        return {
            id:   $('.js-btn-store-selection').data('id'),
            name: $('.js-store-name').html(),
        };
    }, // function

    catalog_section__itemBindHandlers: function(params) {
        /*  catalog_section__itemBindHandlers({
                objItem:      $(this),
                storeId:      store.id,
                storeName:    store.name,
                flagSppStore: true,
            })
        */

        function fLog(msg, label) {
            window.sp_global.v22.fLog(msg, label);
        }
    
        var v22 = window.sp_global.v22,

            storeId    = params.storeId,
            storeName  = params.storeName,
            msgInStore = 'В наличии',

            basketProductIdAndQuantity = v22.dataMain.basketProductIdAndQuantity,
        
            objItem      = params.objItem,
            productId    = objItem.data('id'),
            storeList    = objItem.data('store_list'),
            is_wholesale  = objItem.data('is-wholesale'),
            flagSppStore = false;

        //fLog(storeList, 'storeList ' + productId);

        // ### spp-store
        if (params.flagSppStore && storeId) {
            var dataPrice = objItem.find('.js-data-price').html();

            if (dataPrice) {
                dataPrice = JSON.parse(dataPrice);

                if (dataPrice[ storeId ]) {
                    var price = dataPrice[ storeId ];
                    
                    objItem.find('.main-card__price--main span').html( price['PRICE_STR'] );
                    objItem.find('.product-detailed__bonuses span').html( price['BONUS_PRIHOD'] + ' ' + price['BONUS_PRIHOD_STR']); // page detail
                    objItem.addClass('spp-store');
                    
                    flagSppStore = true;
                }
            } //
        } //

        // ### js-block-product-for-store-list: price (
        var objPrice  = objItem.find('.js-block-price'),
            objPrice2 = objItem.find('.js-block-product-for-store-list .hor-card__price');

        if (objPrice.length) {
            objPrice2.html(objPrice.html());
            if (flagSppStore) {
                objPrice2.addClass('spp-store');
            }
        }
        // ### )

        // ### button buy (
        var objBtnBuy   = objItem.find('.js-btn-buy-container'),
            quantityMax = objItem.data('quantity_max'),
            quantity    = (basketProductIdAndQuantity[ productId ]) ? basketProductIdAndQuantity[ productId ] : 0;

        if (objBtnBuy.length) {
            v22.btnBuy.fAddHandler_BtnBuy({
                objBtnBuyContainer: objBtnBuy,
                quantityMax:        quantityMax,
                quantity:           quantity,
                productId:          productId,
                searchString:       objItem.data('search_string'),
                is_wholesale:       is_wholesale,
            });
        } //
        // ### )

        // В наличии в выбранной аптеке
        if (storeId && storeList.length && storeList.includes(storeId)) {
            objItem.find('.js-text-in-store').html(msgInStore);
            objItem.find('.js-text-in-store-count').html(storeName);
        }

        // Модальное окно "Список аптек"
        if (storeList.length && objBtnBuy.length) {
            objItem.find('.js-btn-catalog-stores').click(function(){

                //Генерация товара для попап окна на лету 
                let productPopap = $(v22.generateDeliveryProductPopap(objItem));

                v22.fShowCatalogStores({
                    storeList:       storeList,
                    objBlockProduct: productPopap,
                    objBtnBuy:       objBtnBuy,
                    productId:       productId,
                });
            });
        } //

        // Модальное окно "Доставка"
        if (objBtnBuy.length) {
            objItem.find('.js-btn-catalog-delivery').click(function(){

                //Генерация товара для попап окна на лету 
                let productPopap = $(v22.generateDeliveryProductPopap(objItem));

                v22.fShowCatalogDelivery({
                    objBlockProduct: productPopap,
                    objBtnBuy:       objBtnBuy,
                    productId:       productId,
                });
            });
        } //

        objItem.find('.js-btn-subscription').click(function(){
            v22.fBtnSubscriptionClick({ product_id: productId });
        });

        /* objItem.find('.js-btn-wholesale-order').click(function(){
            v22.fBtnWholeSaleOrderClick();
        }); */

        objItem.find('.js-btn-special-order').click(function(){
            v22.fBtnSpecialOrderClick({ product_id: productId });
        });

    }, // function

    catalog_section_h__bindHandlers: function() {
        
        var that  = this,
            store = this.catalog_section__getStore();

        $('.block-catalog-section-h .js-catalog-item').each(function(){
            that.catalog_section__itemBindHandlers({
                objItem:      $(this),
                storeId:      store.id,
                storeName:    store.name,
                flagSppStore: false,
            });
        });
    
    }, // function

    catalog_element: function() {

        function fLog(msg, label) {
            window.sp_global.v22.fLog(msg, label);
        }

        var v22 = window.sp_global.v22,
            store     = this.catalog_section__getStore(),
            objItem   = $('.page-catalog-detail'),
            storeList = objItem.data('store_list');

        this.catalog_section__itemBindHandlers({
            objItem:      objItem,
            storeId:      store.id,
            storeName:    store.name,
            flagSppStore: true,
        });
    
        // #####

        var objMain = $('.page-catalog-detail-info'),
            flag_fInitSelectStore = false;

        objMain.find('.product_tabs_nav-btn').click(function(){
            var id = $(this).data('id');

            objMain.find('.m-tab').removeClass('active');
            objMain.find('.m-tab[data-id='+ id +']').addClass('active');

            if (id == 'stores' && !flag_fInitSelectStore) {
                flag_fInitSelectStore = true;
                v22.fInitSelectStore({source: 'pageCatalogDetail', objMain: $('.js-block-store-selection--page-catalog-detail')});
            }
        });

        var objInfo2 = objMain.find('.tab_info .info-2');

        if (objInfo2.length) {
            objInfo2.find('.b-section-list .section-name').click(function(event){
                event.preventDefault();

                objInfo2.find('.b-section-list .section-name').removeClass('active');
                $(this).addClass('active');

                $( $(this).attr('href') )[0].scrollIntoView({
                    behavior: 'smooth',
                    block:    'nearest',
                    inline:   'nearest',
                });
            });

            objInfo2.find('.b-content .section').click(function(){
                if ($(this).hasClass('active')) {
                    $(this).removeClass('active');
                } else {
                    objInfo2.find('.b-content .section').removeClass('active');
                    $(this).addClass('active');
                }
            });

            objInfo2.find('.js-scroll-container').scrollbar();
        } //

        objMain.find('.block-catalog-section-h').slick({
            infinite:       false,
            slidesToShow:   6,
            slidesToScroll: 6,
            variableWidth:  true,
            arrows:         false,

            responsive: [
                {
                    breakpoint: 768,
                    settings: 
                    {
                        slidesToShow: 1,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 432,
                    settings: {
                        slidesToScroll: 1,
                        centerMode:   true,
                    }
                }
            ],
        });      
        // https://kenwheeler.github.io/slick/

        this.catalog_section_h__bindHandlers();
        
        // ### "В наличии" (
        var objStoreList    = objMain.find('.js-block-store-selection--page-catalog-detail'),
            objStoreListTMP = $('.js-block-store-list-tmp');
        
        if (objStoreList.length && objStoreListTMP.length) {
            
            objStoreListTMP.find('.item').each(function(){
                var obj     = $(this),
                    storeId = obj.data('id');

                if (!storeList.includes(storeId)) {
                    obj.remove();
                }
            });
        
            objStoreList.html( objStoreListTMP.html() );
            objStoreListTMP.remove();
        }
        // ### )

        if (navigator.share) {
            
            $('.share-wrapper .btn-mobile').click(function(){
                var title = $(this).data('title'),
                    url   = $(this).data('url');
        
                navigator.share({
                    title: title,   // chrome, firefox: по почте "тема"
                    text:  title,   // chrome: почта (тело письма), whatsapp
                    url:   url,
                });
            });
        
            $('.share-wrapper').addClass('mobile');
        } //
        
        //objMain.find('.product_tabs_nav-btn[data-id=stores]').click();
        
    }, // function

    catalog_search: function() {

        $('.page-search .block-search-msg .js-btn-help').click(function(){
            $('.page-search .block-search-msg .help-text').toggleClass('m-hidden');
        });

        $('.page-search .block-search-msg .js-btn-special-order').click(function(){
            window.sp_global.v22.fBtnSpecialOrderClick({product_str: $(this).data('product_str')});
        });

    }, // function

    unionproduct_index: function() {
        
        $('.unionproduct-index a').click(function(e){
            e.stopPropagation();
        });

        $('.unionproduct-index .group').click(function(){
            $(this).toggleClass('open');
            //$(this).find('.items').slideToggle();
        });

    }, // function

    personal_favorite: function() {

        var fShowMessage = window.sp_global.v22.fShowMessage;

        function fLog(msg, label) {
            //window.sp_global.v22.fLog(msg, label);
        }
        
        var v22 = window.sp_global.v22,

            objMain = $('.page-favorite');
        
        objMain.find('.js-btn-fav-remove').click(function(){
            if (!v22.fSetProcessing(true)) {
                return;
            }
    
            v22.fAjax({
                url: '/local/bitrix_ajax/v22.php',
                data: {
                    oper: 'setProductFavorite',

                    'AJAX_CALL_FAVORITES_ADD':  'Y',
                    'h2o_add_favorites':        'Y',
                    'H2O_FAVORITES_ELEMENT_ID': $(this).closest('.js-catalog-item').data('id'),
                },
                fSuccess: function(response) {
                    fLog(response);

                    v22.fSetProcessing(false);

                    if (response.DELETE) {
                        window.location.reload();
                    } else {
                        fShowMessage('Ошибка'); // Не должно
                    }
                }
            });

        });

    }, // function

    index: function() {

        function fLog(msg, label) {
            window.sp_global.v22.fLog(msg, label);
        }

        var v22 = window.sp_global.v22;

        let objSlider = $('.js-main-page-slider');

        objSlider.find('.js-slider').slick({
            autoplay: false,
            dots: true,
        
            prevArrow: objSlider.find('.main_page_banner_btns .left'),
            nextArrow: objSlider.find('.main_page_banner_btns .right'),
        });
        
        let bannerElement = $(".js-main-page-slider .js-slider");
        let bannerRadialButtonNext = new BannerRadialButton({
            mode: "next",
            bannerElement: bannerElement,
            bannerProgressElement: $(".main_page_banner-progress"),
            buttonElement: $(".banner-radial-button.right"),
            buttonProgressElement: $(".banner-radial-button.right .banner-radial-button__progress")
        });
        
        // Запускать таймер если количество слайдов больше 1
        if (bannerElement.slick("getSlick").slideCount > 1) {
            bannerRadialButtonNext.startTimer();
        }
        
        // Обнуление прогресса при нажатий на кнопки (налево, направо) баннера 
        document.querySelectorAll(".banner-radial-button.left, .banner-radial-button.right").forEach(button => {
            button.addEventListener("click", function () {
                bannerRadialButtonNext.clearProgress();
            });
        })
        
        // Обнуление прогресса при нажатий на кнопки (индивидуальные слайдеры) баннера 
        document.querySelectorAll(".js-slider .slick-dots button").forEach(button => {
            button.addEventListener("click", function () {
                bannerRadialButtonNext.clearProgress();
            });
        })

        // # "Актуальные товары" (
        var objMain         = $('.page-main .union-products'),
            arObjTab        = objMain.find('.js-tab'),
            arObjTabContent = objMain.find('.m-tab-content'),
            objActiveSlider = arObjTabContent.filter('.active').find('.block-catalog-section-h'),
            slidesToShow    = Math.floor( $('.page-main').width() / 280 );

        arObjTab.click(function(){
            arObjTab.removeClass('active');
            $(this).addClass('active');

            arObjTabContent.removeClass('active');

            var obj = arObjTabContent.filter('[data-id='+ $(this).data('id') +']');
            obj.addClass('active');
            objActiveSlider = obj.find('.block-catalog-section-h');
        });

        objMain.find('.block-catalog-section-h').slick({
            infinite:       false,
            slidesToShow:   slidesToShow,
            slidesToScroll: slidesToShow,
            variableWidth:  true, // Иначе если меньше slidesToShow элементов, то некорректно отображается

            prevArrow: objMain.find('.block-template__prev'),
            nextArrow: objMain.find('.block-template__next'),

            responsive: [
                {
                    breakpoint: 768,
                    settings: 
                    {
                        slidesToShow: 1,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 432,
                    settings: {
                        slidesToScroll: 1,
                        centerMode:   true,
                    }
                }
            ],
        });
        // # )

        // # "Акции и скидки" (
        var objMain     = $('.page-main .block-shares'),
            arObjSlider = objMain.find('.block-catalog-section-h');
        
        arObjSlider.slick({
            infinite:       false,
            slidesToShow:   slidesToShow,
            slidesToScroll: slidesToShow,
            variableWidth:  true,

            prevArrow: objMain.find('.block-template__prev'),
            nextArrow: objMain.find('.block-template__next'),

            responsive: [
                {
                    breakpoint: 768,
                    settings: 
                    {
                        slidesToShow: 1,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 432,
                    settings: {
                        slidesToScroll: 1,
                        centerMode:   true,
                    }
                }
            ],
        });

        objMain.find('.js-slider-ad').slick({
            autoplay:      true,
            autoplaySpeed: 6000,
            dots:          true,
            prevArrow:     false, 
            nextArrow:     false,
        });
        // # )

        // # "Ваше персональное предложение" (
        var objMain = $('.page-main .block-similar-products');

        objMain.find('.block-catalog-section-h').slick({
            infinite:       false,
            slidesToShow:   slidesToShow,
            slidesToScroll: slidesToShow,
            variableWidth:  true,
            
            prevArrow: objMain.find('.block-template__prev'),
            nextArrow: objMain.find('.block-template__next'),

            responsive: [
                {
                    breakpoint: 768,
                    settings: 
                    {
                        slidesToShow: 1,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 432,
                    settings: {
                        slidesToScroll: 1,
                        centerMode:   true,
                    }
                }
            ],
        });
        // # )
            
        // # "Новинки" (
        var objMain = $('.page-main .block-new-product');
        
        objMain.find('.block-catalog-section-h').slick({
            infinite:       false,
            slidesToShow:   slidesToShow,
            slidesToScroll: slidesToShow,
            variableWidth:  true,
            
            prevArrow: objMain.find('.block-template__prev'),
            nextArrow: objMain.find('.block-template__next'),

            responsive: [
                {
                    breakpoint: 768,
                    settings: 
                    {
                        slidesToShow: 1,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 432,
                    settings: {
                        slidesToScroll: 1,
                        centerMode:   true,
                    }
                }
            ],
        });
        // # )
        
        this.catalog_section_h__bindHandlers();

        // # Мобильная: список элементов "Объединенные товары" (
        var objMain    = $('.page-main .union-products-mobile .js-items'),
            arObjItems = objMain.find('.js-item'),
            objRow_1   = $(objMain.find('.m-row')[0]),
            objRow_2   = $(objMain.find('.m-row')[1]),

            len   = 0,
            len_2 = 0;

        arObjItems.each(function(){
            len += $(this).width();
        });
        len = len / 2;

        arObjItems.each(function(){
            len_2 += $(this).width();
            if (len_2 < len) {
                $(this).detach().appendTo(objRow_1);
            } else {
                $(this).detach().appendTo(objRow_2);
            }
        });

        objMain.addClass('type-2');
        // ) #

        // # Мобильная: Акции, Новости (
        function fSetImageHeight(arObj) {
            if (arObj.length) {
                var h = Math.floor(arObj.first().width() * .69);
                if (h) {
                    arObj.css('height', h + 'px');
                }
            }
        } //
        //fSetImageHeight( $('.page-main .block-promotions-mobile .item .b-image') );
        //fSetImageHeight( $('.page-main .block-news-mobile .item .b-image') );
        // # )

        var objModal = $('.modal-survey-q-popup');
        if (objModal.length) {
            $.fancybox.open( objModal, {smallBtn: false} );

            objModal.find('.js-btn-send').click(function(){

                if (!v22.fSetProcessing(true)) {
                    return;
                }
    
                var data = {
                        orderId: objModal.data('order'),
                    };
        
                fLog(data, 'SurveyQualityPopup');
        
                v22.fAjax({
                    url:          '/local/components/sp-artgroup/survey_quality_popup/ajax.php',
                    data:         data,
                    showErrorMsg: 'error_msg',

                    fSuccess: function(response) {
                        fLog(response);

                        v22.fSetProcessing(false);

                        $.fancybox.close();
                    },
                });
            });
        } //

    }, // function

    about: function() {
        
        var objMain      = $('.page-about--license'),
            objContent   = objMain.find('.block-slick'),
            slidesToShow = Math.floor( objContent.width() / objContent.find('div:first-child').width() ); // 210

        objContent.slick({
            infinite:       false,
            slidesToShow:   slidesToShow,
            slidesToScroll: slidesToShow,
            //variableWidth:  true,
            
            prevArrow: objMain.find('.block-template__prev'),
            nextArrow: objMain.find('.block-template__next'),
        });
    
    }, // function

} // object

/* End */
;
; /* Start:"a:4:{s:4:"full";s:60:"/local/templates/v22/assets/js/page_order.js?171204128356515";s:6:"source";s:44:"/local/templates/v22/assets/js/page_order.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
window.sp_global.v22.scriptForPageOrder = function() {

    var fShowMessage = window.sp_global.v22.fShowMessage;

    function fLog(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    }

    var v22     = window.sp_global.v22,
        urlAjax = '/local/components/sp-artgroup/order_2/ajax_v22.php',

        objMain         = $('.page-order'),
        objSidebar      = $('.sidebar'),
        objTplContainer = $('.tpl-2-personal-container'),
        
        timer = {
            id:           null,
            basketItemId: 0,     // Данный элемент корзины можно редактировать (количество и "удалить") пока запрос не отправлен
            delay:        1000,
        },

        stepStoreTotalSum = false,
        
        nothing;

    // ###
    
    function fAjax(params) {
        /*
            fAjax({
                data: data,
                fSuccess: function(response) {
                }
            })
        */

        $.ajax({
            url:      urlAjax,
            type:     'POST',
            dataType: 'html',
            data:     params.data,
            success: function (response) {
                //fLog(response);
                params.fSuccess(response);
            },
            error: function () {
                timer.id = null;
                v22.fSetProcessing(false);
                fShowMessage('Нет соединения с сервером (код ошибки: 41)'); // # ajax error #
            }
        });
    } // function

    function fRender(response, params) {
        var objTMP = $('<div></div>');
        objTMP.html(response);

        var objPageOrder = objTMP.find('.page-order'),
            objData      = objTMP.find('#data'),
            objSimilar   = objTMP.find('.page-order--block-similar-products');

        if (!objPageOrder.length) {
            // В response нет контента
            fLog(response);
            var msg = 'Ошибка 1';
            try {
                var data = JSON.parse(response);
                fLog(data);
                if (data.error_msg == undefined) {
                    // Неизвестный json (не содержит "error_msg")
                    msg = 'Ошибка 3';
                } else {
                    msg = 'Ошибка: ' + data.error_msg;
                }
            } catch (e) {
                // Не json
                msg = 'Ошибка 2';
            }
            fShowMessage(msg);
            return;
        } //

        // Для отладки
        if (objData.length) {
            var data = JSON.parse(objData.html());
            fLog(data, 'debugInfo');
        }

        // Обновляем содержимое
        objMain.html( objPageOrder.html() );

        if (!params || !params.doNotBindEventHandlers) {
            fBindHandlers();
            v22.favoriteProduct.fApply(objMain);
        } //

        // Индивидуальные предложения
        if (objPageOrder.data('keep_similar')) {
            fLog('similar: keep');
        } else {
            let parentOfSimilarProducts = $('.page-order--block-similar-products').parent();
            $('.page-order--block-similar-products').remove();

            if (objSimilar.length) {
                fLog('similar: update');
                parentOfSimilarProducts.append(objSimilar);
                fBindHandlersSimilar();
                v22.favoriteProduct.fApply(objSimilar);
            }
        }

        var step = objPageOrder.data('step');
        if (step == 'StepOutOfStock') {
            fHideStep();
            objMain.find('.js-step-out-of-stock').addClass('active');
        } else if (step == 'StepPreview') {
            fHideStep();
            objMain.find('.js-step-preview').addClass('active');
        }

    } // function

    function fHideStep() {
        objMain.find('.block-step').removeClass('active');
        objSidebar.addClass('m-hidden');
        $('.page-order--block-similar-products').addClass('m-hidden');
        $(document).scrollTop(0);
    } // function

    function fBindHandlersQuantity(objQuantity, params) {
        /*  fBindHandlersQuantity(objQuantity, {
                basketItemId:    basketItemId,
                maxQuantity:     maxQuantity,
                quantityInitial: quantityInitial,
                quantityPrev:    quantityPrev,

                source:          'stepOutOfStock',
                mainStoreId:     mainStoreId,
            })
        */
        var basketItemId    = params.basketItemId,
            maxQuantity     = params.maxQuantity,
            quantityInitial = params.quantityInitial,
            quantityPrev    = params.quantityPrev;

        function fObjQuantity_Change(k) {
            var objInput = objQuantity.find('.js-value'),
                value    = parseInt(objInput.val());
    
            if (v22.fIsProcessing() || (timer.id && timer.basketItemId != basketItemId)) {
                fLog('Processing or timer');
                objInput.val(quantityPrev);
                return;
            }

            if (isNaN(value)) {
                value = 1;
            } else {
                value += k;
                value = Math.max(1, value);
            }
    
            if (value > maxQuantity) {
                value = maxQuantity;
                v22.fShowGlobalMsg( objQuantity.find('.global-error-msg') );
            }
    
            objInput.val(value);

            if (value == quantityPrev) {
                fLog('value == quantityPrev');
                return;
            }

            quantityPrev = value;

            if (timer.id) {
                clearTimeout(timer.id);
                fLog('stop timer');
            }

            if (quantityInitial == value) {
                fLog('quantityInitial == value');
                timer.id = null;
                return;
            }

            // Заводим таймер

            fLog('start timer; value=' + value);

            timer.basketItemId = basketItemId;

            timer.id = setTimeout(function(){

                if (!v22.fSetProcessing(true)) {
                    fLog('timer: processing: cancel');
                    quantityPrev = quantityInitial;
                    objInput.val(quantityInitial);

                    timer.id = null;
                    return;
                }

                var data = {
                        oper:         'v22_SetQuantity',
                        basketItemId: basketItemId,
                        quantity:     value,
                    };
        
                if (params.source == 'stepOutOfStock') {
                    data.source      = 'stepOutOfStock';
                    data.mainStoreId = params.mainStoreId;
                }

                fLog(data, 'quantity');

                $.fancybox.close();

                fAjax({
                    data: data,
                    fSuccess: function(response) {
                        fRender(response);
                        timer.id = null;
                        v22.fSetProcessing(false);
                    }
                });
            }, timer.delay);

        } // function

        objQuantity.find('.js-value').change(function(){
            fObjQuantity_Change(0);
        });

        objQuantity.find('.js-btn-plus').click(function(){
            fObjQuantity_Change(1);
        });

        objQuantity.find('.js-btn-minus').click(function(){
            fObjQuantity_Change(-1);
        });

        // Удалить
        objQuantity.find('.js-btn-remove').click(function(){
            fRemoveItem(params);
        });
        
    } // function

    function fRemoveItem(params) {
        /*  fRemoveItem({
                basketItemId: basketItemId,

                source:       'stepOutOfStock',
                mainStoreId:  mainStoreId,
            });
        */
        var basketItemId = params.basketItemId;

        if ((timer.id && timer.basketItemId != basketItemId) || !v22.fSetProcessing(true)) {
            fLog('Processing or timer');
            return;
        }

        if (timer.id) {
            clearTimeout(timer.id);
            timer.id = null;
            fLog('stop timer');
        }

        var data = {
                oper:         'v22_RemoveItem',
                basketItemId: basketItemId,
            };
            
        if (params.source == 'stepOutOfStock') {
            data.source      = 'stepOutOfStock';
            data.mainStoreId = params.mainStoreId;
        }

        fLog(data, 'remove item');

        $.fancybox.close();

        fAjax({
            data: data,
            fSuccess: function(response) {
                fRender(response);
                v22.fSetProcessing(false);
            }
        });
    } // function

    function fBindHandlers() {

        // Список товаров
        objMain.find('.order-products .js-catalog-item').each(function(){
            var objItem     = $(this),
                objQuantity = objItem.find('.js-quantity-container'),
                objBlockProductForStoreList = objItem.find('.js-block-product-for-store-list'),

                productId       = objItem.data('product_id'),
                basketItemId    = objItem.data('basket_item_id'),
                storeList       = objItem.data('store_list'),

                maxQuantity     = objItem.data('quantity_max'),
                quantityInitial = objQuantity.find('.js-value').val(),
                quantityPrev    = quantityInitial,

                canBuy = Boolean(objQuantity.length),

                nothing;

            // js-block-product-for-store-list: price, quantity
            if (canBuy) {
                var objPrice     = objItem.find('.js-block-price'),
                    objPrice2    = objBlockProductForStoreList.find('.hor-card__price'),
                    objQuantity2 = objBlockProductForStoreList.find('.js-btn-buy-container-modal');

                objPrice2.html( objPrice.html() );
                objQuantity2.html( objQuantity.html() );
            }

            var params_fBindHandlersQuantity = {
                    basketItemId:    basketItemId,
                    maxQuantity:     maxQuantity,
                    quantityInitial: quantityInitial,
                    quantityPrev:    quantityPrev,
                };
            
            if (objQuantity.length) {
                fBindHandlersQuantity(objQuantity, params_fBindHandlersQuantity);
            } else {
                objItem.find('.js-btn-remove').click(function(){
                    fRemoveItem({basketItemId: basketItemId});
                });
            }

            // Модальное окно "Доставка"
            objItem.find('.js-btn-catalog-delivery').click(function(){

                if (timer.id || v22.btnBuy.timer.id) {
                    fLog('wait');
                    return;
                }

                var objModal = $('.modal-catalog-delivery');

                objModal.find('.js-block-product').html( objBlockProductForStoreList.html() );

                fBindHandlersQuantity( objModal.find('.js-block-product .js-btn-buy-container-modal'), params_fBindHandlersQuantity );

                $.fancybox.open(objModal);
            });

            // Модальное окно "Список аптек"
            objItem.find('.js-btn-catalog-stores').click(function(){

                if (timer.id || v22.btnBuy.timer.id) {
                    fLog('wait');
                    return;
                }

                var objModal = $('.catalog-store-list');

                objModal.find('.js-block-product').html( objBlockProductForStoreList.html() );

                fBindHandlersQuantity( objModal.find('.js-block-product .js-btn-buy-container-modal'), params_fBindHandlersQuantity );

                v22.fShowCatalogStores({
                    storeList:     storeList,
                    flagPageOrder: true,
                });
            });

            objItem.find('.js-btn-subscription').click(function(){
                v22.fBtnSubscriptionClick({ product_id: productId });
            });
    
            objItem.find('.js-btn-special-order').click(function(){
                v22.fBtnSpecialOrderClick({ product_id: productId });
            });
                
        });

        // Пакеты (упаковка)
        objMain.find('.block-step-first .block-bag-list .item:not(.active)').each(function(){

            var obj       = $(this),
                productId = obj.data('product_id');

            obj.find('.b-checkbox').click(function(){

                if (timer.id || !v22.fSetProcessing(true)) {
                    fLog('Processing or timer');
                    return;
                }
    
                var data = {
                        oper:      'basketAdd',
                        productId: productId,
                        quantity:  1,
                    };
                
                fLog(data, 'add package');

                v22.fAjax({
                    url:  '/local/bitrix_ajax/v22.php',
                    data: data,
                    fSuccess: function(response) {
                        fLog(response);
                        v22.fSetProcessing(false);
                        v22.fOrderRefresh();
                    }
                });
            });
        });

        // Очистить корзину
        objMain.find('.block-template__clear').click(function(){
    
            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                    oper:         'v22_RemoveItem',
                    basketItemId: 'all_delete',
                };
    
            fLog(data, 'remove all items');

            fAjax({
                data:     data,
                fSuccess: function(response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });
        });

        // Купон: активировать
        objMain.find('.js-btn-set-coupon').click(function(){
            var coupon = objMain.find('.js-coupon').val();
            coupon = coupon.trim();

            if (coupon.search(/^[а-яА-ЯёЁa-zA-Z0-9\-]{3,}$/) == -1) {
                objMain.find('.js-coupon').addClass('m-error');
                objMain.find('.block-coupon .error-msg').addClass('m-hidden');
                objMain.find('.block-coupon .error-msg.js-invalid-coupon').removeClass('m-hidden');
                return;
            }

            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                    operation: 'set_coupon',
                    coupon:    coupon,
                };
    
            fLog(data, 'set coupon');

            fAjax({
                data:     data,
                fSuccess: function(response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });
            
        });
        objMain.find('.js-coupon').keydown(function(event){
            if (event.which == 13) {
                objMain.find('.js-btn-set-coupon').click();
            }
        });

        // Купон: деактивировать
        objMain.find('.js-btn-unset-coupon').click(function(){

            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                    operation: 'unset_coupon',
                    coupon:    'unset',
                };
    
            fLog(data, 'unset coupon');

            fAjax({
                data:     data,
                fSuccess: function(response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });
            
        });

        // Купон: деактивировать один
        objMain.find('.coupon-list .item').click(function(){

            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                    operation: 'unset_coupon',
                    coupon:    $(this).html(),
                };
    
            fLog(data, 'unset one coupon');

            fAjax({
                data:     data,
                fSuccess: function(response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });
            
        });

        // Авторизация
        objMain.find('.js-step-1 .stb-total .b-bonus .btn-auth').click(function(){
            v22.fAuthShowForm({form: 'authorization'});
        });

        objBlockTotal.fInit();

        // Бонусы
        if (objMain.find('.js-step-1 .stb-total .b-bonus .b-auth .js-value-input').length) {

            v22.objBonusWithdraw.fInit({
                objBlockSlider: objMain.find('.js-step-1 .stb-total .b-bonus .b-auth'),
                fCalcTotal: function(){
                    objBlockTotal.fCalc();
                },
            });
        } //

        objBlockTotal.fCalc();

        // Бесплатная доставка
        var objModalBuyMore = objMain.find('.modal-buy-more'),
            flagModalBuyMore = false,
            flagModalBuyMoreDisable = false;

        if (objModalBuyMore.length) {
            objMain.find('.js-step-1 .js-btn-next-step').click(function(){
                if (flagModalBuyMoreDisable) {
                    return;
                }
                flagModalBuyMore = true;
                flagModalBuyMoreDisable = true;

                $.fancybox.open(objModalBuyMore);
            });
            //objMain.find('.js-step-1 .js-btn-next-step').click();
        } //

        objMain.find('.js-step-1 .js-btn-next-step, .js-step-preview .js-btn-step-store').click(function(){
            // step-1       -> step-store
            // step-preview -> step-store

            if (flagModalBuyMore) {
                flagModalBuyMore = false;
                return;
            }
            fHideStep();

            objMain.find('.js-step-store').addClass('active');

            if (stepStoreTotalSum) {
                var objStoreList = objMain.find('.js-step-store .js-store-list');
                
                objStoreList.toggleClass('bonus-withdraw', stepStoreTotalSum.flagBonusWithdraw);
                objStoreList.find('.item[data-type=all] .js-order-price').html(stepStoreTotalSum.sum);
                stepStoreTotalSum = false;
            };
        });

        objMain.find('.js-step-store .js-btn-prev-step').click(function(){
            // step-store -> step-1
            fHideStep();

            objMain.find('.js-step-1').addClass('active');
            objSidebar.removeClass('m-hidden');
            $('.page-order--block-similar-products').removeClass('m-hidden');
        });

        objMain.find('.js-step-out-of-stock .js-btn-next-step').click(function(){
            // step-out-of-stock -> step-preview
            fHideStep();
            objMain.find('.js-step-preview').addClass('active');
        });
        objMain.find('.js-step-out-of-stock .js-btn-prev-step').click(function(){
            // step-out-of-stock -> step-store
            fHideStep();
            objMain.find('.js-step-store').addClass('active');
        });

        objMain.find('.js-step-preview .js-btn-step-out-of-stock').click(function(){
            // step-preview -> step-out-of-stock
            fHideStep();
            objMain.find('.js-step-out-of-stock').addClass('active');
        });

        fBindHandlersStepStore();
        fBindHandlersStepOutOfStock();
        fBindHandlersStepPreview();
    } // function

    function fBindHandlersSimilar() {
        var objTMP = $('.page-order--block-similar-products');
    
        if (!objTMP.length) {
            return;
        }

        let slidesToShow = Math.floor( $('.tpl-2-personal-container').width() / 280 );

        objTMP.find('.block-catalog-section-h').slick({
            infinite:       false,
            slidesToShow:   slidesToShow,
            slidesToScroll: slidesToShow,
            variableWidth:  true,
            
            prevArrow: objTMP.find('.block-template__prev'),
            nextArrow: objTMP.find('.block-template__next'),
    
            responsive: [
                {
                    breakpoint: 768,
                    settings: 
                    {
                        slidesToShow: 1,
                        slidesToScroll: 1,
                    }
                },
                {
                    breakpoint: 432,
                    settings: {
                        slidesToScroll: 1,
                        centerMode:   true,
                    }
                }
            ],
        });
    
        v22.scriptForPage.catalog_section_h__bindHandlers();
        
    } // function

    v22.fOrderRefresh = function() {
        if (timer.id || !v22.fSetProcessing(true)) {
            fLog('Processing or timer');
            return;
        }

        var data = {
                oper: 'render',
            };

        fLog(data, 'fOrderRefresh');

        fAjax({
            data: data,
            fSuccess: function(response) {
                fRender(response);
                v22.fSetProcessing(false);
            }
        });
    };

    function fBindHandlersStepStore() {

        function fLog2(msg, label) {
            //window.sp_global.v22.fLog(msg, label);
        }

        var objStepStore   = objMain.find('.js-step-store'),
            objStoreList   = objStepStore.find('.js-store-list'),
            objBtnNextStep = objMain.find('.js-btn-next-step'),

            arObjItem      = objStoreList.find('.item'),
            arObjBtnShowProductList = objStoreList.find('.b-status span'),
            objNotFound    = objStoreList.find('.not-found'),

            objMapStore    = objStepStore.find('.block-map-wrapper .block-map-store'),

            btnShowProductList_text   = arObjBtnShowProductList.first().html(),
            btnShowProductList_text_2 = arObjBtnShowProductList.first().data('text_2'),
            
            storeId            = 0,
            flagDeliveryYandex = false,
            
            flagSearchProcessing = false,
            searchStringCurrent  = '',
            searchStringNew      = null,
            
            urlOrderDelivery = objStoreList.data('url_order_delivery'),
            flagMobile       = (window.innerWidth < 768),

            nothing;

        function fSetStoreActive(params) {
            /*  fSetStoreActive({
                    obj:                $(this),
                    id:                 id,
                    flagScrollMap:      true,
                    flagDeliveryYandex: true,
                })
            */
            var obj = params.obj;
            
            if (!obj) {
                obj = arObjItem.filter('[data-id='+params.id+']');
            }
            if (!obj) {
                return;
            }

            var id        = obj.data('id'),
                latitude  = obj.data('latitude'),
                longitude = obj.data('longitude'),
                
                flagDisabled = Boolean( obj.data('disabled') );

            objBtnNextStep.toggleClass('disabled', flagDisabled);

            arObjItem.removeClass('active');
            arObjItem.find('.js-delivery-type').removeClass('active');

            flagDeliveryYandex = (params.flagDeliveryYandex) ? true : false;

            if (flagDisabled) {
                storeId            = 0;
                flagDeliveryYandex = false;
            } else {
                storeId = id;
                obj.addClass('active');
                if (!flagDeliveryYandex) {
                    obj.find('.js-delivery-type:not(.js-yandex)').addClass('active');
                } else {
                    obj.find('.js-delivery-type.js-yandex').addClass('active');
                }
            }

            if (ymapsMap) {
                if (id == 'delivery') {
                    fSetPlacemarkActive(false);
                } else {
                    fSetPlacemarkActive( placemarkList[ id ] );
                    if (params.flagScrollMap) {
                        ymapsMap.setCenter([latitude, longitude], 16);
                    }
                }
            }

            if (flagMobile) {
                if (id == 'delivery') {
                    objMapStore.html('');
                } else {
                    objMapStore.html( obj[0].outerHTML );
                    fBindHandlerBtnShowProductList( objMapStore.find('.b-status span') );
                }
            }
        } // function

        function fSetStoreInactive() {
            storeId            = 0;
            flagDeliveryYandex = false;

            arObjItem.removeClass('active');
            arObjItem.find('.js-delivery-type').removeClass('active');

            objBtnNextStep.addClass('disabled');
        } // function

        function fBindHandlerBtnShowProductList(obj) {

            obj.click(function(event){
                event.stopPropagation();

                var objItem  = $(this).closest('.item'),
                    flagOpen = !objItem.hasClass('product-list-open');

                objItem.toggleClass('product-list-open', flagOpen);
                $(this).html( (flagOpen) ? btnShowProductList_text_2 : btnShowProductList_text );
            });
        } // function

        fBindHandlerBtnShowProductList(arObjBtnShowProductList);

        arObjItem.find('.js-delivery-type').click(function(event){
            event.stopPropagation();

            var objItem     = $(this).closest('.item');
                arObjType_1 = objItem.find('.js-delivery-type:not(.js-yandex)'),
                arObjType_2 = objItem.find('.js-delivery-type.js-yandex'),
                obj         = $(this);

            if (!objItem.hasClass('active')) {
                fSetStoreActive({
                    obj:                objItem,
                    flagScrollMap:      true,
                    flagDeliveryYandex: obj.hasClass('js-yandex'),
                });
                objStoreShowOnMap = objItem;
    
            } else {
                // item active
                if (!arObjType_2.length) {
                    // js-delivery-type active
                    fSetStoreInactive();
                } else {
                    if (obj.hasClass('active')) {
                        fSetStoreInactive();
                    } else {
                        flagDeliveryYandex = obj.hasClass('js-yandex');

                        arObjType_1.toggleClass('active', !flagDeliveryYandex);
                        arObjType_2.toggleClass('active', flagDeliveryYandex);
                    }
                }
            } //
        });

        arObjItem.find('.b-delivery-info a').click(function(event){
            event.stopPropagation();
        });

        arObjItem.click(function(){
            var objItem = $(this);

            if (objItem.hasClass('active')) {
                fSetStoreInactive();
            } else {
                fSetStoreActive({
                    obj:           objItem,
                    flagScrollMap: true,
                });
                objStoreShowOnMap = objItem;
            }
        });

        objStepStore.find('.js-btn-show-all').click(function(){
            objStoreList.addClass('show-all');
            $(this).hide();
        });
        
        objBtnNextStep.click(function(){
            if (!storeId) {
                return;
            }

            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            if (storeId == 'delivery') {
                window.location.assign(urlOrderDelivery);
                return;

            } else if (flagDeliveryYandex) {
                window.location.assign(urlOrderDelivery + '?deliveryYandexStore=' + storeId);
                return;
            }

            var data = {
                    oper:    'v22StepStore_SetStore',
                    storeId: storeId,
                };
    
            fLog(data, 'nextStep');

            fAjax({
                data: data,
                fSuccess: function(response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });
        }); 

        function fBindHandlersStoreListPanel() {
            var arObjBtnList = objStepStore.find('.store-list-panel .js-btn-list'),
                arObjBtnMap  = objStepStore.find('.store-list-panel .js-btn-map'),
                objInput     = objStepStore.find('.block-store-list-top .block-1 input'),
                objInput_2   = objStepStore.find('.block-store-list-top .block-2 input'),
                objBtnSearch = objStepStore.find('.store-list-panel .button-search'),

                nothing;

            function fStoreListPanel(tab) {
                var flagList = (tab == 'list');
                
                arObjBtnList.toggleClass('active', flagList);
                arObjBtnMap.toggleClass('active', !flagList);

                if (!flagMobile) {
                    objStepStore.find('.block-main').toggleClass('type-2', !flagList);
                    objStoreList.toggleClass('type-2', !flagList).toggleClass('type-1', flagList);
                } else {
                    objStepStore.find('.block-main .block-store-list-wrapper').toggle(flagList);
                    objStepStore.find('.block-main .block-map-wrapper').toggle(!flagList);
                }

                if (!flagList && !ymapsMap) {
                    if (!flagMobile) {
                        objStepStore.find('.js-btn-show-all').click();
                        objStepStore.find('.js-scroll-container-tmp').addClass('js-scroll-container').scrollbar(); // \js\components\template\template.js
                    }
                    ymaps.ready(function(){
                        fMapInit();
                        if (objStoreShowOnMap) {
                            fSetStoreActive({obj: objStoreShowOnMap, flagScrollMap: true});
                            objStoreShowOnMap = null;
                        }
                    });
                }

                if (!flagList && !flagMobile && storeId) {
                    fScrollStoreList( arObjItem.filter('.active')[0].offsetTop );
                }

                if (flagList && flagMobile && scrollStoreListObj) {
                    objStepStore.find('.js-btn-show-all').click();
                    fScrollStoreList2(scrollStoreListObj);
                    scrollStoreListObj = null;
                }
            } // function

            arObjBtnList.click(function(){
                if (!$(this).hasClass('active')) {
                    fStoreListPanel('list');
                }
            });
            arObjBtnMap.click(function(){
                if (!$(this).hasClass('active')) {
                    fStoreListPanel('map');
                }
            });

            objBtnSearch.click(function(){
                objStepStore.find('.block-store-list-top .block-2').addClass('active');
                objInput_2.focus();
            });
            objInput_2.blur(function(){
                objStepStore.find('.block-store-list-top .block-2').removeClass('active');
            });

            if (flagMobile) {
                objStepStore.find('.block-main').addClass('mobile');
                objStoreList.addClass('type-2').removeClass('type-1');
            }

            objInput.add(objInput_2).on('input', function(){
                fSearch( $(this).val().trim() );
            });

        } // function
    
        fBindHandlersStoreListPanel();

        // ### map (

        var ymapsMap,
            placemarkList = {},
            //scrollStoreListOffset = null,
            scrollStoreListObj = null,
            objStoreShowOnMap,
            placemarkActive;

        function fMapInit() {
            fLog('fMapInit');

            var objMap    = objStepStore.find('.block-map'),
                latitude  = objMap.data('latitude'),
                longitude = objMap.data('longitude');
    
            ymapsMap = new ymaps.Map(
                objMap[0],
                {
                    center:   [latitude, longitude],
                    zoom:     12,
                    controls: [],
                }, {
                    searchControlProvider: 'yandex#search'
                }
            );
    
            fMapRefresh();
        } // function

        function fMapRefresh() {
            if (!ymapsMap) {
                return;
            }
    
            ymapsMap.geoObjects.removeAll();
    
            var imageList = {
                    'all':     '/local/templates/v22/assets/images/placemark.svg',
                    'some':    '/local/templates/v22/assets/images/placemark_some.svg',
                    'nothing': '/local/templates/v22/assets/images/placemark_nothing.svg',
                },
                placemarkOptions = {
                    iconLayout:      'default#image',
                    iconImageHref:   imageList['all'],
                    iconImageSize:   [20, 26],
                    iconImageOffset: [-10, -26],
                    balloonShadow:   false,
                },
                offset = 0,
                nothing;
            
            arObjItem.not('.m-hidden').each(function(){
                
                var objStore = $(this),
                    id       = objStore.data('id');
                
                if (id == 'delivery') {
                    offset += objStore.outerHeight(true);
                    return;
                }

                var type      = objStore.data('type'),
                    latitude  = objStore.data('latitude'),
                    longitude = objStore.data('longitude'),
                    
                    placemark = new ymaps.Placemark(
                        [latitude, longitude],
                        {},
                        placemarkOptions
                    ),
                    offsetThis = offset;

                if (type != 'all') {
                    placemark.options.set('iconImageHref', imageList[ type ]);
                }

                offset += objStore.outerHeight(true);

                placemark.events.add('click', function(event){
                    if (id == storeId) {
                        //return;
                    }
                    
                    fSetStoreActive({obj: objStore});

                    if (0) {
                        fLog(objStore.data('id'), 'id');
                        fLog(objStore.find('.block-1 .warehouse-list__label.warehouse-list__label--bold').html().trim(), 'address');
                    }

                    if (flagMobile) {
                        //scrollStoreListOffset = offsetThis;
                        scrollStoreListObj = objStore;
                    } else {
                        fScrollStoreList(offsetThis);
                    }
                });

                ymapsMap.geoObjects.add(placemark);

                placemarkList[ id ] = placemark;
            });
            
            objMapStore.html('');
            scrollStoreListObj = null;

        } // function

        function fScrollStoreList(offset) {
            objMain.find('.js-scroll-container').animate({
                scrollTop: offset,
            }, 1000);
        } // function

        function fScrollStoreList2(obj) {
            obj = obj[0];
            
            if (0) {
                obj.scrollIntoView({behavior: 'smooth', block: 'center'});

            } else if (1) {
                var offset = obj.offsetTop;

                while (obj = obj.offsetParent) {
                    offset += obj.offsetTop;
                }
                offset -= $('.header__fixed').outerHeight();
                
                //fLog(offset);

                if (0) {
                    $(document).scrollTop(offset);
                } else {
                    $('html, body').animate({
                        scrollTop: offset,
                    }, 1000);
                }
            } //
        } // function
        
        function fSetPlacemarkActive(placemark) {
            if (placemarkActive) {
                placemarkActive.options.set('iconImageSize', [20, 26]);
                placemarkActive.options.set('iconImageOffset', [-10, -26]);
            }
            if (!placemark) {
                placemarkActive = null;
                return;
            }
            placemarkActive = placemark;
            placemarkActive.options.set('iconImageSize', [30, 40]);
            placemarkActive.options.set('iconImageOffset', [-15, -40]);
        } // function

        // ### )

        function fSearch(searchString) {
            
            fLog2(searchString, 'fSearch');

            if (flagSearchProcessing) {
                fLog2('fSearch: flagSearchProcessing');
                searchStringNew = searchString;
                return;
            }

            if (searchString == searchStringCurrent) {
                fLog2('fSearch: searchString == searchStringCurrent');
                return;

            } else if (!searchString.length) {
                fLog2('fSearch: flagShowAll');
                searchStringCurrent = '';
                fRefreshStoreList({flagShowAll: true});
                return;
            }

            flagSearchProcessing = true;

            var data = {
                oper:         'orderSearchStore',
                searchString: searchString,
            };
    
            fLog2(data, 'data');
    
            v22.fAjax({
                url:      '/local/bitrix_ajax/v22.php',
                data:     data,
                dataType: 'json',
    
                fSuccess: function(response) {
                    fLog2(response);
                    flagSearchProcessing = false;

                    if (searchStringNew !== null) {
                        fLog2({searchString, searchStringNew});
                        searchString    = searchStringNew;
                        searchStringNew = null;
                        fSearch(searchString);
                        return;
                    }
                    searchStringCurrent = searchString;
                    fRefreshStoreList({
                        flagShowAll: false,
                        items:       response.items,
                    });
                    objStepStore.find('.js-btn-show-all').click();
                }
            });

        } // function
    
        function fRefreshStoreList(params) {
            var flagShowAll = params.flagShowAll,
                items       = params.items;

            if (flagShowAll) {
                arObjItem.removeClass('m-hidden');
                objNotFound.addClass('m-hidden');

            } else if (!items.length) {
                arObjItem.addClass('m-hidden');
                objNotFound.removeClass('m-hidden');

            } else {
                arObjItem.each(function(){
               
                    var objStore = $(this),
                        id       = objStore.data('id');

                    objStore.toggleClass('m-hidden', !items.includes(id));
                });
                objNotFound.addClass('m-hidden');
            }
            
            if (storeId) {
                fSetStoreInactive();
                fSetPlacemarkActive(false);
            }

            fMapRefresh();
        } // function

        if (0) {
            storeId = 7;
            objBtnNextStep.click();
        }
    } // function

    function fBindHandlersStepOutOfStock() {
        var objStepOutOfStock = objMain.find('.js-step-out-of-stock'),
            mainStoreId       = objStepOutOfStock.data('main_store_id');

        objStepOutOfStock.find('.js-catalog-item-2').each(function(){
            var objItem     = $(this),
                objQuantity = objItem.find('.js-quantity-container'),

                basketItemId    = objItem.data('basket_item_id'),

                maxQuantity     = objItem.data('quantity_max'),
                quantityInitial = objQuantity.find('.js-value').val(),
                quantityPrev    = quantityInitial,

                canBuy = Boolean(objQuantity.length),

                nothing;

            if (objQuantity.length) {
                fBindHandlersQuantity(objQuantity, {
                    objQuantity:     objQuantity,
                    basketItemId:    basketItemId,
                    maxQuantity:     maxQuantity,
                    quantityInitial: quantityInitial,
                    quantityPrev:    quantityPrev,
    
                    source:          'stepOutOfStock',
                    mainStoreId:     mainStoreId,
                });
            } else {
                objItem.find('.js-btn-remove').click(function(){
                    fRemoveItem({
                        basketItemId: basketItemId,
                        source:       'stepOutOfStock',
                        mainStoreId:  mainStoreId,
                    });
                });
            } //
    
            objItem.find('.js-store').change(function(){
                
                if (timer.id || !v22.fSetProcessing(true)) {
                    fLog('Processing or timer');
                    return;
                }

                var data = {
                        oper:         'v22StepOutOfStock_SetStore',
                        basketItemId: basketItemId,
                        storeId:      $(this).val(),
                        mainStoreId:  mainStoreId,
                    };
        
                fLog(data, 'setStore');

                fAjax({
                    data: data,
                    fSuccess: function(response) {
                        fRender(response);
                        v22.fSetProcessing(false);
                    }
                });
            });
        });
    } // function

    function fBindHandlersStepPreview() {
        var objStepPreview   = objMain.find('.js-step-preview'),
            objBlockCustomer = objStepPreview.find('.block-customer'),
            
            dataForCreateOrder = {};

        if (objBlockCustomer.length) {
            var objCustomerFio    = objBlockCustomer.find('.js-field-customer-fio')
                objCustomerPhone  = objBlockCustomer.find('.js-field-customer-phone input'),
                objMsgWrongCoupon = objBlockCustomer.find('.js-field-customer-phone .msg-wrong-coupon');

            objCustomerPhone.focus(function(){
                $(this).removeClass('error');
            });

            // Проверка купона
            if (objMsgWrongCoupon.length) {

                v22.objCheckCouponOnePerUser.fInit({
                    objPhone:          objCustomerPhone,
                    objIconProcessing: objBlockCustomer.find('.js-field-customer-phone .icon-processing'),
                    objMsg:            objMsgWrongCoupon,

                    timer:             timer,
                });
            } //
            
            objCustomerPhone.inputmask({
                mask: '+7 (999) 999 99 99',
            });
        } //

        function fCreateOrder(flagRecursion) {

            if (!flagRecursion && !v22.fSetProcessing(true)) {
                return; // Не должно
            }
    
            fAjax({
                data: dataForCreateOrder,
                fSuccess: function(response) {
                    fRender(response, {doNotBindEventHandlers: true});
  
                    var obj = objMain.find('.js-data-create-order');
    
                    if (!obj.length) {
                        v22.fSetProcessing(false);
                    } else {
                        dataForCreateOrder = JSON.parse( obj.html() );
    
                        setTimeout(function(){
                            fCreateOrder(true);
                        }, 1000);
                    }
                }
            });
        } // function

        objStepPreview.find('.js-btn-next-step').click(function(){
            
            if (timer.id || v22.fIsProcessing()) {
                fLog('Processing or timer');
            }
            
            $(document).scrollTop(0);

            dataForCreateOrder = {
                BREAK_POINT:    objStepPreview.data('break_point'), // BEGIN | ADD_USER
                sessid:         objStepPreview.data('sessid'),
                BONUS_WITHDRAW: v22.objBonusWithdraw.bonusWithdraw,
                USER_CONSENT: $('#user_consent').is(':checked') ? 'Y' : 'N'
            };
            
            if (objBlockCustomer.length) {
                dataForCreateOrder.USER_PHONE = objCustomerPhone.val();
                dataForCreateOrder.USER_FIO   = objCustomerFio.val();

                if (!v22.fValidatePhone(dataForCreateOrder.USER_PHONE)) {
                    objCustomerPhone.addClass('error');
                    return;
                }

                if (objMsgWrongCoupon.length && !v22.objCheckCouponOnePerUser.fIsChecked()) {

                    if (!v22.fSetProcessing(true)) {
                        return;
                    }
                    v22.objCheckCouponOnePerUser.fCheckCouponOnePerUser({
                        fCallback: function(flagValid){
                            if (flagValid) {
                                fCreateOrder();
                            }
                        },
                    });
                } else {
                    fCreateOrder();
                }
            } else {
                fCreateOrder();
            }
        });

    } // function

    // # objBlockTotal (
    var objBlockTotal = {

        fInit: function(){
            var objTotal       = objMain.find('.js-step-1 .stb-total'),
                objTotalSum    = objTotal.find('.js-order-price'),
                objTotalSum_2  = objTotal.find('.js-order-price-2'), // Сумма с учетом купона
                objTotalSumOld = objTotal.find('.order_oldprice'),
                objCertificateSum = objTotal.find('.js-certificate-sum'),
            
                totalSum       = objTotalSum.data('value'),
                totalSum_2     = (objTotalSum_2.length) ? objTotalSum_2.data('value') : 0,
                certificateSum = (objCertificateSum.length) ? objCertificateSum.data('value') : 0,
                flagDiscount   = !objTotalSumOld.hasClass('m-hidden');

            this.fCalc = function() {
                var bonusWithdraw = v22.objBonusWithdraw.bonusWithdraw,
                    totalSumNew   = totalSum,
                    totalSumNew_2 = totalSum_2,
                    certificateSumNew = certificateSum,
                    flagWithdraw  = Boolean(bonusWithdraw);
    
                if (flagWithdraw) {
                    totalSumNew   -= bonusWithdraw;
                    totalSumNew_2 -= bonusWithdraw;
                };
    
                if (certificateSum) {
                    certificateSumNew = Math.min(certificateSum, totalSumNew);
    
                    totalSumNew   = Math.max(totalSumNew - certificateSum, 0);
                    totalSumNew_2 = Math.max(totalSumNew_2 - certificateSum, 0);
                }
    
                // ###

                if (!flagDiscount) {
                    objTotalSumOld.toggleClass('m-hidden', !flagWithdraw);
                }
    
                if (objCertificateSum.length) {
                    certificateSumNew = v22.fFormatNum(certificateSumNew);
                    objCertificateSum.html(certificateSumNew);
                }
    
                totalSumNew = v22.fFormatNum(totalSumNew);
                objTotalSum.html(totalSumNew);
    
                if (objTotalSum_2.length) {
                    totalSumNew_2 = v22.fFormatNum(totalSumNew_2);
                    objTotalSum_2.html(totalSumNew_2);
                }
    
                stepStoreTotalSum = {
                    sum:               totalSumNew,
                    flagBonusWithdraw: flagWithdraw,
                };
            };
    
            this.fLogData = function(){
                fLog({
                    totalSum:     totalSum,
                    totalSum_2:   totalSum_2,
                    flagDiscount: flagDiscount,
                }, 'objBlockTotal data');
            };
        },
    }; // obj
    
    if (0) {
        objBlockTotal.fInit();
        objBlockTotal.fLogData();
        v22.objBonusWithdraw.bonusWithdraw = 1;
        objBlockTotal.fCalc();
    }
    // # )

    fBindHandlers();
    fBindHandlersSimilar();

    if (0) {
        objMain.find('.js-step-1 .js-btn-next-step').click();
    } else if (0) {
        fHideStep();
        objMain.find('.js-step-out-of-stock').addClass('active');
    } else if (0) {
        fHideStep();
        objMain.find('.js-step-preview').addClass('active');
    }

}; // function

// Блок "Cписание бонусов": слайдер, поле ввода, кнопка "Сбросить". Используется в page_order_delivery.js
window.sp_global.v22.objBonusWithdraw = {
    
    bonusWithdraw: 0,     // v22.objBonusWithdraw.bonusWithdraw
    flagInit:      false,

    fInit: function(params) {
        /*  v22.objBonusWithdraw.fInit({
                objBlockSlider: objMain.find('.order-submit__slider'),
                fCalcTotal: function(){
                    objBlockTotal.fCalc();
                },
            })
        */

        function fLog(msg, label) {
            window.sp_global.v22.fLog(msg, label);
        }
        
        var objBlockSlider = params.objBlockSlider,
            fCalcTotal     = params.fCalcTotal;

        var that = this,

            objRangeSlider = objBlockSlider.find('.js-range-slider'),
            objValueInput  = objBlockSlider.find('.js-value-input'),
            objValueMsg    = objBlockSlider.find('.js-value-msg'),
            objMsgNoPrihod = objBlockSlider.find('.msg-no-bonus-prihod'),

            maxValue       = objValueInput.val(),
            instanceRangeSlider;

        function fSetValue(params) {
            var value = that.bonusWithdraw;
            
            if (!params || !params.flagDontUpdateSlider) {
                instanceRangeSlider.update({from: value});
            }
            objValueInput.val(value);
            objValueMsg.html(value);

            objMsgNoPrihod.toggle( Boolean(value) );

            fCalcTotal();
        } //

        if (!this.flagInit) {
            this.flagInit = true;
            this.bonusWithdraw = 0;
            //this.bonusWithdraw = maxValue;
        } else {
            this.bonusWithdraw = Math.min(this.bonusWithdraw, maxValue);
        }

        objRangeSlider.ionRangeSlider({
            min:  0,
            max:  maxValue,
            from: this.bonusWithdraw,

            hide_min_max: true,
            hide_from_to: true,

            onChange: function (data) {
                that.bonusWithdraw = data.from;
                fSetValue({flagDontUpdateSlider: true});
            },
        });
        // http://ionden.com/a/plugins/ion.rangeSlider/

        instanceRangeSlider = objRangeSlider.data('ionRangeSlider');

        objBlockSlider.find('.js-btn-reset').click(function(){
            that.bonusWithdraw = 0;
            fSetValue();
        });

        objValueInput.change(function(){
            var value = parseInt( objValueInput.val() );

            if (isNaN(value)) {
                value = 0;
            } else {
                value = Math.max(value, 0);
                value = Math.min(value, maxValue);
            }
                
            that.bonusWithdraw = value;
            fSetValue();
        });

        fSetValue({flagDontUpdateSlider: true});

    }, // function

} // object

// Проверка купона. Используется в page_order_delivery.js
window.sp_global.v22.objCheckCouponOnePerUser = {

    fInit: function(params) {
        /*  v22.objCheckCouponOnePerUser.fInit({
                objPhone:          objMain.find('.js-field-customer-phone input'),
                objIconProcessing: objMain.find('.js-field-customer-phone .icon-processing'),
                objMsg:            objMsgWrongCoupon,

                timer:             timer,
            })
        */
        var fShowMessage = window.sp_global.v22.fShowMessage;

        function fLog(msg, label) {
            window.sp_global.v22.fLog(msg, label);
        }
    
        var objPhone          = params.objPhone,
            objIconProcessing = params.objIconProcessing,
            objMsg            = params.objMsg,
            timer             = params.timer;
        
        var v22  = window.sp_global.v22,
            that = this,

            phone_prev   = objPhone.val();
            checkedPhone = phone_prev,

        this.fCheckCouponOnePerUser = function(params) {
            /*  v22.objCheckCouponOnePerUser.fCheckCouponOnePerUser({
                    fCallback: function(flagValid){
                        if (flagValid) {
                            fCreateOrder();
                        } else {
                            $(document).scrollTop(0);
                        }
                    },
                })
            */
            var phone = objPhone.val();
        
            if (!params) {
                params = {};
            }

            objIconProcessing.show();
                
            var data = {
                    oper:  'checkCouponOnePerUser',
                    phone: phone,
                };
        
            fLog(data, 'data');
        
            $.ajax({
                url:      '/local/bitrix_ajax/ajax_misc.php',
                type:     'POST',
                dataType: 'json',
                data:     data,
        
                success: function (response) {
                    fLog(response, 'response');

                    objIconProcessing.hide();
                    v22.fSetProcessing(false);

                    if (!response) {
                        fShowMessage('Ошибка 100');
                    } else {
                        if (response.error_msg) {
                            fShowMessage(response.error_msg);
                        } else {
                            checkedPhone = phone;
        
                            objMsg.toggleClass('m-hidden', response.couponValid);

                            if (params.fCallback) {
                                params.fCallback(response.couponValid);
                            }
                        }
                    } //
                },
                error: function () {
                    objIconProcessing.hide();
                    v22.fSetProcessing(false);
                    fShowMessage('Нет соединения с сервером (код ошибки: 42)'); // # ajax error #
                }
            });    
        }; // function

        this.fIsChecked = function() {
            return (checkedPhone == objPhone.val());
        }; // function

        objPhone.keyup(function(){
            var phone = $(this).val();

            if (phone === phone_prev) {
                // Изменений нет
                return;
            }
            phone_prev   = phone;
            checkedPhone = null;

            objMsg.addClass('m-hidden');

            if (!v22.fValidatePhone(phone)) {
                fLog('phone invalid');
                return;
            } //

            if (timer.id || !v22.fSetProcessing(true)) {
                return;
            }

            that.fCheckCouponOnePerUser();
        });

    }, // function

}; // object

/* End */
;
; /* Start:"a:4:{s:4:"full";s:69:"/local/templates/v22/assets/js/page_order_delivery.js?170200679534586";s:6:"source";s:53:"/local/templates/v22/assets/js/page_order_delivery.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
window.sp_global.v22.scriptForPageOrderDelivery = function() {

    var fShowMessage = window.sp_global.v22.fShowMessage;

    function fLog(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    }

    var v22     = window.sp_global.v22,
        objMain = $('.page-order-delivery'),
        urlAjax = objMain.data('url-ajax'),
        
        deliveryYandexStore = objMain.data('delivery-yandex-store'),

        timer = {
            id:           null,
            basketItemId: 0,     // Данный элемент корзины можно редактировать (количество и "удалить") пока запрос не отправлен
            delay:        1000,
        },

        step = 1,

        sessid     = $('#sessid').val(),
        arObjField = null,

        nothing;

    // ###
    
    function fAjax(params) {
        /*
            fAjax({
                data: data,
                fSuccess: function(response) {
                }
            })
        */

        $.ajax({
            url:      urlAjax,
            type:     'POST',
            dataType: 'json',
            data:     params.data,

            success: function (response) {
                if (!response) {
                    fShowMessage('Ошибка 100');
                } else {
                    //fLog(response);
                    if (response.error_msg && !response.field_invalid) {
                        fShowMessage('Ошибка: ' + response.error_msg);
                    } else {
                        params.fSuccess(response);
                        return;
                    }
                } //
                v22.fSetProcessing(false);
            },
            error: function () {
                v22.fSetProcessing(false);
                fShowMessage('Нет соединения с сервером (код ошибки: 51)'); // # ajax error #
            }
        });
    } // function

    function fRender(html, params) {
        
        objMain.html(html);

        if (params && params.doNotBindEventHandlers) {
            fLog('doNotBindEventHandlers');
            return;
        } //

        //objMain.find('.delivery-type-tab').removeClass('active');

        fBindEventHandlers();

        objAddress.fInit();
        objAddress.fAfterRefreshPage();

        fShowStep();
    } // function

    function fAddOrderData(data) {
        var deliveryTypeAndStore = fGetDeliveryTypeAndStore();

        var d = {
                delivery_type:  deliveryTypeAndStore.delivery_type,
                delivery_store: deliveryTypeAndStore.delivery_store,
                deliveryYandexStore: deliveryYandexStore,

                delivery_date:  objDeliveryDateAndTime.date,
                delivery_time:  objDeliveryDateAndTime.time,

                bonusWithdraw:  v22.objBonusWithdraw.bonusWithdraw,

                PAY_SYSTEM_ID:  objMain.find('.block-paysystem .active').data('id'),

                ORDER_PROP_2:   'delivery@example.com',
            };

        for (var propName in d) {
            data[ propName ] = d[ propName ];
        }

        var ar = [
                'customer_fio',
                'customer_email',
                'customer_phone', // Для неавторизованных

                'address_podezd',
                'address_floor',
                'address_apartment',
                'address_comment',
            ],
            fieldName,
            i;

        for (i=0; i<ar.length; i++) {
            fieldName = ar[i];
            data[ fieldName ] = (arObjField[ fieldName ]) ? arObjField[ fieldName ].val() : '';
        }

    } // function

    function fGetDeliveryTypeAndStore() {

        var result = {
                flagDeliveryStoreEnabled: false,
                delivery_type:            '',
                delivery_store:           0,
            },

            obj = objMain.find('.delivery-type');

        if (obj.length) {
            result.flagDeliveryStoreEnabled = true;

            obj = obj.filter('.active');

            if (obj.length) {
                result.delivery_type = obj.data('id');

                obj = objMain.find('.delivery_store .js-store.active');

                if (obj.length) {
                    result.delivery_store = obj.data('id');
                }
            }
        } //
    
        return result;
    } // function

    function fOnSelectStore() {
        objMain.find('.delivery-type[data-id=store]').removeClass('error');
    } // function

    function fCreateOrder() {

        if (!v22.fSetProcessing(true)) {
            return; // Не должно
        }

        var data = {
            oper: 'save',
            address_streetAndHouse: objAddress.addressValue.dataForOrder,
            USER_CONSENT: $('#user_consent').is(':checked') ? 'Y' : 'N',
        };

        fAddOrderData(data);

        data.confirmorder = 'Y';
        data.sessid       = sessid;

        fLog(data, 'create order');
        
        if (0) {
            v22.fSetProcessing(false);
            return;
        }

        fAjax({
            data: data,
            fSuccess: function(response) {

                if (!response.field_invalid) {
                    fRender(response.order, {doNotBindEventHandlers: true});

                } else {
                    if (response.order) {
                        fRender(response.order);
                    }

                    response.order = '';
                    fLog(response);

                    var flag = false;
                    
                    if (window.sp_global.v22.is_wholesale != 'Y') {
                        if (response.field_invalid.customer_fio) {
                            arObjField.customer_fio.addClass('error');
                            flag = true;
                        }
                        if (response.field_invalid.customer_email) {
                            arObjField.customer_email.addClass('error');
                            flag = true;
                        }
                        if (response.field_invalid.customer_phone) {
                            if (!arObjField.customer_phone) {
                                // Был авторизован, а теперь нет. Нужно перезагрузить страницу.
                            } else {
                                arObjField.customer_phone.addClass('error');
                                flag = true;
                            }
                        }
                    }

                    if (response.field_invalid.address_streetAndHouse) {
                        objAddress.objFieldStreetStr.addClass('error');
                        objAddress.objFieldHouseStr.addClass('error');
                        flag = true;
                    }

                    if (!deliveryYandexStore) {
                        if (response.field_invalid.delivery_type) {
                            objMain.find('.delivery-type').addClass('error');
                            flag = true;
                        }
                        if (response.field_invalid.delivery_store) {
                            objMain.find('.delivery-type[data-id=store]').addClass('error');
                            flag = true;
                        }

                        if (response.field_invalid.delivery_date) {
                            objDeliveryDateAndTime.objDate.addClass('error');
                            flag = true;
                        }
                        if (response.field_invalid.delivery_time) {
                            objDeliveryDateAndTime.objTime.addClass('error');
                            flag = true;
                        }
                    }

                    if (!flag) {
                        // Не должно
                        fShowMessage('Ошибка: ' + response.error_msg);
                    }
                    $(document).scrollTop(0);
                } //

                v22.fSetProcessing(false);
            } // function
        });

    } // function

    function fCheckForm() {
        //return true;

        var result = true,
            value;

        objMain.find('.step-2 .error').removeClass('error');

        if (window.sp_global.v22.is_wholesale != 'Y') {
            // customer_fio
            value = arObjField.customer_fio.val().trim();
            if (!value.length) {
                arObjField.customer_fio.addClass('error');
                result = false;
            }
    
            // customer_email
            value = arObjField.customer_email.val().trim();
            if (value.length && !v22.fValidateEmail(value)) {
                arObjField.customer_email.addClass('error');
                result = false;
            } 

            // customer_phone
            if (arObjField.customer_phone) {
                value = arObjField.customer_phone.val();
                if (!v22.fValidatePhone(value)) {
                    arObjField.customer_phone.addClass('error');
                    result = false;
                }
            } 
        }

        var deliveryTypeAndStore = fGetDeliveryTypeAndStore();

        if (deliveryTypeAndStore.flagDeliveryStoreEnabled && !deliveryTypeAndStore.delivery_type) {
            objMain.find('.delivery-type').addClass('error');
            result = false;

        } else if (!deliveryTypeAndStore.flagDeliveryStoreEnabled || deliveryTypeAndStore.delivery_type == 'address') {

            // street
            if (!objAddress.addressValue || !objAddress.addressValue.streetValueIsSet) {
                objAddress.objFieldStreetStr.addClass('error');
                result = false;
            }

            // house
            if (!objAddress.addressValue || !objAddress.addressValue.houseValueIsSet) {
                objAddress.objFieldHouseStr.addClass('error');
                result = false;
            }

        } else if (deliveryTypeAndStore.delivery_type == 'store' && !deliveryTypeAndStore.delivery_store) {
            objMain.find('.delivery-type[data-id=store]').addClass('error');
            result = false;
        } //


        // delivery date and time
        if (!deliveryYandexStore && window.sp_global.v22.is_wholesale != 'Y') {
            if (!objDeliveryDateAndTime.date) {
                objDeliveryDateAndTime.objDate.addClass('error');
                result = false;
            }
            if (!objDeliveryDateAndTime.time) {
                objDeliveryDateAndTime.objTime.addClass('error');
                result = false;
            }
        }

        return result;
    } // function

    function fShowStep(stepNew) {
        if (stepNew) {
            step = stepNew;
        }
        objMain.find('.step-1').toggleClass('m-hidden', step != 1);
        objMain.find('.step-2').toggleClass('m-hidden', step != 2);

        $(document).scrollTop(0);
    } // function

    function fBindHandlersQuantity(objQuantity, params) {
        /*  fBindHandlersQuantity(objQuantity, {
                basketItemId,
                maxQuantity,
                quantityInitial,
                quantityPrev,
            })
        */
        var basketItemId    = params.basketItemId,
            maxQuantity     = params.maxQuantity,
            quantityInitial = params.quantityInitial,
            quantityPrev    = params.quantityPrev;

        function fObjQuantity_Change(k) {
            var objInput = objQuantity.find('.js-value'),
                value    = parseInt(objInput.val());
    
            if (v22.fIsProcessing() || (timer.id && timer.basketItemId != basketItemId)) {
                fLog('Processing or timer');
                objInput.val(quantityPrev);
                return;
            }

            if (isNaN(value)) {
                value = 1;
            } else {
                value += k;
                value = Math.max(1, value);
            }
    
            if (value > maxQuantity) {
                value = maxQuantity;
                v22.fShowGlobalMsg( objQuantity.find('.global-error-msg') );
            }
    
            objInput.val(value);

            if (value == quantityPrev) {
                fLog('value == quantityPrev');
                return;
            }

            quantityPrev = value;

            if (timer.id) {
                clearTimeout(timer.id);
                fLog('stop timer');
            }

            if (quantityInitial == value) {
                fLog('quantityInitial == value');
                timer.id = null;
                return;
            }

            // Заводим таймер

            fLog('start timer; value=' + value);

            timer.basketItemId = basketItemId;

            timer.id = setTimeout(function(){

                if (!v22.fSetProcessing(true)) {
                    fLog('timer: processing: cancel');
                    quantityPrev = quantityInitial;
                    objInput.val(quantityInitial);

                    timer.id = null;
                    return;
                }

                var data = {
                        oper:         'changeQuantity',
                        basketItemId: basketItemId,
                        quantity:     value,
                        flagDelivery: true,
                    };
        
                fAddOrderData(data);

                fLog(data, 'quantity');

                fAjax({
                    data: data,
                    fSuccess: function(response) {
                        fRender(response.order);
                        timer.id = null;
                        v22.fSetProcessing(false);
                    }
                });
            }, timer.delay);

        } // function

        objQuantity.find('.js-value').change(function(){
            fObjQuantity_Change(0);
        });

        objQuantity.find('.js-btn-plus').click(function(){
            fObjQuantity_Change(1);
        });

        objQuantity.find('.js-btn-minus').click(function(){
            fObjQuantity_Change(-1);
        });

        // Удалить
        objQuantity.find('.js-btn-remove').click(function(){
            
            if ((timer.id && timer.basketItemId != basketItemId) || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            if (timer.id) {
                clearTimeout(timer.id);
                timer.id = null;
                fLog('stop timer');
            }

            var data = {
                    oper:         'removeItem',
                    basketItemId: basketItemId,
                };

            fAddOrderData(data);

            fLog(data, 'remove item');

            fAjax({
                data: data,
                fSuccess: function(response) {
                    fRender(response.order);
                    v22.fSetProcessing(false);
                }
            });
        });
        
    } // function

    function fBindEventHandlers() {

        // Список товаров
        objMain.find('.step-1 .js-catalog-item').each(function(){
            var objItem     = $(this),
                objQuantity = objItem.find('.js-quantity-container'),

                basketItemId    = objItem.data('basket_item_id'),

                maxQuantity     = objItem.data('quantity_max'),
                quantityInitial = objQuantity.find('.js-value').val(),
                quantityPrev    = quantityInitial,

                nothing;

            var params_fBindHandlersQuantity = {
                    basketItemId,
                    maxQuantity,
                    quantityInitial,
                    quantityPrev,
                };
            
            fBindHandlersQuantity(objQuantity, params_fBindHandlersQuantity);
        });

        // #####

        var flag_fInitSelectStore = false;

        arObjField = {};

        objMain.find('.step-2 .js-field').each(function(){
            arObjField[ $(this).attr('name') ] = $(this);
        });
        //fLog(arObjField, 'arObjField');

        // ### Тип доставки (
        function fOnChangeDeliveryType() {
            var deliveryTypeAndStore = fGetDeliveryTypeAndStore(),
                deliveryType         = deliveryTypeAndStore.delivery_type;

            if (deliveryType == 'store' && !flag_fInitSelectStore) {
                flag_fInitSelectStore = true;
                v22.fInitSelectStore({source: 'orderDelivery', objMain: objMain.find('.delivery_store'), onSelectStore: fOnSelectStore});
            }

            objMain.find('.step-2 .block-date-time .msg').toggleClass('m-hidden', deliveryType != 'store');
        } // function

        objMain.find('.delivery-type').click(function(){
            
            if ($(this).hasClass('active')) {
                return;
            }

            var id = $(this).data('id');

            objMain.find('.delivery-type').removeClass('active').removeClass('error');
            $(this).addClass('active');

            objMain.find('.delivery-type-tab').removeClass('active');
            objMain.find('.delivery-type-tab[data-id='+id+']').addClass('active');

            fOnChangeDeliveryType();

            objBlockTotal.fCalc();
        });
        // ### )

        if (!deliveryYandexStore) {
            objDeliveryDateAndTime.fInit({
                objDate: objMain.find('.step-2 .js-delivery_date'),
                objTime: objMain.find('.step-2 .js-delivery_time'),
            });
        }

        // Кнопка "Оформить заказ"
        objMain.find('.js-btn-create-order').click(function(){

            if (v22.fIsProcessing() || timer.id) {
                fLog('Processing or timer');
                return;
            }
    
            if (step == 1) {
                fShowStep(2);
            } else {
                if (!fCheckForm()) {
                    $(document).scrollTop(0);
                } else {
                    if (objMsgWrongCoupon.length && !v22.objCheckCouponOnePerUser.fIsChecked()) {

                        if (!v22.fSetProcessing(true)) {
                            return;
                        }
                        v22.objCheckCouponOnePerUser.fCheckCouponOnePerUser({
                            fCallback: function(flagValid){
                                if (flagValid) {
                                    fCreateOrder();
                                } else {
                                    $(document).scrollTop(0);
                                }
                            },
                        });
                    } else {
                        fCreateOrder();
                    }
                }
            }
        });

        // Кнопка "Назад"
        objMain.find('.js-btn-prev-step').click(function(event){

            if (v22.fIsProcessing() || timer.id) {
                fLog('Processing or timer');
                event.preventDefault();
                return;
            }
    
            if (step == 2) {
                event.preventDefault();
                fShowStep(1);
            }
        });

        // ### remove class 'error' (
        objMain.find('.step-2 .block-customer .js-field').focus(function(){
            $(this).removeClass('error');
        });
        // ### )

        fOnChangeDeliveryType();

        objBlockTotal.fInit();
        objBlockTotal.fCalc();

        // Авторизация
        objMain.find('.b-total-bonus .btn-auth').click(function(){
            v22.fAuthShowForm({form: 'authorization'});
        });

        // Бонусы
        if (!objMain.find('.b-total-bonus .b-auth .js-value-input').length) {
            if (v22.objBonusWithdraw.bonusWithdraw) {
                // В предыдущем запросе были
                v22.objBonusWithdraw.bonusWithdraw = 0;
                objBlockTotal.fCalc();
            }
        } else {
            v22.objBonusWithdraw.fInit({
                objBlockSlider: objMain.find('.b-total-bonus .b-auth'),
                fCalcTotal: function(){
                    objBlockTotal.fCalc();
                },
            });
        } //

        // Проверка купона
        var objMsgWrongCoupon = objMain.find('.js-field-customer-phone .msg-wrong-coupon');
        if (objMsgWrongCoupon.length) {

            v22.objCheckCouponOnePerUser.fInit({
                objPhone:          objMain.find('.js-field-customer-phone input'),
                objIconProcessing: objMain.find('.js-field-customer-phone .icon-processing'),
                objMsg:            objMsgWrongCoupon,

                timer:             timer,
            });
        } //

        if (arObjField.customer_phone) {
            arObjField.customer_phone.inputmask({
                mask: '+7 (999) 999 99 99',
            });
        }

        // Для разработки
        objMain.find('.block-paysystem img').click(function(){

            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }
    
            var geo_lat = objAddress.addressValue.geo_lat,
                geo_lon = objAddress.addressValue.geo_lon,
                
                data = {
                    oper: 'renderOrder',
                };
            
            if (0 && geo_lat && geo_lon) {
                data.geo_lat = geo_lat;
                data.geo_lon = geo_lon;
            }
                
            fAddOrderData(data);
    
            //fLog(data, 'fOrderDeliveryRefresh');
    
            fAjax({
                data: data,
                fSuccess: function(response) {
                    fRender(response.order);
                    v22.fSetProcessing(false);
                }
            });
        });

        // Для разработки
        objMain.find('.block-map .btn-map').click(function(){
            fShowOnMap({source: 'btn'});
        });
    } // function

    // # objBlockTotal (
    var objBlockTotal = {

        price:         0,
        deliveryPrice: 0,
        deliveryTime:  0,

        deliveryPrice_2:        0,
        flagDeliveryPriceExact: false,
        
        fInit: function(){
            var objTotal = objMain.find('.pgb-total');

            this.objTotalPrice           = objTotal.find('.js-order-price');
            this.objDeliveryPriceBlock   = objTotal.find('.js-delivery-price');
            this.objDeliveryPrice        = objTotal.find('.js-delivery-price .js-value');
            this.objDeliveryPriceMsgFrom = objTotal.find('.js-delivery-price .js-msg-from');
            this.objBonusMsgRashod       = objTotal.find('.js-msg-rashod');
            this.objBonusMsgRashodValue  = objTotal.find('.js-msg-rashod .js-value');
            this.objCertificateSum       = objTotal.find('.js-certificate-sum');

            this.objDYPriceAndTimeBlock  = objMain.find('.js-dy-price-and-time');
            this.objDYPrice              = this.objDYPriceAndTimeBlock.find('.js-price-value');
            this.objDYTime               = this.objDYPriceAndTimeBlock.find('.js-time-value');

            if (!deliveryYandexStore) {
                this.deliveryPrice = this.objDeliveryPrice.data('value');
                this.price         = this.objTotalPrice.data('value') - this.deliveryPrice;
            } else {
                this.price = this.objTotalPrice.data('value');
            }

            this.certificateSum = (this.objCertificateSum.length) ? this.objCertificateSum.data('value') : 0;
        },

        fCalc: function(){
            var totalPrice    = this.price,
                deliveryPrice = this.deliveryPrice,
                bonusWithdraw = v22.objBonusWithdraw.bonusWithdraw,
                certificateSum = this.certificateSum,
                
                deliveryTypeAndStore = fGetDeliveryTypeAndStore(),
                flagTypeIsAddress    = (!deliveryTypeAndStore.flagDeliveryStoreEnabled || deliveryTypeAndStore.delivery_type == 'address');
            
            if (!deliveryYandexStore
                && flagTypeIsAddress
                && this.flagDeliveryPriceExact
                && (this.deliveryPrice_2 === 0 || this.deliveryPrice_2)
            ) {
                deliveryPrice = this.deliveryPrice_2;
            }

            totalPrice += deliveryPrice;
            totalPrice -= bonusWithdraw;

            if (certificateSum) {
                certificateSum = Math.min(totalPrice, certificateSum);
                totalPrice -= certificateSum;
            } 

            // ###

            if (!deliveryYandexStore) {
                this.objDeliveryPrice.html( v22.fFormatNum(deliveryPrice) );
                this.objDeliveryPriceMsgFrom.toggle(flagTypeIsAddress && !this.flagDeliveryPriceExact);
            } else {
                this.objDeliveryPriceBlock.toggle(Boolean(deliveryPrice));
                this.objDeliveryPrice.html( v22.fFormatNum(deliveryPrice) );

                this.objDYPriceAndTimeBlock.toggle(Boolean(deliveryPrice));
                this.objDYPrice.html( v22.fFormatNum(deliveryPrice) );
                this.objDYTime.html(this.deliveryTime);
            }

            this.objBonusMsgRashodValue.html( v22.fFormatNum(bonusWithdraw) );
            this.objBonusMsgRashod.toggleClass('m-hidden', !Boolean(bonusWithdraw));

            if (this.objCertificateSum.length) {
                this.objCertificateSum.html( v22.fFormatNum(certificateSum) );
            }

            this.objTotalPrice.html( v22.fFormatNum(totalPrice) );
        },

        fLogData: function(){
            fLog({
                price:                  this.price,
                deliveryPrice:          this.deliveryPrice,
                deliveryPrice_2:        this.deliveryPrice_2,
                flagDeliveryPriceExact: this.flagDeliveryPriceExact,
            }, 'objBlockTotal data');
        },
    }; // obj
    
    if (0) {
        objBlockTotal.fLogData();
        //objBlockTotal.deliveryPrice_2        = 202.1;
        //objBlockTotal.flagDeliveryPriceExact = 1;
        v22.objBonusWithdraw.bonusWithdraw = 1;
        objBlockTotal.fCalc();
    }
    // # )

    // # Для разработки (
    var objMap,
        ymapsMap;

    function fShowOnMap(params) {
        //fLog(params, 'fShowOnMap');

        objMap = objMain.find('.block-map .map');
       
        if (!objMap.length) {
            return;
        }

        var data = objAddress.addressValue.dataForOrder;

        if (data) {
            fLog(objAddress.addressValue.dataForOrder);
        }

        if (params.source == 'btn') {
            if (!objMap.hasClass('active')) {
                if (data) {
                    objMap.addClass('active');
                    fShowOnMap_hlp({flagInit: true});
                }
            } else {
                if (data) {
                    fShowOnMap_hlp();
                } else {
                    fShowOnMap_hlp({flagClear: true});
                }
            }
        } else if (params.source == 'fAddressOnChange') {
            if (objMap.hasClass('active')) {
                if (data) {
                    fShowOnMap_hlp();
                } else {
                    fShowOnMap_hlp({flagClear: true});
                }
            }
        }
    } // function

    function fShowOnMap_hlp(params) {
        //fLog(params, 'fShowOnMap_hlp');
        
        if (!params) {
            params = {};
        }

        if (params.flagClear) {
            ymapsMap.geoObjects.removeAll();
            return;
        }

        var lat = objAddress.addressValue.dataForOrder.latitude,
            lon = objAddress.addressValue.dataForOrder.longitude;

        if (!params.flagInit) {
            ymapsMap.geoObjects.removeAll();
            ymapsMap.setCenter([lat, lon], 17);
            
        } else {
            if (ymapsMap) {
                ymapsMap.destroy();
            }
            ymapsMap = new ymaps.Map(objMap[0], {
                center: [lat, lon],
                zoom: 17,
            });
        }
        ymapsMap.geoObjects.add(new ymaps.Placemark([lat, lon]));
    } // function
    // # )

    // # Адрес (
    function fAddressOnChange(params) {
        /*  fOnChange({flagUnset: true})
            fOnChange({flagUnset: false, deliveryPrice: response.deliveryPrice, deliveryTime: response.deliveryTime})
        */
        //fLog(params, 'fAddressOnChange');
        if (!deliveryYandexStore) {
            if (params.flagUnset) {
                objBlockTotal.deliveryPrice_2        = 0;
                objBlockTotal.flagDeliveryPriceExact = false;
            } else {
                objBlockTotal.deliveryPrice_2        = params.deliveryPrice;
                objBlockTotal.flagDeliveryPriceExact = true;
            }
        } else {
            objBlockTotal.deliveryPrice = (params.flagUnset) ? 0 : params.deliveryPrice;
            objBlockTotal.deliveryTime  = (params.flagUnset) ? 0 : params.deliveryTime;
        }

        objBlockTotal.fCalc();
        fShowOnMap({source: 'fAddressOnChange'});
    } // function

    var objAddress = window.sp_global.fGet_objOrderDeliveryAddress({mainObj: objMain, fOnChange: fAddressOnChange, deliveryYandexStore: deliveryYandexStore});
    objAddress.fInit();
    // # )

    // Дата и время доставки
    var objDeliveryDateAndTime = v22.order_delivery_objDeliveryDateAndTime;

    fBindEventHandlers();

    fShowStep(1);

    if (0) {
        fShowStep(2);
        objMain.find('.delivery-type[data-id=store]').click();
    }

}; // function

// Дата и время доставки
window.sp_global.v22.order_delivery_objDeliveryDateAndTime = {

    date: '',
    time: '',

    fInit: function(params) {
        /*  objDeliveryDateAndTime.fInit({
                objDate: objMain.find('.step-2 .js-delivery_date'),
                objTime: objMain.find('.step-2 .js-delivery_time'),
            })
        */
        var that    = this,
            objDate = params.objDate,
            objTime = params.objTime;

        this.objDate = objDate;
        this.objTime = objTime;

        function fSetTime(){
            var obj = objTime.find('.js-item.active');
            that.time = (obj.length) ? obj.html().trim() : '';
        } //

        // datepicker
        var options = $.datepicker.regional['ru'];
        options.minDate = new Date(objDate.data('date_min') + 'T00:00:00');
        options.maxDate = new Date(objDate.data('date_max') + 'T00:00:00');
        objDate.datepicker(options);
        //fLog(options, 'options');
    
        objDate.change(function(){
            objDate.removeClass('error');
            
            that.date = objDate.val();

            var flag_today = (objDate.data('date_today') == that.date);

            objTime.find('.js-item[data-enable-today=0]').toggleClass('disabled', flag_today);

            if (flag_today) {
                objTime.find('.js-item.active[data-enable-today=0]').removeClass('active');
            }
            fSetTime();
        });

        objTime.find('.js-item').click(function(){
            var obj = $(this);

            objTime.removeClass('error');

            if (obj.hasClass('active') || obj.hasClass('disabled')) {
                return;
            }
            objTime.find('.js-item').removeClass('active');
            obj.addClass('active');

            fSetTime();
        });

        objDate.change();
    }, // function

}; // obj

window.sp_global.v22.scriptForPageOrderDelivery_changeDate = function() {

    var fShowMessage = window.sp_global.v22.fShowMessage;

    function fLog(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    }

    var v22 = window.sp_global.v22,

        objMain = $('.page-order-delivery .order-d-created'),
        objDeliveryDateAndTime = v22.order_delivery_objDeliveryDateAndTime;

    function fCheckForm() {
        var result = true;

        if (!objDeliveryDateAndTime.date) {
            objDeliveryDateAndTime.objDate.addClass('error');
            result = false;
        }
        if (!objDeliveryDateAndTime.time) {
            objDeliveryDateAndTime.objTime.addClass('error');
            result = false;
        }

        return result;
    } // function

    objMain.find('.js-btn-send').click(function(){
        if (v22.fIsProcessing() || !fCheckForm() || !v22.fSetProcessing(true)) {
            return;
        }

        var urlAjax    = $('.page-order-delivery').data('url-ajax'),
            urlSuccess = $(this).data('url-success'),
            data = {
                oper:          'changeDeliveryDate',
                orderId:       $(this).data('id'),
                delivery_date: objDeliveryDateAndTime.date,
                delivery_time: objDeliveryDateAndTime.time,
            };

        fLog(data, 'data');

        if (0) {
            v22.fSetProcessing(false);
            return;
        }

        v22.fAjax({
            url:          urlAjax,
            data:         data,
            showErrorMsg: false,
            fSuccess: function(response) {
                fLog(response);

                v22.fSetProcessing(false);
                
                if (response.render) {
                    window.location.assign(urlSuccess); // Нужно перерисовать форму

                } else if (!response.error_msg) {
                    window.location.assign(urlSuccess); // Успешно
                    
                } else {
                    var flag = false;

                    if (response.field_invalid) {
                        if (response.field_invalid.delivery_date) {
                            objDeliveryDateAndTime.objDate.addClass('error');
                            flag = true;
                        }
                        if (response.field_invalid.delivery_time) {
                            objDeliveryDateAndTime.objTime.addClass('error');
                            flag = true;
                        }
                    } //

                    if (!flag) {
                        fShowMessage(response.error_msg);
                    }
                }
            }
        });

    });

    objDeliveryDateAndTime.fInit({
        objDate: objMain.find('.js-delivery_date'),
        objTime: objMain.find('.js-delivery_time'),
    });

}; // function

/* End */
;
; /* Start:"a:4:{s:4:"full";s:59:"/local/templates/v22/assets/js/address_2.js?170650174417123";s:6:"source";s:43:"/local/templates/v22/assets/js/address_2.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
window.sp_global.fGet_objOrderDeliveryAddress = function(params) {
    /*  fGet_objOrderDeliveryAddress({
            mainObj:   objMain,
            fOnChange: function(){},
            deliveryYandexStore: deliveryYandexStore,
            flagDY2:   true,
        })
    */

    return {

        mainObj: params.mainObj,
        deliveryYandexStore: (params.deliveryYandexStore) ? params.deliveryYandexStore : 0,
        flagDY2: Boolean(params.flagDY2),

        urlAjax: '/old/personal/order_delivery/ajax.php',

        fShowMessage:   window.sp_global.fShowMessage,
        fSetProcessing: window.sp_global.fSetProcessing,
        params: {
            selectors: {
                orderTotal: '.block-total'
            }
        },

        fLog: function(msg, label) {
            //window.sp_global.fLog(msg, label);
        },

        fLog2: function(msg, label) {
            window.sp_global.fLog(msg, label);
        },

        timer: {
            id:           null,
            searchString: null,
            processing:   false,
            repeat:       false,
            cancel:       false,
            delay:        300,
        },
        
        streetLenMin: 1,

        addressValue:    {},   // cityId, likeStreetKladrId, dataForOrder, streetValueIsSet, houseValueIsSet # version=2: cityId, likeCityId, likeStreet, dataForOrder, streetValueIsSet, houseValueIsSet
        streetValue:     null, // Значение "Улица" которое показываем пользователю
        streetValueUser: null, // ... которое пользователь ввел

        fieldStreetStrValue_prev: null,
        fieldHouseStrValue_prev:  null,

        last_fOnChange_flagUnset: true,

        version: 1,

        fGetOrderPrice: function () {
            var that = this;
            var orderTotal = $(that.params.selectors.orderTotal);

            if (typeof orderTotal === "undefined" || !orderTotal.length) {
                return 0;
            }

            var orderPrice = parseFloat($(orderTotal[0]).data('order_price'));
            if (!orderPrice) {
                return 0;
            }

            return orderPrice;
        },

        fInit: function() {
            var that = this;

            this.objFieldStreet               = this.mainObj.find('.js-blockaddress-field-street');
            this.objFieldStreetStr            = this.objFieldStreet.find('.field-value-str');
            this.objFieldStreetOptions        = this.objFieldStreet.find('.field-value-str-options');
            this.objFieldStreetIconProcessing = this.objFieldStreet.find('.icon-processing');

            //this.objFieldStreetOptions_contentNotFound = this.objFieldStreetOptions.html(); // Пока не используется

            if (this.objFieldStreet.data('version_js') == 2) {
                this.version = 2;

                this.streetLenMin = 3;
                this.timer.delay  = 1000;
            }
            this.fLog(this.version, 'version');

            var cityId = this.objFieldStreet.data('city_id');
            if (this.version == 1) {
                if (this.addressValue.cityId && this.addressValue.cityId != cityId) {
                    // Выбран другой город
                    this.addressValue = {};
                    this.streetValue  = null;
                    this.fLog('cityId changed');
                }
                this.addressValue.cityId = cityId;
            } else {
                if (!this.addressValue.cityId) {
                    this.addressValue.cityId = cityId;
                }
            }

            this.objFieldHouse               = this.mainObj.find('.js-blockaddress-field-house');
            this.objFieldHouseStr            = this.objFieldHouse.find('.field-value-str');
            this.objFieldHouseIconProcessing = this.objFieldHouse.find('.icon-processing');

            this.objFieldStreetStr.keyup(function(){
                var fieldStreetStrValue = that.objFieldStreetStr.val(),
                    timer               = that.timer;

                if (fieldStreetStrValue === that.fieldStreetStrValue_prev) {
                    // Изменений нет
                    return;
                }
                that.fieldStreetStrValue_prev = fieldStreetStrValue;

                timer.searchString = fieldStreetStrValue;

                if (this.version == 1) {
                    that.addressValue.likeStreetKladrId = null;
                }
                that.addressValue.streetValueIsSet = false;
                that.addressValue.houseValueIsSet  = false;

                that.addressValue.dataForOrder      = null;
                that.streetValue                    = null;
                that.fLog('clear addressValue');

                fOnChange({flagUnset: true});

                that.objFieldStreetOptions.hide();

                if (fieldStreetStrValue.length < that.streetLenMin) {
                    if (timer.id) {
                        if (timer.processing) {
                            timer.repeat = false;
                            timer.cancel = true;
                        } else {
                            clearTimeout(timer.id);
                            timer.id = null;
                        }
                    }
                    that.fLog('fieldStreetStrValue.length short');

                    //that.fFieldOptionsShow(that.objFieldStreetOptions_contentNotFound);
                    return;
                }

                if (timer.id) {
                    if (timer.processing) {
                        // Выполняется обработка. По окончании нужно повторить
                        timer.repeat = true;
                        timer.cancel = false;
                        that.fLog('set repeat');
                        return;
                    }

                    clearTimeout(timer.id);
                    timer.id = null;
                    that.fLog('clearTimeout');
                }

                that.fRefreshAddressList();
            });

            this.objFieldStreetOptions.on('click', 'li', function(){

                if (that.version == 1) {
                    var likeStreetKladrId = $(this).data('like_street_kladr_id'),
                        value_input       = $(this).data('value_input');
                    
                    if (!likeStreetKladrId) {
                        return;
                    }
            
                    that.addressValue.likeStreetKladrId = likeStreetKladrId;
                } else {
                    var likeCityId  = $(this).data('like_city_id'),
                        likeStreet  = $(this).data('like_street'),
                        value_input = $(this).find('.js-value-input').html();
                    
                    if (likeCityId === undefined) {
                        return;
                    }

                    that.addressValue.likeCityId = likeCityId;
                    that.addressValue.likeStreet = likeStreet;
                }

                that.addressValue.streetValueIsSet = true;

                that.streetValue = value_input;
                that.objFieldStreetStr.val(value_input);
        
                that.objFieldStreetOptions.hide();
        
                that.fLog(that.addressValue, 'addressValue');

                // Проверим дом
                that.fieldHouseStrValue_prev = null;
                that.objFieldHouseStr.focusout();
            });
        
            this.objFieldStreetStr.focusin(function(){
                that.objFieldStreetOptions.hide(); // Может быть открыт список улиц

                $(this).val(that.streetValueUser);
                that.fLog('objFieldStreetStr focusin');
            });

            this.objFieldStreetStr.focusout(function(){
                setTimeout(function(){
                    // Может быть открыт список улиц. Закроем его с задержкой иначе нажатие на пункт списка не сработает
                    that.objFieldStreetOptions.hide();
                }, 300);

                that.streetValueUser = $(this).val();
                $(this).val(that.streetValue);
                that.fLog('objFieldStreetStr focusout');
            });

            this.objFieldHouseStr.focusout(function(){
                var value = $(this).val();
                value = value.replace(' ', '');

                $(this).val(value);

                if (that.fieldHouseStrValue_prev == value) {
                    return;
                }
                that.fieldHouseStrValue_prev = value;

                that.addressValue.dataForOrder = null;
                that.addressValue.houseValueIsSet = false;

                fOnChange({flagUnset: true});

                if (!value.length
                    || !that.addressValue.streetValueIsSet
                    || !that.fSetProcessing(true) // Не должно
                ) {
                    return;
                }

                var data = {
                    oper:   'checkAddress_2',
                    likeStreetKladrId: that.addressValue.likeStreetKladrId,
                    house:  $(this).val(),
                    flagGetDeliveryPrice: 1,
                    orderPrice: that.fGetOrderPrice(),
                };
                if (that.version == 2) {
                    data = {
                        oper:       'checkAddress_3',
                        cityId:     that.addressValue.cityId,
                        likeCityId: that.addressValue.likeCityId,
                        likeStreet: that.addressValue.likeStreet,
                        strSearch:  $(this).val(),
                    };
                    if (that.flagDY2) {
                        data.flagDY2 = 1;
                    } else {
                        data.flagGetDeliveryPrice = 1;
                        data.orderPrice = that.fGetOrderPrice();
                    }
                }
                if (that.deliveryYandexStore) {
                    data.deliveryYandexStore = that.deliveryYandexStore;
                }

                that.fLog(data, 'data');

                that.objFieldHouseIconProcessing.show();

                that.fAjax({
                    data: data,
                    fSuccess: function(response) {
                        if (!response.dataForOrder) {
                            that.objFieldHouseStr.addClass('error');
                            that.fLog('not found');

                        } else if (that.deliveryYandexStore && !response.deliveryPrice) {
                            that.fieldHouseStrValue_prev = null;
                            that.objFieldHouseStr.addClass('error');
                            that.fShowMessage('Ошибка сервиса Яндекс Такси (код 1). Повторите попытку позже');

                        } else {
                            that.addressValue.dataForOrder        = response.dataForOrder;
                            if (that.version == 1) {
                                that.addressValue.dataForOrder.cityId = that.addressValue.cityId;
                            }
                            that.addressValue.houseValueIsSet = true;

                            that.objFieldHouseStr.val(response.house);
                            that.fieldHouseStrValue_prev = response.house;

                            if (that.deliveryYandexStore) {
                                fOnChange({flagUnset: false, deliveryPrice: parseFloat(response.deliveryPrice), deliveryTime: response.deliveryTime});
                            } else if (that.flagDY2) {
                                fOnChange({flagUnset: false, deliveryPriceList: response.deliveryPriceList});
                            } else {
                                fOnChange({flagUnset: false, deliveryPrice: parseFloat(response.deliveryPrice)});
                            }
                        }
                        that.fLog(that.addressValue, 'addressValue');

                        that.objFieldHouseIconProcessing.hide();
                        that.fSetProcessing(false);
                    } //
                });
            });

            function fOnChange(params_2) {
                if (params_2.flagUnset && that.last_fOnChange_flagUnset) {
                    return;
                }
                that.last_fOnChange_flagUnset = params_2.flagUnset;

                //that.fLog2(params_2, 'fOnChange');
                params.fOnChange(params_2);
            } //

            var arObjField = this.objFieldStreetStr.add(this.objFieldHouseStr);
            arObjField.focus(function(){
                arObjField.removeClass('error');
            });
    
        }, // function

        fRefreshAddressList: function() {
            this.fLog('setTimeout');

            var that  = this,
                timer = this.timer;

            timer.processing = false;
            timer.repeat     = false;
            timer.cancel     = false;

            timer.id = setTimeout(function(){
                that.fRefreshAddressList_hlp();
            }, timer.delay);
        }, // function
        
        fRefreshAddressList_hlp: function () {
            var that  = this,
                timer = this.timer;

            timer.processing = true;

            this.fLog('processing start');

            that.objFieldStreetIconProcessing.show();

            var data = {
                oper:         'getAddressList_2',
                searchString: timer.searchString
            };

            if (that.version == 2) {
                data = {
                    oper:         'getAddressList_3',
                    cityId:       that.addressValue.cityId,
                    searchString: timer.searchString,
                };
            }

            this.fLog(data, 'data');

            this.fAjax({
                data: data,
                fSuccess: function(response) {
                    if (timer.cancel) {
                        timer.id = null;
                        that.fLog('processing cancel');
                        return;
                    }
                    if (timer.repeat) {
                        that.fLog('repeat...');
                        that.fRefreshAddressList();
                        return;
                    }

                    if (that.version == 1) {
                        that.addressValue.cityId = response.cityId;
                    }
                    that.fFieldOptionsShow(response.html);

                    that.fLog('processing stop');
            
                    that.objFieldStreetIconProcessing.hide();

                    timer.id = null;
                } //
            });
        }, // function
        
        fFieldOptionsShow: function (content) {
            this.objFieldStreetOptions.css('top',   this.objFieldStreet.outerHeight() + 'px');
            this.objFieldStreetOptions.css('width', this.objFieldStreet.outerWidth() + 'px');

            this.objFieldStreetOptions.html(content);

            this.objFieldStreetOptions.show();
        }, // function

        fAfterRefreshPage: function() {
            this.objFieldStreetStr.val(this.streetValue);
            this.objFieldHouseStr.val(this.fieldHouseStrValue_prev);
        }, // function

        fAjax: function(params) {
            /*
                fAjax({
                    data: data,
                    fSuccess: function(response) {
                    }
                })
            */
            var that = this;

            $.ajax({
                url:      this.urlAjax,
                type:     'POST',
                dataType: 'json',
                data:     params.data,
                success: function (response) {

                    if (!response) {
                        that.fShowMessage('Ошибка 100');
                        that.fAjaxAfterError();
                    } else {
                        that.fLog(response);
                        if (response.error_msg) {
                            that.fShowMessage('Ошибка');
                            that.fAjaxAfterError();
                        } else {
                            params.fSuccess(response);
                        }
                    } //
                },
                error: function (response) {
                    that.fLog(response);
                    that.fShowMessage('Сбой на сервере!');
                    that.fAjaxAfterError();
                }
            });
        }, // function

        fAjaxAfterError: function() {
            this.timer.id = null;
            this.fSetProcessing(false);
            
            this.objFieldStreetIconProcessing.hide();
            this.objFieldHouseIconProcessing.hide();
        } // function

    }; // object

} // function

/* End */
;
; /* Start:"a:4:{s:4:"full";s:92:"/local/templates/.default/components/bitrix/sale.order.ajax/main/btnPayLog.js?16594265742069";s:6:"source";s:77:"/local/templates/.default/components/bitrix/sale.order.ajax/main/btnPayLog.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
window.sp_global.orderDeliveryBtnPayLog = function(params){

    function fLog(msg, label) {
        window.sp_global.fLog(msg, label);
    }

    fLog(params, 'orderDeliveryBtnPayLog');

    var objMain = $('.page-order-delivery .js-btn-pay-container'),
        flagLog = (params && params.flagLog);

    if (!objMain.length) {
        return;
    }

    function fAjax(data) {
        if (!flagLog) {
            return;
        }
        fLog(data, 'orderDeliveryBtnPayLog');

        $.ajax({
            url:  '/local/bitrix_ajax/ajax_misc.php',
            type: 'POST',
            data: {
                oper: 'logLogBtnPay',
                data: JSON.stringify(data),
            }
        });
    }

    var obj     = objMain.find('.sberbank__payment-link'),
        orderId = objMain.data('id'),
        msg     = 'width=' + window.innerWidth + ' ';
        
    msg += (!obj.length) ? 'jsBtnPay_no_link' : 'href=' + obj.attr('href');

    fAjax({
        oper:    'btnPayView',
        orderId: orderId,
        msg:     msg,
    });

    if (obj.length) {
        var time = new Date().getTime(),
            n    = 0,
            deltaTime;

        obj.append( objMain.find('.icon-processing').detach() );

        obj.click(function(e){
            if (objMain.hasClass('processing')) {
                e.preventDefault();
                return;
            }
            objMain.addClass('processing');

            deltaTime = ((new Date().getTime()) - time) / 1000;
            n++;

            fAjax({
                oper:    'btnPayClick',
                orderId: orderId,
                time:    deltaTime.toFixed(2),
                n:       n,
            });
        
            setTimeout(function(){
                objMain.removeClass('processing').addClass('show-msg-2');

                fAjax({
                    oper:    'btnPayClickAfter',
                    orderId: orderId,
                    time:    deltaTime.toFixed(2),
                    n:       n,
                });
            }, 5000);
        });
    }
} //

/* End */
;
; /* Start:"a:4:{s:4:"full";s:62:"/local/templates/v22/assets/js/page_order_2.js?171211423453555";s:6:"source";s:46:"/local/templates/v22/assets/js/page_order_2.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
window.sp_global.v22.scriptForPageOrder2 = function () {

    var fShowMessage = window.sp_global.v22.fShowMessage;

    function fLog(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    }

    var v22 = window.sp_global.v22,
        urlAjax = '/local/components/sp-artgroup/order_2.1/ajax_v22_2.php',

        objMain = $('.page-order-2'),
        objSidebar = $('.sidebar'),
        objTplContainer = $('.tpl-2-personal-container'),

        timer = {
            id: null,
            basketItemId: 0,     // Данный элемент корзины можно редактировать (количество и "удалить") пока запрос не отправлен
            delay: 1000,
        },

        stepStoreTotalSum = false,

        storesController = new sp_global.orderShared2.storesController();
    // ###



    function fAjax(params) {
        /*
            fAjax({
                data: data,
                fSuccess: function(response) {
                }
            })
        */

        $.ajax({
            url: urlAjax,
            type: 'POST',
            dataType: 'html',
            data: params.data,
            success: function (response) {
                //fLog(response);
                params.fSuccess(response);
            },
            error: function () {
                timer.id = null;
                v22.fSetProcessing(false);
                fShowMessage('Нет соединения с сервером (код ошибки: 41)'); // # ajax error #
            }
        });
    } // function

    function fRender(response, params) {
        storesController.storesMapController.fResetMap();

        var objTMP = $('<div></div>');
        objTMP.html(response);

        var objPageOrder = objTMP.find('.page-order-2'),
            objData = objTMP.find('#data'),
            objSimilar = objTMP.find('.page-order-2--block-similar-products');
            objViewed = objTMP.find('.page-order-2--block-viewed-products');

        if (!objPageOrder.length) {
            // В response нет контента
            fLog(response);
            var msg = 'Ошибка 1';
            try {
                var data = JSON.parse(response);
                fLog(data);
                if (data.error_msg == undefined) {
                    // Неизвестный json (не содержит "error_msg")
                    msg = 'Ошибка 3';
                } else {
                    msg = 'Ошибка: ' + data.error_msg;
                }
            } catch (e) {
                // Не json
                msg = 'Ошибка 2';
            }
            fShowMessage(msg);
            return;
        } //

        // Для отладки
        if (objData.length) {
            var data = JSON.parse(objData.html());
            fLog(data, 'debugInfo');
        }

        // Обновляем содержимое
        objMain.html(objPageOrder.html());

        if (!params || !params.doNotBindEventHandlers) {
            fBindHandlers();
            v22.favoriteProduct.fApply(objMain);
        } //

        // Индивидуальные предложения
        if (objPageOrder.data('keep_similar')) {
            fLog('similar: keep');
        } else {
            let parentOfSimilarProducts = $('.page-order-2--block-similar-products').parent();
            $('.page-order-2--block-similar-products').remove();

            if (objSimilar.length) {
                fLog('similar: update');
                parentOfSimilarProducts.append(objSimilar);
                fBindHandlersSimilar();
                v22.favoriteProduct.fApply(objSimilar);
            }
        }

        // Вы смотрели
        if (objPageOrder.data('keep_viewed')) {
            fLog('viewed: keep');
        } else {
            let parentOfViewedProducts = $('.page-order-2--block-viewed-products').parent();
            $('.page-order-2--block-viewed-products').remove();

            if (objViewed.length) {
                fLog('similar: update');
                parentOfViewedProducts.append(objViewed);
                fBindHandlersViewed();
                v22.favoriteProduct.fApply(objViewed);
            }
        }

        var step = objPageOrder.data('step');
        if (step == 'StepOutOfStock') {
            fHideStep();
            objMain.find('.js-step-out-of-stock').addClass('active');
        } else if (step == 'StepPreview') {
            fHideStep();
            objMain.find('.js-step-preview').addClass('active');
        }

    } // function

    function fHideStep() {
        objMain.find('.block-step').removeClass('active');
        objSidebar.addClass('m-hidden');
        $('.page-order-2__step-first-additional-blocks').addClass('m-hidden');
        $(document).scrollTop(0);
    } // function

    function fBindHandlersQuantity(objQuantity, params) {
        /*  fBindHandlersQuantity(objQuantity, {
                basketItemId:    basketItemId,
                maxQuantity:     maxQuantity,
                quantityInitial: quantityInitial,
                quantityPrev:    quantityPrev,

                source:          'stepOutOfStock',
                mainStoreId:     mainStoreId,
            })
        */
        var basketItemId = params.basketItemId,
            maxQuantity = params.maxQuantity,
            quantityInitial = params.quantityInitial,
            quantityPrev = params.quantityPrev;

        function fObjQuantity_Change(k) {
            var objInput = objQuantity.find('.js-value'),
                value = parseInt(objInput.val());

            if (v22.fIsProcessing() || (timer.id && timer.basketItemId != basketItemId)) {
                fLog('Processing or timer');
                objInput.val(quantityPrev);
                return;
            }

            if (isNaN(value)) {
                value = 1;
            } else {
                value += k;
                value = Math.max(1, value);
            }

            if (value > maxQuantity) {
                value = maxQuantity;
                v22.fShowGlobalMsg(objQuantity.find('.global-error-msg'));
            }

            objInput.val(value);

            if (value == quantityPrev) {
                fLog('value == quantityPrev');
                return;
            }

            quantityPrev = value;

            if (timer.id) {
                clearTimeout(timer.id);
                fLog('stop timer');
            }

            if (quantityInitial == value) {
                fLog('quantityInitial == value');
                timer.id = null;
                return;
            }

            // Заводим таймер

            fLog('start timer; value=' + value);

            timer.basketItemId = basketItemId;

            timer.id = setTimeout(function () {

                if (!v22.fSetProcessing(true)) {
                    fLog('timer: processing: cancel');
                    quantityPrev = quantityInitial;
                    objInput.val(quantityInitial);

                    timer.id = null;
                    return;
                }

                var data = {
                    oper: 'v22_SetQuantity',
                    basketItemId: basketItemId,
                    quantity: value,
                };

                if (params.source == 'stepOutOfStock') {
                    data.source = 'stepOutOfStock';
                    data.mainStoreId = params.mainStoreId;
                }

                fLog(data, 'quantity');

                $.fancybox.close();

                fAjax({
                    data: data,
                    fSuccess: function (response) {
                        fRender(response);
                        timer.id = null;
                        v22.fSetProcessing(false);
                    }
                });
            }, timer.delay);

        } // function

        objQuantity.find('.js-value').change(function () {
            fObjQuantity_Change(0);
        });

        // Привязка события для прибавления товара в корзину
        objQuantity.find('.js-btn-plus').click(function () {
            fObjQuantity_Change(1);
        });

        // Привязка события для убавления товара в корзину
        objQuantity.find('.js-btn-minus').click(function () {
            fObjQuantity_Change(-1);
        });

        // Привязка события для удаления товара из корзины
        let objActionButtons = objQuantity.closest(".item");
        if (objActionButtons) {
            objActionButtons.find('.js-btn-remove').click(function () {
                fRemoveItem(params);
            });
        }

    } // function

    function fRemoveItem(params) {
        /*  fRemoveItem({
                basketItemId: basketItemId,

                source:       'stepOutOfStock',
                mainStoreId:  mainStoreId,
            });
        */
        var basketItemId = params.basketItemId;

        if ((timer.id && timer.basketItemId != basketItemId) || !v22.fSetProcessing(true)) {
            fLog('Processing or timer');
            return;
        }

        if (timer.id) {
            clearTimeout(timer.id);
            timer.id = null;
            fLog('stop timer');
        }

        var data = {
            oper: 'v22_RemoveItem',
            basketItemId: basketItemId,
        };

        if (params.source == 'stepOutOfStock') {
            data.source = 'stepOutOfStock';
            data.mainStoreId = params.mainStoreId;
        }

        fLog(data, 'remove item');

        $.fancybox.close();

        fAjax({
            data: data,
            fSuccess: function (response) {
                fRender(response);
                v22.fSetProcessing(false);
            }
        });
    } // function

    function fBindHandlers() {

        let objDeliveryTypesPanel = $('.js-step-1 .js-delivery-type'),
            objDeliveryType = objDeliveryTypesPanel.find('.js-btn-delivery'),
            objPickupType = objDeliveryTypesPanel.find('.js-btn-pickup');

        // Список товаров
        objMain.find('.order-products .js-catalog-item').each(function () {
            var objItem = $(this),
                objQuantity = objItem.find('.js-quantity-container'),
                objActionButtons = objItem.find('.js-action-buttons-container'),
                objBlockProductForStoreList = objItem.find('.js-block-product-for-store-list'),

                productId = objItem.data('product_id'),
                basketItemId = objItem.data('basket_item_id'),
                storeList = objItem.data('store_list'),

                maxQuantity = objItem.data('quantity_max'),
                quantityInitial = objQuantity.find('.js-value').val(),
                quantityPrev = quantityInitial,

                canBuy = Boolean(objQuantity.length);


            // js-block-product-for-store-list: price, quantity
            if (canBuy) {
                var objPrice = objItem.find('.js-block-price'),
                    objPrice2 = objBlockProductForStoreList.find('.hor-card__price'),
                    objQuantity2 = objBlockProductForStoreList.find('.js-btn-buy-container-modal');

                objPrice2.html(objPrice.html());
                objQuantity2.html(objQuantity.html());
            }

            var params_fBindHandlersQuantity = {
                basketItemId: basketItemId,
                maxQuantity: maxQuantity,
                quantityInitial: quantityInitial,
                quantityPrev: quantityPrev,
            };

            if (objQuantity.length) {
                fBindHandlersQuantity(objQuantity, params_fBindHandlersQuantity);
            } else {
                objItem.find('.js-btn-remove').click(function () {
                    fRemoveItem({ basketItemId: basketItemId });
                });
            }

            // Модальное окно "Доставка"
            objItem.find('.js-btn-catalog-delivery').click(function () {

                if (timer.id || v22.btnBuy.timer.id) {
                    fLog('wait');
                    return;
                }

                var objModal = $('.modal-catalog-delivery');

                objModal.find('.js-block-product').html(objBlockProductForStoreList.html());

                fBindHandlersQuantity(objModal.find('.js-block-product .js-btn-buy-container-modal'), params_fBindHandlersQuantity);

                $.fancybox.open(objModal);
            });

            // Модальное окно "Список аптек"
            objItem.find('.js-btn-catalog-stores').click(function () {

                if (timer.id || v22.btnBuy.timer.id) {
                    fLog('wait');
                    return;
                }

                var objModal = $('.catalog-store-list');

                objModal.find('.js-block-product').html(objBlockProductForStoreList.html());

                fBindHandlersQuantity(objModal.find('.js-block-product .js-btn-buy-container-modal'), params_fBindHandlersQuantity);

                v22.fShowCatalogStores({
                    storeList: storeList,
                    flagPageOrder: true,
                });
            });

            objItem.find('.js-btn-subscription').click(function () {
                v22.fBtnSubscriptionClick({ product_id: productId });
            });

            objItem.find('.js-btn-special-order').click(function () {
                v22.fBtnSpecialOrderClick({ product_id: productId });
            });

        });

        // Пакеты (упаковка)
        objMain.find('.block-step-first .block-bag-list .item:not(.active)').each(function () {

            var obj = $(this),
                productId = obj.data('product_id');

            obj.find('.b-checkbox').click(function () {

                if (timer.id || !v22.fSetProcessing(true)) {
                    fLog('Processing or timer');
                    return;
                }

                var data = {
                    oper: 'basketAdd',
                    productId: productId,
                    quantity: 1,
                };

                fLog(data, 'add package');

                v22.fAjax({
                    url: '/local/bitrix_ajax/v22.php',
                    data: data,
                    fSuccess: function (response) {
                        fLog(response);
                        v22.fSetProcessing(false);
                        v22.fOrderRefresh();
                    }
                });
            });
        });

        // Очистить корзину
        objMain.find('.block-template__clear').click(function () {

            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                oper: 'v22_RemoveItem',
                basketItemId: 'all_delete',
            };

            fLog(data, 'remove all items');

            fAjax({
                data: data,
                fSuccess: function (response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });
        });

        // Промокоды
        fBindHandlersPromocodes();

        /* DEPRECATED */
        // Купон: активировать
        objMain.find('.js-btn-set-coupon').click(function () {
            var coupon = objMain.find('.js-coupon').val();
            coupon = coupon.trim();

            if (coupon.search(/^[а-яА-ЯёЁa-zA-Z0-9\-]{3,}$/) == -1) {
                objMain.find('.js-coupon').addClass('m-error');
                objMain.find('.block-coupon .error-msg').addClass('m-hidden');
                objMain.find('.block-coupon .error-msg.js-invalid-coupon').removeClass('m-hidden');
                return;
            }

            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                operation: 'set_coupon',
                coupon: coupon,
            };

            fLog(data, 'set coupon');

            fAjax({
                data: data,
                fSuccess: function (response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });

        });
        objMain.find('.js-coupon').keydown(function (event) {
            if (event.which == 13) {
                objMain.find('.js-btn-set-coupon').click();
            }
        });

        // Купон: деактивировать
        objMain.find('.js-btn-unset-coupon').click(function () {

            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                operation: 'unset_coupon',
                coupon: 'unset',
            };

            fLog(data, 'unset coupon');

            fAjax({
                data: data,
                fSuccess: function (response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });

        });

        // Купон: деактивировать один
        objMain.find('.coupon-list .item').click(function () {

            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                operation: 'unset_coupon',
                coupon: $(this).html(),
            };

            fLog(data, 'unset one coupon');

            fAjax({
                data: data,
                fSuccess: function (response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });

        });
        /* DEPRECATED */

        // Авторизация
        objMain.find('.js-step-1 .stb-total .b-bonus .btn-auth').click(function () {
            v22.fAuthShowForm({ form: 'authorization' });
        });

        objBlockTotal.fInit();

        // Бонусы
        if (objMain.find('.js-step-1 .stb-total .b-bonus .b-auth .js-value-input').length) {

            v22.objBonusWithdraw.fInit({
                objBlockSlider: objMain.find('.js-step-1 .stb-total .b-bonus .b-auth'),
                fCalcTotal: function () {
                    objBlockTotal.fCalc();
                },
            });
        } //

        objBlockTotal.fCalc();

        // Бесплатная доставка
        var objModalBuyMore = objMain.find('.modal-buy-more'),
            flagModalBuyMore = false,
            flagModalBuyMoreDisable = false;

        if (objModalBuyMore.length) {
            objMain.find('.js-step-1 .js-btn-next-step').click(function () {
                if (flagModalBuyMoreDisable) {
                    return;
                }
                flagModalBuyMore = true;
                flagModalBuyMoreDisable = true;

                $.fancybox.open(objModalBuyMore);
            });
            //objMain.find('.js-step-1 .js-btn-next-step').click();
        } //


        // Кнопки для переключение способа получения заказа
        function fBindHandlersDeliveryTypePanel() {
            objDeliveryType.click(function () {
                objPickupType.removeClass("active");
                objDeliveryType.addClass("active");
            });

            objPickupType.click(function () {
                objDeliveryType.removeClass("active");
                objPickupType.addClass("active");
            });
        }

        fBindHandlersDeliveryTypePanel();


        objMain.find('.js-step-1 .js-btn-next-step').click(function () {
            // step-1       -> step-store


            // Если выбран способ получения заказа "Доставка"
            if (objDeliveryType.hasClass('active')) {
                // Перенаправление на страницу с оформлением доставки
                document.location.href = $(".js-data-delivery-page-url").text();
                return;
            }

            if (flagModalBuyMore) {
                flagModalBuyMore = false;
                return;
            }
            fHideStep();

            objMain.find('.js-step-store').addClass('active');

            if (stepStoreTotalSum) {
                var objStoreList = objMain.find('.js-step-store .js-store-list');

                objStoreList.toggleClass('bonus-withdraw', stepStoreTotalSum.flagBonusWithdraw);
                objStoreList.find('.item[data-type=all] .js-order-price').html(stepStoreTotalSum.sum);

                let lastActiveStoreItem = objStoreList.find('.js-select-store.active');
                if (lastActiveStoreItem.length > 0) {
                    lastActiveStoreItem.click().click();
                    console.log(v22.objBonusWithdraw.bonusWithdraw);
                }

                stepStoreTotalSum = false;
            };
        });

        objMain.find('.js-step-store .js-btn-prev-step').click(function () {
            // step-store -> step-1
            fHideStep();

            objMain.find('.js-step-1').addClass('active');
            objSidebar.removeClass('m-hidden');
            $('.page-order-2__step-first-additional-blocks').removeClass('m-hidden');
        });

        fBindHandlersStepStore();
        fBindHandlersBreakPointSuccess();
    } // function

    function fBindHandlersPromocodes(params) {
        let objCouponBlock = objMain.find(".b-coupon"),

            objPromocodeField = objCouponBlock.find(".js-field-promocode"),

            objPromocodesListWrapper = objCouponBlock.find(".promocodes-list__wrapper"),
            objPromocodesList = objPromocodesListWrapper.find(".promocodes-list"),

            objPromocodes = objPromocodesList.find(".promocode");


        if (objPromocodesListWrapper.length == 0) {
            return;
        }

        if(params == undefined) {
            params = {};
        }

        if(params.isCallFromListUpdated == undefined) {
            params.isCallFromListUpdated = false;
        }

        if(!params.isCallFromListUpdated)
        {
            objPromocodesListWrapper.on("listUpdated", function() {
                objPromocodesListWrapper.find('.js-scroll-container').scrollbar();
                fBindHandlersPromocodes({isCallFromListUpdated: true});
            });
        }


        // Отображение / Скрытие списка промокодов при активации / деактивации поля ввода
        objPromocodeField.focus(function () {
            objCouponBlock.addClass("open");
        });

        $(document).click(function (event) {
            if ($(event.target).is(objPromocodeField) || $(event.target).hasClass("promocodes-list") || $(event.target).parents(".promocodes-list").length > 0) {
                return;
            }

            objCouponBlock.removeClass("open");
        })


        // При нажатии на промокод вставить его в поле ввода
        objPromocodes.find(".promocode__number-wrapper").click(function () {
            objPromocodeField.val($(this).text().trim());
            objCouponBlock.removeClass("open");
        });


        // Действия при нажатии на "Применить" (Прикрепить промокод к заказу)
        objCouponBlock.find(".js-apply-promocode").click(function () {
            let coupon = objPromocodeField.val().trim();

            if (coupon.search(/^[а-яА-ЯёЁa-zA-Z0-9\-]{3,}$/) == -1) {
                objMain.find('.js-field-promocode').addClass('error');
                objMain.find('.b-coupon .error-msg').addClass('m-hidden');
                objMain.find('.b-coupon .error-msg.js-msg-invalid-coupon').removeClass('m-hidden');
                return;
            }


            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                operation: 'set_coupon_v2',
                coupon: coupon,
            };

            fLog(data, 'set set_coupon_v2');

            fAjax({
                data: data,
                fSuccess: function (response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });
        });


        // Действия при нажатии на "Применён" (Открепить промокод от заказа)
        objCouponBlock.find(".js-unset-promocode").click(function () {
            if (timer.id || !v22.fSetProcessing(true)) {
                fLog('Processing or timer');
                return;
            }

            var data = {
                operation: 'unset_coupon',
                coupon: 'unset',
            };

            fLog(data, 'unset coupon');

            fAjax({
                data: data,
                fSuccess: function (response) {
                    fRender(response);
                    v22.fSetProcessing(false);
                }
            });
        });
    }

    function fBindHandlersSimilar() {
        var objTMP = $(`.page-order-2--block-similar-products`);
        fBindHandlersAdditionalBlock(objTMP);
    } // function

    function fBindHandlersViewed() {
        var objTMP = $(`.page-order-2--block-viewed-products`);
        fBindHandlersAdditionalBlock(objTMP);
    } // function

    function fBindHandlersAdditionalBlock(objTMP) {
        if (!objTMP.length) {
            return;
        }

        let slidesToShow = Math.floor($('.tpl-2-container').width() / 280);

        objTMP.each(function () {
            $(this).find(".block-catalog-section-h").slick({
                infinite: true,
                slidesToShow: slidesToShow,
                slidesToScroll: slidesToShow,
                variableWidth: true,

                prevArrow: $(this).find('.block-template__prev'),
                nextArrow: $(this).find('.block-template__next'),

                responsive: [
                    {
                        breakpoint: 768,
                        settings:
                        {
                            infinite: false,
                            slidesToShow: 1,
                            slidesToScroll: 1,
                        }
                    },
                    {
                        breakpoint: 432,
                        settings: {
                            infinite: false,
                            slidesToScroll: 1
                        }
                    }
                ],
            });
        });

        v22.scriptForPage.catalog_section_h__bindHandlers();
    }

    v22.fOrderRefresh = function () {
        if (timer.id || !v22.fSetProcessing(true)) {
            fLog('Processing or timer');
            return;
        }

        var data = {
            oper: 'render',
        };

        fLog(data, 'fOrderRefresh');

        fAjax({
            data: data,
            fSuccess: function (response) {
                fRender(response);
                v22.fSetProcessing(false);
            }
        });
    };

    function fBindHandlersStepStore() {

        function fLog2(msg, label) {
            //window.sp_global.v22.fLog(msg, label);
        }

        var objStepStore = objMain.find('.js-step-store'),
            objStoreList = objStepStore.find('.js-store-list'),
            objBlockConfirmOrder = objStepStore.find('.js-b-confirm-order'),
            objBtnConfirmOrder = objMain.find('.js-btn-confirm-order'),
            objSelectStoreWarning = objMain.find('.js-select-store-warning'),

            objStoreSelection = objMain.find(".b-store-selection"),
            arObjStoreItems = objStoreList.find('.item'),
            arObjBtnShowProductList = objStoreList.find('.b-status .b-status-wrapper'),
            objNotFound = objStoreList.find('.not-found'),

            objMapStore = objStepStore.find('.block-map-wrapper .block-map-store'),

            objNearbyStoreMode = objStepStore.find('.js-store-modes .js-nearby-store-mode'),
            objTypeAllStoreMode = objStepStore.find('.js-store-modes .js-type-all-store-mode'),
            obj24hStoreMode = objStepStore.find('.js-store-modes .js-24h-store-mode'),

            storeId = 0,

            objSearchInput = objStepStore.find('.search-wrapper input'),

            objStoresPager = objStepStore.find('.js-btn-stores-pager'),

            urlOrderDelivery = objStoreList.data('url_order_delivery'),
            flagMobile = (window.innerWidth < 768),

            objBlockCustomer = objStepStore.find('.block-user-enter-info'),
            dataForCreateOrder = {};


        storesController.objStoreList = objStoreList;
        storesController.objSelectedStoreBottomPanel = $(".js-bottom-panel-selected-store");

        storesController.objStoreSelection = objStoreSelection;


        /** Обновление данных в зависимости от выбранного магазина, для второго шага с подтверждением заказа */
        function fCalcConfirmOrderInfo(params) {
            let obj = params.obj;

            // Изначальная цена
            let oldPrice = obj.find(".js-order-old-price").text();

            // При её отсутствий использовать текущую
            let price = obj.find(".js-order-price").text();
            objBlockConfirmOrder.find(".js-confirm-order-old-price").text((oldPrice) ? oldPrice : price);


            // Доступное количество товаров
            let availiableQuantity = obj.data("availiable-quantity");
            let quantityString = obj.data("quantity-string");
            objBlockConfirmOrder.find(".js-confirm-order-quantity").text(`${availiableQuantity} ${quantityString}`);


            // Скидка
            let discount = obj.data("discount");
            objBlockConfirmOrder.find(".js-confirm-order-discount").text(discount);


            // Бонусы за покупку
            let bonusPrihod = obj.data("bonus-prihod");
            objBlockConfirmOrder.find(".js-confirm-order-bonus-prihod").text(bonusPrihod);
            objBlockConfirmOrder.find(".js-b-confirm-order-bonus-prihod").toggleClass("d-none", (v22.objBonusWithdraw.bonusWithdraw > 0));


            // Итоговая цена
            objBlockConfirmOrder.find(".js-confirm-order-price").text(price);
        }

        function fSetStoreActive(params) {
            /*  
                fSetStoreActive({
                    obj:                  $(this),
                    id:                   id,
                    flagScrollMap:        true,
                    flagDisableMapModes:  true
                })
            */

            var obj = params.obj;
            storesController.objSelectedOnMap = obj;

            if (!obj) {
                obj = arObjStoreItems.filter('[data-id=' + params.id + ']');
            }
            if (!obj) {
                return;
            }

            var id = obj.data('id'),
                latitude = obj.data('latitude'),
                longitude = obj.data('longitude'),
                type = obj.data('type'),
                arObjStoreItemsOnMap = objMain.find(`.item-on-map[data-id=${id}]`),

                flagDisabled = Boolean(obj.data('disabled'));

            if (flagDisabled || type == "nothing") {
                storeId = 0;
                return;
            }

            objSelectStoreWarning.toggleClass('show', flagDisabled);
            objBlockConfirmOrder.toggleClass('store-selected', !flagDisabled);


            arObjStoreItems.removeClass('active');
            arObjStoreItemsOnMap.removeClass('active');

            arObjStoreItems.find('.js-select-store').removeClass('active');
            arObjStoreItemsOnMap.find('.js-select-store').removeClass('active');


            storeId = id;

            obj.addClass('active');
            arObjStoreItemsOnMap.addClass('active');

            obj.find('.js-select-store:not(.js-yandex)').addClass('active');
            arObjStoreItemsOnMap.find('.js-select-store:not(.js-yandex)').addClass('active');


            if (storesController.ymapsMap()) {
                storesController.fSetPlacemarkActive(storesController.placemarkList()[id]);
                if (params.flagScrollMap) {
                    storesController.ymapsMap().setCenter([latitude, longitude], 16);
                }
            }

            if (flagMobile) {
                if (id == 'delivery') {
                    objMapStore.html('');
                } else {
                    objMapStore.html(obj[0].outerHTML);
                }
            }

            fCalcConfirmOrderInfo({ obj })
        } // function

        function fSetStoreInactive(params) {
            /*  
                fSetStoreInactive({
                    calledFrom: "list" || "map",
                })
            */
            params = params ?? {};

            if (params.calledFrom == "list" && storesController.getActivatedPlacemark()) {
                try {
                    $(".js-bottom-panel-selected-store").empty();
                    storesController.getActivatedPlacemark().balloon.close();
                } catch (error) { }
            }

            storesController.fSetPlacemarkActive(false);
            storesController.objSelectedOnMap = null;

            let arObjStoreItemsOnMap = objMain.find(`.item-on-map[data-id=${storeId}]`);

            storeId = 0;

            arObjStoreItems.removeClass('active');
            arObjStoreItemsOnMap.removeClass('active');

            arObjStoreItems.find('.js-select-store').removeClass('active');
            arObjStoreItemsOnMap.find('.js-select-store').removeClass('active');

            objBlockConfirmOrder.removeClass('store-selected');
        } // function

        function fBindHandlerBtnShowProductList(objStoreList) {

            /** Метод для деактивации всех открытых выпадающих списков с товарами (Кроме, находящегося снизу карты для моб. платформ) */
            function fDeactivateAnyOpenedDropdown(event) {
                $(":not(.js-bottom-panel-selected-store) > .dropdown-list-open .dropdown-list").each(function (i) {
                    if (!$.contains($(this)[0], event.target) && $(this)[0] != event.target) {
                        // Сбросить скролл в выпадающем списке
                        $(this).scrollTop(0);

                        // Закрыть выпадающий список
                        $(this).closest(".dropdown-list-open").removeClass("dropdown-list-open");
                    }
                })
            }

            objStoreList.on("click", '.b-status-wrapper', function (event) {
                event.stopPropagation();
                fDeactivateAnyOpenedDropdown(event);

                var objItem = $(this).closest('.item, .item-on-map'),
                    flagOpen = !objItem.hasClass('dropdown-list-open');

                objItem.toggleClass('dropdown-list-open', flagOpen);
            });

            $("body").click(function (event) {
                if (window.innerWidth > 1300) {
                    fDeactivateAnyOpenedDropdown(event);
                }
            })


        } // function

        objNearbyStoreMode.click(function (event) {
            storesController.fToggleMapMode({
                obj: $(this),
                state: event.state,
                isFireAction: event.isFireAction,
            });
        });

        objTypeAllStoreMode.click(function (event) {
            storesController.fToggleMapMode({
                obj: $(this),
                state: event.state,
                isFireAction: event.isFireAction,
            });
        });

        obj24hStoreMode.click(function (event) {
            storesController.fToggleMapMode({
                obj: $(this),
                state: event.state,
                isFireAction: event.isFireAction,
            });
        });

        arObjStoreItems.find('.js-select-store').click(function (event) {
            event.stopPropagation();

            var objItem = $(this).closest('.item');
            arObjType_1 = objItem.find('.js-select-store:not(.js-yandex)'),
                arObjType_2 = objItem.find('.js-select-store.js-yandex'),
                obj = $(this);

            if (!objItem.hasClass('active')) {
                fSetStoreActive({
                    obj: objItem,
                    flagScrollMap: true,
                    flagDisableMapModes: true
                });
            } else {
                // item active
                if (!arObjType_2.length) {
                    // js-select-store active
                    fSetStoreInactive({
                        calledFrom: 'list'
                    });
                } else {
                    if (obj.hasClass('active')) {
                        fSetStoreInactive({
                            calledFrom: 'list'
                        });
                    }
                }
            } //
        });

        objStoreList.on('click', '.item', function (event) {
            if ($(event.target).hasClass("dropdown-list") || $(event.target).closest(".dropdown-list").length > 0) {
                return;
            }

            var objItem = $(this);

            if (!objItem.hasClass('active')) {
                fSetStoreActive({
                    obj: objItem,
                    flagScrollMap: true,
                    flagDisableMapModes: true
                });
            } else {
                fSetStoreInactive({
                    calledFrom: 'list'
                });
            }
        });


        /* Инициализация работы постепенного отображения аптек при нажатии на кнопку "Показать ещё" */
        storesController.storesPager.init(objStoresPager);


        function fBindHandlersStoreListPanel() {
            var arObjBtnList = objStepStore.find('.store-list-panel .js-btn-list'),
                arObjBtnMap = objStepStore.find('.store-list-panel .js-btn-map'),
                objBtnSearch = objStepStore.find('.store-list-panel .button-search');


            ymaps.ready(function () {
                fInitMap();
                storesController.storesFilterController.fFilterMap();

                setTimeout(function () {
                    if (storesController.objSelectedOnMap) {
                        fSetStoreActive({
                            obj: storesController.objSelectedOnMap,
                            flagScrollMap: true
                        });
                        storesController.objSelectedOnMap = null;
                    }
                }, 100);
            });

            function fStoreListPanel(tab) {
                var flagList = (tab == 'list');

                arObjBtnList.toggleClass('active', flagList);
                arObjBtnMap.toggleClass('active', !flagList);

                objStepStore.find('.block-main').toggleClass('type-2', !flagList).toggleClass('type-1', flagList);
                objStoreList.toggleClass('type-2', !flagList).toggleClass('type-1', flagList);

                if (!flagList && !flagMobile && storeId) {
                    fScrollStoreList(arObjStoreItems.filter('.active')[0].offsetTop);
                }

                if (flagList && flagMobile && scrollStoreListObj) {
                    fScrollStoreList2(scrollStoreListObj);
                    scrollStoreListObj = null;
                }
            } // function

            arObjBtnList.click(function () {
                if (!$(this).hasClass('active')) {
                    fStoreListPanel('list');
                }
            });
            arObjBtnMap.click(function () {
                if (!$(this).hasClass('active')) {
                    fStoreListPanel('map');
                }
            });

            // Установка отображения аптек в виде списка
            arObjBtnList.click();

            if (flagMobile) {
                objStepStore.find('.block-main').addClass('mobile');
            }

            objSearchInput.on('input', function () {
                storesController.storesFilterController.fFilterMap();
            });

        } // function

        fBindHandlersStoreListPanel();
        fBindHandlerBtnShowProductList(objStoreList);

        if (objBlockCustomer.length) {
            var objCustomerFio = objBlockCustomer.find('.js-field-customer-fio')
            objCustomerPhone = objBlockCustomer.find('.js-field-customer-phone input'),
                objCustomerEmail = objBlockCustomer.find('.js-field-customer-email input'),
                objMsgWrongCoupon = objBlockCustomer.find('.js-field-customer-phone .msg-wrong-coupon');

            objCustomerPhone.focus(function () {
                $(this).removeClass('error');
            });

            // Проверка купона
            if (objMsgWrongCoupon.length) {

                v22.objCheckCouponOnePerUser.fInit({
                    objPhone: objCustomerPhone,
                    objIconProcessing: objBlockCustomer.find('.js-field-customer-phone .icon-processing'),
                    objMsg: objMsgWrongCoupon,

                    timer: timer,
                });
            } //

            objCustomerPhone.inputmask({
                mask: '+7 (999) 999 99 99',
            });
        } //

        function fCreateOrder(flagRecursion) {

            if (!flagRecursion && !v22.fSetProcessing(true)) {
                return; // Не должно
            }

            fAjax({
                data: dataForCreateOrder,
                fSuccess: function (response) {
                    fRender(response, { doNotBindEventHandlers: false });

                    var obj = objMain.find('.js-data-create-order');

                    if (!obj.length) {
                        v22.fSetProcessing(false);
                    } else {
                        dataForCreateOrder = JSON.parse(obj.html());

                        setTimeout(function () {
                            fCreateOrder(true);
                        }, 1000);
                    }
                }
            });
        } // function

        objBtnConfirmOrder.click(function () {

            if (!storeId) {
                objSelectStoreWarning.addClass('show');
                return;
            }

            if (timer.id || v22.fIsProcessing()) {
                fLog('Processing or timer');
            }

            $(document).scrollTop(0);

            dataForCreateOrder = {
                BREAK_POINT: objStepStore.data('break_point'), // BEGIN | ADD_USER
                sessid: objStepStore.data('sessid'),
                BONUS_WITHDRAW: v22.objBonusWithdraw.bonusWithdraw,
                USER_CONSENT: $('#user_consent').is(':checked') ? 'Y' : 'N',
                skipPreviewPage: 1,
                isSkipAbsentProducts: 1,
                storeId: storeId
            };

            if (objBlockCustomer.length) {
                dataForCreateOrder.USER_PHONE = objCustomerPhone.val();
                dataForCreateOrder.USER_FIO = objCustomerFio.val();

                if (!v22.fValidatePhone(dataForCreateOrder.USER_PHONE)) {
                    objCustomerPhone.addClass('error');
                    return;
                }

                if (objMsgWrongCoupon.length && !v22.objCheckCouponOnePerUser.fIsChecked()) {

                    if (!v22.fSetProcessing(true)) {
                        return;
                    }
                    v22.objCheckCouponOnePerUser.fCheckCouponOnePerUser({
                        fCallback: function (flagValid) {
                            if (flagValid) {
                                fCreateOrder();
                            }
                        },
                    });
                } else {
                    fCreateOrder();
                }
            } else {
                fCreateOrder();
            }
        });

        storesController.fSetStoreActive = fSetStoreActive;
        storesController.fSetStoreInactive = fSetStoreInactive;
        storesController.fBindHandlerBtnShowProductList = fBindHandlerBtnShowProductList;

        // ### map (

        var scrollStoreListObj = null;

        objMapBlock = objStepStore.find('.block-map');

        storesController.objMapBlock = objMapBlock;

        function fInitMap() {
            let params = {}

            params.yMapsBalloonContentLayoutClass = ymaps.templateLayoutFactory.createClass(
                `
                <div 
                    class="item-on-map"
                    data-id={{ properties.storeId }}
                    data-type={{ properties.storeType }}>    
                    <div class="close-button"></div>

                    <div class="store-top">
                        <div class="store-address-container">
                            <div class="store-icon js-store-icon">   
                                <img></img>
                            </div>

                            <div class="store-address">
                                {{ properties.storeAddress }}
                            </div>
                        </div>

                        <div class="store-name">
                            {{ properties.storeName }}
                        </div>

                        <div class="store-schedule">
                            {{ properties.storeSchedule }}
                        </div>
                    </div>

                    <div class="js-b-status"></div>


                    <div class="store-bottom">
                        <div class="price js-b-price"></div>
                        
                        <div class="b-select-store">
                            <button class="template__button select-store js-select-store">
                                <div class="template__button__active-text">
                                    <div class="template__button__active-text--all">
                                        Забрать заказ здесь
                                    </div>

                                    <div class="template__button__active-text--mobile">
                                        Забрать здесь
                                    </div>
                                </div>
                                <div class="template__button__disabled-text">
                                    Выбрано
                                </div>
                            </button>
                        </div>
                    </div>
                </div>
                `
            );

            storesController.fInitMap(params);
        } // function

        storesController.fInitMapFromComponent = fInitMap;

        function fRefreshMap() {
            storesController.fRefreshMap();
        } // function

        function fScrollStoreList(offset) {
            objMain.find('.js-scroll-container').animate({
                scrollTop: offset,
            }, 1000);
        } // function

        function fScrollStoreList2(obj) {
            obj = obj[0];

            if (0) {
                obj.scrollIntoView({ behavior: 'smooth', block: 'center' });

            } else if (1) {
                var offset = obj.offsetTop;

                while (obj = obj.offsetParent) {
                    offset += obj.offsetTop;
                }
                offset -= $('.header__fixed').outerHeight();

                //fLog(offset);

                if (0) {
                    $(document).scrollTop(offset);
                } else {
                    $('html, body').animate({
                        scrollTop: offset,
                    }, 1000);
                }
            } //
        } // function

        // ### )

    } // function

    function fBindHandlersBreakPointSuccess() {

        var objBtnResumeBuy = objMain.find('.js-btn-resume-buy');

        objBtnResumeBuy.click(function () {
            v22.fOrderRefresh();
        });
    } // function


    // # objBlockTotal (
    var objBlockTotal = {

        fInit: function () {
            var objTotal = objMain.find('.js-step-1 .stb-total'),
                objTotalSum = objTotal.find('.js-order-price'),
                objTotalSum_2 = objTotal.find('.js-order-price-2'), // Сумма с учетом купона
                objTotalSumOld = objTotal.find('.order_oldprice'),
                objCertificateSum = objTotal.find('.js-certificate-sum'),

                totalSum = objTotalSum.data('value'),
                totalSum_2 = (objTotalSum_2.length) ? objTotalSum_2.data('value') : 0,
                certificateSum = (objCertificateSum.length) ? objCertificateSum.data('value') : 0,
                flagDiscount = !objTotalSumOld.hasClass('m-hidden');

            this.fCalc = function () {
                var bonusWithdraw = v22.objBonusWithdraw.bonusWithdraw,
                    totalSumNew = totalSum,
                    totalSumNew_2 = totalSum_2,
                    certificateSumNew = certificateSum,
                    flagWithdraw = Boolean(bonusWithdraw);

                if (flagWithdraw) {
                    totalSumNew -= bonusWithdraw;
                    totalSumNew_2 -= bonusWithdraw;
                };

                if (certificateSum) {
                    certificateSumNew = Math.min(certificateSum, totalSumNew);

                    totalSumNew = Math.max(totalSumNew - certificateSum, 0);
                    totalSumNew_2 = Math.max(totalSumNew_2 - certificateSum, 0);
                }

                // ###

                if (!flagDiscount) {
                    objTotalSumOld.toggleClass('m-hidden', !flagWithdraw);
                }

                if (objCertificateSum.length) {
                    certificateSumNew = v22.fFormatNum(certificateSumNew);
                    objCertificateSum.html(certificateSumNew);
                }

                totalSumNew = v22.fFormatNum(totalSumNew);
                objTotalSum.html(totalSumNew);

                if (objTotalSum_2.length) {
                    totalSumNew_2 = v22.fFormatNum(totalSumNew_2);
                    objTotalSum_2.html(totalSumNew_2);
                }

                stepStoreTotalSum = {
                    sum: totalSumNew,
                    flagBonusWithdraw: flagWithdraw,
                };
            };

            this.fLogData = function () {
                fLog({
                    totalSum: totalSum,
                    totalSum_2: totalSum_2,
                    flagDiscount: flagDiscount,
                }, 'objBlockTotal data');
            };
        },
    }; // obj

    if (0) {
        ``
        objBlockTotal.fInit();
        objBlockTotal.fLogData();
        v22.objBonusWithdraw.bonusWithdraw = 1;
        objBlockTotal.fCalc();
    }
    // # )

    fBindHandlers();
    fBindHandlersSimilar();
    fBindHandlersViewed();

    if (0) {
        objMain.find('.js-step-1 .js-btn-next-step').click();
    } else if (0) {
        fHideStep();
        objMain.find('.js-step-out-of-stock').addClass('active');
    } else if (0) {
        fHideStep();
        objMain.find('.js-step-preview').addClass('active');
    }

}; // function
/* End */
;
; /* Start:"a:4:{s:4:"full";s:69:"/local/templates/v22/assets/js/page_order_shared_2.js?171220728640512";s:6:"source";s:53:"/local/templates/v22/assets/js/page_order_shared_2.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
sp_global.orderShared2 = {};

/** Класс для отображения магазинов на Яндекс Карте */
sp_global.orderShared2.StoresMapController = class {

    yMapsImageList = {
        'all': '/local/templates/v22/assets/images/placemark.svg',
        'some': '/local/templates/v22/assets/images/placemark_some.svg',
        'nothing': '/local/templates/v22/assets/images/placemark_nothing.svg',
    };
    yMapsIconImageSize = [20, 26];
    yMapsIconImageOffset = [-10, -26];

    ymapsMap;
    yMapsBalloonLayoutClass;
    yMapsBalloonContentLayoutClass;

    objSelectedOnMap;

    activatedPlacemark;
    placemarkList = [];


    /** Инициализация Яндекс Карты
     *  @param object params
     */
    fInitMap(params) {
        /*
            fInitMap({
                objMapBlock, (Должен включать в себя data "latitude" и "longitude"),
                objStoreList,
                objSelectedStoreBottomPanel,

                yMapsBalloonLayoutClass,
                yMapsBalloonContentLayoutClass,
    
                fSetStoreActive,
                fSetStoreInactive,
                fBindHandlerBtnShowProductList
            })
        */
        window.sp_global.v22.fLog('fInitMap');
        let thisClass = this;

        this.ymapsMap = new ymaps.Map(
            params.objMapBlock.get(0),
            {
                center: [
                    params.objMapBlock.data('latitude'),
                    params.objMapBlock.data('longitude')
                ],
                zoom: 12,
                controls: [],
            },
            {
                searchControlProvider: 'yandex#search',
                balloonPanelMaxMapArea: 0
            }
        );

        // Создание кастомного макета балуна при выборе магазина
        this.yMapsBalloonLayoutClass = ymaps.templateLayoutFactory.createClass(
            `$[[options.contentLayout]]`,
            {
                /** Действия при инициализации балуна */
                build() {
                    thisClass.yMapsBalloonLayoutClass.superclass.build.call(this);

                    let storeId = this._data.properties._data.storeId,
                        storeObj = params.objStoreList.find(`.item[data-id=${storeId}]`),
                        isStoreActive = storeObj.hasClass("active"),
                        storeIcon = this._data.properties._data.storeIcon;


                    this._$element = $('.item-on-map', this.getParentElement());
                    let storeObjOnMap = this._$element;


                    // Установка значка аптеки
                    this._$element.find(".js-store-icon img").attr("src", storeIcon);


                    // Установка списка с товарами
                    let storeProductsList = storeObj.find(`.js-b-status`);
                    this._$element.find(".js-b-status").replaceWith(storeProductsList.clone());


                    params.fBindHandlerBtnShowProductList(this._$element);


                    // Установка цены
                    let storePrice = storeObj.find(`.js-b-price`);
                    this._$element.find(".js-b-price").html(storePrice.clone())

                    // Установка выбрана ли аптека
                    if (isStoreActive) {
                        this._$element.addClass('active');
                        this._$element.find('.js-select-store').addClass('active');
                    }


                    // Настройка кнопки выбора аптеки
                    this._$element.find(".js-select-store").click(function (event) {
                        isStoreActive = storeObj.hasClass("active");

                        if (!isStoreActive) {
                            params.fSetStoreActive({
                                obj: storeObj,
                                flagScrollMap: false,
                                flagDisableMapModes: true
                            });
                        } else {
                            params.fSetStoreInactive({
                                calledFrom: 'map'
                            });
                        }
                    });


                    // Настройка кнопки закрытия балуна
                    this._$element.find('.close-button')
                        .on('click', $.proxy(this.onCloseClick, this));


                    // Дублирование открытого попапа в нижнюю панель для мобильного вида
                    params.objSelectedStoreBottomPanel.html(storeObjOnMap.clone(true));
                },

                /** Закрытие балуна при клике на кнопку закрытия */
                onCloseClick: function (e) {
                    e.preventDefault();

                    this.events.fire('userclose');
                    params.objSelectedStoreBottomPanel.empty();
                }
            }
        );

        // Создание кастомного макета содержимого балуна при выборе магазина
        this.yMapsBalloonContentLayoutClass = params.yMapsBalloonContentLayoutClass;

        $(window).on("resize", function () {
            params.objMapBlock.children("ymaps").width(0);
        });

        this.fRefreshMap({
            objMapBlock: params.objMapBlock,
            objStoreList: params.objStoreList,
            objSelectedStoreBottomPanel: params.objSelectedStoreBottomPanel
        });
    } // function

    /** Обновление Яндекс Карты, включающее в себя:
     *  - Удаление и воссоздание всех меток
     *  @param object params
     */
    fRefreshMap(params) {
        if (!this.ymapsMap) {
            return;
        }

        /*
            fRefreshMap({
                objMapBlock, (Должен включать в себя data "latitude" и "longitude"),
                objStoreList,
                objSelectedStoreBottomPanel
            })
        */

        window.sp_global.v22.fLog('fRefreshMap');

        this.ymapsMap.geoObjects.removeAll();

        let thisClass = this,
            offset = 0,
            placemarkOptions = {
                hideIconOnBalloonOpen: false,

                iconLayout: 'default#image',
                iconImageHref: this.yMapsImageList['all'],
                iconImageSize: this.yMapsIconImageSize,
                iconImageOffset: this.yMapsIconImageOffset,

                balloonShadow: false,
                balloonLayout: this.yMapsBalloonLayoutClass,
                balloonContentLayout: this.yMapsBalloonContentLayoutClass,
            },

            dataStore = null;

        try {
            dataStore = JSON.parse(params.objStoreList.find('.js-data-json').html())
        } catch (error) { }

        params.objStoreList.find(".item").not(".m-hidden").each(function () {
            let objStore = $(this),
                id = objStore.data('id'),
                type = objStore.data('type') || dataStore[id].type,
                latitude = objStore.data('latitude') || dataStore[id].latitude,
                longitude = objStore.data('longitude') || dataStore[id].longitude;

            let placemark = new ymaps.Placemark(
                [latitude, longitude],
                {
                    storeId: id,
                    storeType: type,
                    storeAddress: objStore.find(".js-store-address").text(),
                    storeName: objStore.find(".js-store-name").text(),
                    storeSchedule: objStore.find(".js-store-schedule").text(),
                    storeIcon: objStore.find(".js-store-icon").attr("src")
                },
                placemarkOptions
            );

            if (type != 'all') {
                placemark.options.set('iconImageHref', thisClass.yMapsImageList[type]);
            }

            offset += objStore.outerHeight(true);

            thisClass.ymapsMap.geoObjects.add(placemark);
            thisClass.placemarkList[id] = placemark;
        });


        this.ymapsMap.setCenter([
            params.objMapBlock.data('latitude'),
            params.objMapBlock.data('longitude')
        ]);
        this.ymapsMap.setZoom(12);
        params.objSelectedStoreBottomPanel.empty();
    } // function

    /** Очистка данных Яндекс Карты */
    fResetMap() {
        if (this.ymapsMap) {
            this.ymapsMap.destroy();
        }

        this.ymapsMap = null;
        this.storesController.objSelectedOnMap = null;
        this.activatedPlacemark = null;
        this.placemarkList = [];
    }

    /** Изменить активную метку на карте */
    fSetPlacemarkActive(placemark) {
        let thisClass = this;

        if (this.activatedPlacemark) {
            this.activatedPlacemark.options.set('iconImageSize', [20, 26]);
            this.activatedPlacemark.options.set('iconImageOffset', [-10, -26]);
        }

        if (!placemark) {
            this.activatedPlacemark = null;
            return;
        }

        this.activatedPlacemark = placemark;
        this.activatedPlacemark.options.set('iconImageSize', [30, 40]);
        this.activatedPlacemark.options.set('iconImageOffset', [-15, -40]);

        setTimeout(function () {
            try {
                thisClass.activatedPlacemark.balloon.open();
            } catch (error) { }
        }, 5)
    } // function
}

/** Класс для фильтрации магазинов */
sp_global.orderShared2.StoresFilterController = class {

    flagSearchProcessing = false;
    searchString;
    searchStringCurrent = '';
    searchStringNew = null;

    /** Переключение режима карты */
    fToggleMapMode(params) {
        let objCheckmark = params.obj.find(".m-checkmark"),
            state = objCheckmark.hasClass("active"),
            nextState = !state;

        params.isFireAction = params.isFireAction == undefined ? true : params.isFireAction;

        // Принудительная установка состояния
        if (params.state !== undefined) {
            // Пропуск, если принудительное состояние равно текущему состоянию
            if (params.state == state) {
                return;
            }

            nextState = params.state;
        }

        objCheckmark.toggleClass("active", nextState);

        if (params.isFireAction) {
            this.fFilterMap();
        }

        return nextState;
    } // function

    /** Фильтрация аптек по фильтрам и введёному адресу */
    fFilterMap() {
        let thisClass = this,
            objStoreSelection = this.storesController.getObjStoreSelection(),
            objStoreList = this.storesController.getObjStoreList(),
            isFilterByNearby = objStoreSelection.find(".js-nearby-store-mode .m-checkmark").hasClass("active"),
            isFilterByTypeAll = objStoreSelection.find(".js-type-all-store-mode .m-checkmark").hasClass("active"),
            isFilterBy24h = objStoreSelection.find(".js-24h-store-mode .m-checkmark").hasClass("active"),

            dataStore = null;
        this.searchString = objStoreSelection.find(".search-wrapper input").val().trim();

        try {
            dataStore = JSON.parse(objStoreSelection.find('.js-data-json').html())
        } catch (error) { }

        // Сброс результатов фильтрации
        objStoreSelection.find(".not-found").addClass("m-hidden");
        objStoreSelection.find(".item").removeClass("m-hidden");


        if (dataStore) {
            objStoreSelection.find(".item").each(function () {
                let objStore = $(this),
                    storeId = objStore.data('id');

                if (isFilterByTypeAll && dataStore[storeId].type != "all") {
                    objStore.addClass("m-hidden");
                }

                if (isFilterBy24h && !dataStore[storeId]['24h']) {
                    objStore.addClass("m-hidden");
                }
            });
        } else {
            if (isFilterByTypeAll) {
                objStoreSelection.find(".item:not([data-type='all'])").addClass("m-hidden");
            }

            if (isFilterBy24h) {
                objStoreSelection.find(".item:not([data-24h=1])").addClass("m-hidden");
            }
        }


        this.fSearchStores(this.searchString).then(function () {
            thisClass.storesController.fRefreshMap();

            let objStoreSelection = thisClass.storesController.getObjStoreSelection(),
                dataStore = null;
            try {
                dataStore = JSON.parse(objStoreSelection.find('.js-data-json').html())
            } catch (error) { }

            thisClass.fTryToGetLocationPermission().then(
                function () {
                    if (isFilterByNearby) {
                        // Включен фильтр "Рядом" - сортировать аптеки поблизости без учёта наличия в них товара
                        thisClass.fSortByNearbyStore(function (params) {
                            let order = 0,
                                sortedByDistance = params.sortedByDistance,
                                ymapsMap = params.ymapsMap;

                            sortedByDistance.each(function (obj) {
                                let coords = obj.geometry.getCoordinates(),
                                    storePlacemark = ymaps.geoQuery(ymapsMap.geoObjects).getClosestTo(coords),
                                    storeId = storePlacemark.properties._data.storeId;

                                let storeObj = objStoreSelection.find(`.item[data-id=${storeId}]`);
                                storeObj.css("order", order++);
                            })

                            thisClass.fApplyCSSOrderToDOM({
                                items: objStoreSelection.find('.item'),
                                container: objStoreList
                            });

                            thisClass.storesController.storesPager.reset();
                        });
                    } else {
                        // По умолчанию, сортировать аптеки поблизости с учётом наличия в них товара
                        thisClass.fSortByNearbyStore(function (params) {
                            let order = 1,
                                sortedByDistance = params.sortedByDistance,
                                ymapsMap = params.ymapsMap,
                                currentStoreId = params.currentStoreId;


                            let sortedByDistanceStores = [];
                            let nearness = 0;
                            sortedByDistance.each(function (obj) {
                                let coords = obj.geometry.getCoordinates(),
                                    storePlacemark = ymaps.geoQuery(ymapsMap.geoObjects).getClosestTo(coords),
                                    storeId = storePlacemark.properties._data.storeId,
                                    objStore = objStoreSelection.find(`.item[data-id=${storeId}]`);

                                objStore.attr("data-nearness", nearness++);

                                sortedByDistanceStores.push(objStore);
                            });

                            // Сортировка магазинов по итоговой сумме заказа
                            sortedByDistanceStores.sort(function (a, b) {
                                if (dataStore) {
                                    let objAStoreId = a.data("id"),
                                        objBStoreId = b.data("id"),

                                        objASum = dataStore[objAStoreId].total.sum - dataStore[objAStoreId].total.discount,
                                        objBSum = dataStore[objBStoreId].total.sum - dataStore[objBStoreId].total.discount;

                                    return objBSum - objASum || $(a).data("nearness") - $(b).data("nearness");
                                } else {
                                    return $(b).data("sum") - $(a).data("sum") || $(a).data("nearness") - $(b).data("nearness");
                                }
                            });

                            $(sortedByDistanceStores).each(function () {
                                let storeId = $(this).data("id");

                                if (currentStoreId == storeId) {
                                    // Показать выбранную в шапке аптеку, первой в списке
                                    $(this).css("order", 0);

                                    // continue (return в JQuery)
                                    return;
                                }

                                $(this).css("order", order++);
                            });

                            thisClass.fApplyCSSOrderToDOM({
                                items: objStoreSelection.find('.item'),
                                container: objStoreList
                            });

                            thisClass.storesController.storesPager.reset();
                        });
                    }

                    thisClass.storesController.isGeolocationCheckedOnStart = true;
                },

                function(reason) {
                    if(!thisClass.storesController.isGeolocationCheckedOnStart || isFilterByNearby) {
                        thisClass.fHandleAndShowGeoLocationError(reason);

                        // Деактивировать активную кнопку для сортировки по ближайшему магазину, чтобы показать пользователю, что этот режим точно не активировался 
                        objStoreSelection.find(".js-nearby-store-mode").trigger({
                            type: "click",
                            state: false,
                            isFireAction: false
                        })
                    }

                    thisClass.storesController.storesPager.reset();
                    thisClass.storesController.isGeolocationCheckedOnStart = true;
                }    
            );


            if (objStoreSelection.find(".item:not(.m-hidden)").length == 0) {
                objStoreSelection.find(".not-found").removeClass('m-hidden');
                thisClass.storesController.storesPager.forceRun();
            }


            // Если есть выбранная аптека и она не скрыта, то выбрать её ещё раз для активации метки на карте
            let objActiveStore = objStoreList.find(".item.active");
            if (objActiveStore.length > 0) {
                if (objActiveStore.hasClass("m-hidden")) {
                    thisClass.storesController.fSetStoreInactive();
                } else {
                    thisClass.storesController.fSetStoreActive({
                        obj: objActiveStore,
                        flagScrollMap: true,
                    })
                }
            }
        })
    } // function

    /** Изменение реального порядка элементов в DOM, на основе CSS order */
    fApplyCSSOrderToDOM(params) {
        params.items.detach().sort(function (a, b) {
            return $(a).css("order") - $(b).css("order");
        });

        params.container.append(params.items);
    }

    async fSearchStores(searchString) {
        let thisClass = this,
            objStoreSelection = this.storesController.getObjStoreSelection();

        return new Promise((resolve) => {
            window.sp_global.v22.fLog(searchString, 'fSearchStores');

            if (thisClass.flagSearchProcessing) {
                window.sp_global.v22.fLog('fSearchStores: flagSearchProcessing');
                thisClass.searchStringNew = thisClass.searchString;
                resolve();
                return;
            }

            if (!thisClass.searchString) {
                window.sp_global.v22.fLog('fSearchStores: searchString is empty');
                resolve();
                return;
            }

            thisClass.flagSearchProcessing = true;

            let data = {
                oper: 'orderSearchStore',
                searchString: thisClass.searchString,
            };

            window.sp_global.v22.fLog(data, 'data');

            window.sp_global.v22.fAjax({
                url: '/local/bitrix_ajax/v22.php',
                data: data,
                dataType: 'json',

                fSuccess: async function (response) {
                    window.sp_global.v22.fLog(response);
                    thisClass.flagSearchProcessing = false;

                    if (thisClass.searchStringNew !== null) {
                        thisClass.searchString = thisClass.searchStringNew;
                        thisClass.searchStringNew = null;

                        await thisClass.fSearchStores(thisClass.searchString);
                        resolve();
                        return;
                    }

                    thisClass.searchStringCurrent = thisClass.searchString;


                    let isAnyStoreFound = false;
                    objStoreSelection.find(".item").each(function () {
                        var objStore = $(this),
                            id = objStore.data('id'),
                            isHidden = objStore.hasClass('m-hidden');

                        if (isHidden) {
                            return;
                        }

                        let isNeedToShowStore = response.items.includes(id);
                        objStore.toggleClass('m-hidden', !isNeedToShowStore);
                        if (isNeedToShowStore) {
                            isAnyStoreFound = true;
                        }
                    });

                    resolve();
                }
            });
        });

    } // function

    /** Попытка получить местоположение пользователя, перед аналогичным запросом к Яндекс Карте. */
    async fTryToGetLocationPermission() {
        return new Promise(function (resolve, reject) {

            if ("permissions" in navigator) {
                navigator.permissions.query({
                    name: "geolocation"

                }).then((result) => {
                    switch (result.state) {
                        /* 
                        Доступ к местоположению, пока не разрешён / заблокирован 
                        P.S - iOS + Safari всегда отвечает как 'prompt', независимо от уровень доступа был уже выбран
                        */
                        case 'prompt':

                        /*
                        Доступ к местоположению уже разрешён 
                        В таком случае, можно было бы сразу возвращать ответ - можно получить местоположение
                        Но, в случае, если страница открывается в новой вкладке (при этом она неактивна), доступ может быть уже и разрешён, 
                        но попытка получить местоположение закончится бесконечным ожиданием, когда же пользователь включит эту вкладку
                        */
                        case 'granted':
                        case 'denied':
                            // Вызов окна с просьбой разрешить доступ к местоположению
                            navigator.geolocation.getCurrentPosition(
                                // Доступ разрешён
                                // Задержка из-за, того что на устройствах Apple дальнейший запрос к Яндекс Картам зависает с ошибкой "timeout"
                                () => setTimeout(() => resolve(), 100),
                                // Проблема с получением доступа (Доступ запрещён / Нет интернета)
                                function (error) {
                                    switch (error.code) {
                                        case 1:
                                            reject(error);
                                            break;

                                        case 2:
                                            reject(error);
                                            break;

                                        default:
                                            reject(error);
                                            console.log(`fTryToGetLocationPermission unknown error - ${error.code}: ${error.message}`);
                                            break;
                                    }
                                },
                                {
                                    maximumAge: 0
                                });
                            break;
                    }
                });
            }
        });
    }

    fHandleAndShowGeoLocationError(error) {
        let errorMessages = {
            geolocationDenied: "Ошибка: Нет доступа к местоположению.\n" +
                "Пожалуйста, разрешите доступ к местоположению в настройках вашего браузера или устройства.",

            noInternet: "Ошибка: Нет доступа к интернету.\n" +
                "Пожалуйста, проверьте своё подключение к интернету и повторите попытку позже.",

            timeout: "Ошибка: Время ожидания вышло:\n" +
                "1. Проверьте свой доступ к интернету.\n" +
                "2. Повторите попытку позже.",

            unknown: "Неизвестная ошибка:\n" +
                "Повторите попытку позже."
        };

        // Является ли ошибка типом GeolocationPositionError (Выбрасывается браузером)
        let isGeolocationPositionError = error instanceof GeolocationPositionError;

        // Является ли ошибка ответом от Яндекса
        let isYandexError = !isGeolocationPositionError && (typeof (error) === "object");

        let errorMessage = "";


        // Ошибка связанная с тем, что доступ к геолокации запрещён / 
        if (isGeolocationPositionError && error.code == 1) {
            errorMessage = errorMessages.geolocationDenied;
        }
        // Ошибка связанная с тем, что GeoLocation не может получить местоположение из-за отсуствия интернета
        else if (isGeolocationPositionError && error.code == 2) {
            errorMessage = errorMessages.noInternet;
        }

        // Ошибка связанная с тем, что Яндекс Карта не получила ответа
        if (isYandexError && error.name === "TimedOut") {
            errorMessage = errorMessages.timeout;
        }

        // Неизвестная ошибка
        if (errorMessage === "") {
            errorMessage = errorMessages.unknown;

            console.log(error);
        }

        alert(errorMessage);
    }

    /** Сортировка списка магазинов по расстоянию */
    async fSortByNearbyStore(fHandler) {
        if (!this.storesController.ymapsMap()) {
            this.storesController.fInitMapFromComponent();
        }

        let objStoreSelection = this.storesController.getObjStoreSelection(),
            ymapsMap = this.storesController.ymapsMap(),
            geolocation = ymaps.geolocation,
            customCoords = null;
        window.sp_global.v22.fSetProcessing(true);


        try {
            customCoords = JSON.parse($('.js-data-selected-coordinates-on-map').html())
        } catch (error) { }


        this.fTryToGetLocationPermission().then(
            async function () {
                let triesData = {
                    currentTries: 1,
                    allowedTries: 3,
                    baseDelayInMs: 2000,
                    isSuccess: false
                };

                do {
                    console.log(`Trying to get geolocation by ymaps: ${triesData.currentTries} / ${triesData.allowedTries}`);

                    await geolocation.get({
                        provider: 'browser', // Определение позиции текущего пользователя по браузеру
                        mapStateAutoApply: true,
                        timeout: 15000, // Таймаут, мсек

                        /*
                        Отключить обратное геокодирование. 
                        Преимущества этого функционала не используется, но при этом такие запросы:
                            - Тарифицируются
                            - Выполняются медленее
                        */
                        autoReverseGeocode: false

                    }).then(
                        function (result) {
                            let myCoords = customCoords ? [customCoords.latitude, customCoords.longitude] : result.geoObjects.position,
                                sortedByDistance = ymaps.geoQuery(ymapsMap.geoObjects).sortByDistance(myCoords),
                                currentStoreId = $(".js-data-current-store-id").text().trim();

                            try {
                                fHandler({
                                    sortedByDistance: sortedByDistance,
                                    ymapsMap: ymapsMap,
                                    currentStoreId: currentStoreId
                                });
                            } catch (error) {
                                console.error(error);
                            }

                            triesData.isSuccess = true;
                        },

                        // Операция по сортировке по ближайшему магазину провалилась полностью
                        function (error) {
                            // Попытки закончились, нужно вывести сообщение об ошибке
                            if (triesData.currentTries >= triesData.allowedTries) {
                                thisClass.fHandleAndShowGeoLocationError(error);
                            }

                            // Деактивировать активную кнопку для сортировки по ближайшему магазину, чтобы показать пользователю, что этот режим точно не активировался 
                            objStoreSelection.find(".js-nearby-store-mode").trigger({
                                type: "click",
                                state: false,
                                isFireAction: false
                            })
                        }
                    );

                    if (triesData.isSuccess) {
                        break;
                    }

                    if (triesData.currentTries + 1 <= triesData.allowedTries) {
                        let delayToWait = triesData.baseDelayInMs * (triesData.currentTries + 1);

                        console.log(`Cannot get geolocation, waiting ${delayToWait} ms`);
                        await new Promise(resolve => setTimeout(resolve, delayToWait));
                    }

                    triesData.currentTries++;
                } while (triesData.currentTries <= triesData.allowedTries);

                // Скрыть анимацию загрузки
                window.sp_global.v22.fSetProcessing(false);
            },

            async function (reason) {
                thisClass.fHandleAndShowGeoLocationError(reason);
                window.sp_global.v22.fSetProcessing(false);
            });
    } // function
}

/** Класс для реализации списка магазинов и связанной Яндекс Карты */
sp_global.orderShared2.storesController = class {

    storesMapController;
    storesFilterController;
    storesPager;

    constructor() {
        this.storesMapController = new sp_global.orderShared2.StoresMapController();
        this.storesFilterController = new sp_global.orderShared2.StoresFilterController();
        this.storesPager = new sp_global.orderShared2.StoresPager();

        this.storesMapController.storesController = this;
        this.storesFilterController.storesController = this;
        this.storesPager.storesController = this;
    }

    /* HTML элементы */
    objMapBlock; // Должен включать в себя data "latitude" и "longitude")
    objStoreList;
    objSelectedStoreBottomPanel;

    /* Функции */
    fSetStoreActive;
    fSetStoreInactive;
    fBindHandlerBtnShowProductList;

    fInitMapFromComponent;

    isGeolocationCheckedOnStart = false;

    /** Получение экземпляра Яндекс Карты */
    ymapsMap = function () {
        return this.storesMapController.ymapsMap;
    }

    placemarkList = function () {
        return this.storesMapController.placemarkList;
    }

    getObjStoreList = function () {
        return this.objStoreList;
    }

    getObjStoreSelection = function () {
        return this.objStoreSelection;
    }

    getActivatedPlacemark = function () {
        return this.storesMapController.activatedPlacemark;
    }

    /** Инициализация Яндекс Карты */
    fInitMap(params) {
        this.storesMapController.fInitMap({
            objMapBlock: this.objMapBlock,
            objStoreList: this.objStoreList,
            objSelectedStoreBottomPanel: this.objSelectedStoreBottomPanel,

            yMapsBalloonContentLayoutClass: params.yMapsBalloonContentLayoutClass,

            fSetStoreActive: this.fSetStoreActive,
            fSetStoreInactive: this.fSetStoreInactive,
            fBindHandlerBtnShowProductList: this.fBindHandlerBtnShowProductList
        });
    }

    /** Обновление Яндекс Карты, включающее в себя:
     *  - Удаление и воссоздание всех меток
     */
    fRefreshMap() {
        this.storesMapController.fRefreshMap({
            objMapBlock: this.objMapBlock,
            objStoreList: this.objStoreList,
            objSelectedStoreBottomPanel: this.objSelectedStoreBottomPanel
        });
    }

    fToggleMapMode(params) {
        this.storesFilterController.fToggleMapMode(params);
    }

    /** Изменить активную метку на карте */
    fSetPlacemarkActive(placemark) {
        this.storesMapController.fSetPlacemarkActive(placemark);
    }
}

/** Класс для реализации постепенного отображения аптек при нажатии на кнопку "Показать ещё". При отображении всех аптек - скрытие через "Свернуть" */
sp_global.orderShared2.StoresPager = class {

    element = null;
    options = {
        visibleCount: 5, // Изначальное отображаемое аптек элементов в списке
        loadCount: 10 // Количество отображаемых аптек при нажатии на кнопку "Показать ещё"
    };

    init(objPharmaciesPager) {
        let thisClass = this,
            objStoreList = this.storesController.getObjStoreList();

        this.element = objPharmaciesPager;

        objStoreList.attr("data-visible-count", this.options.visibleCount);
        objStoreList.attr("data-pager-mode", "init");

        this.element.click(function () {
            let currentVisibleCount = parseInt(objStoreList.attr("data-visible-count"));

            if (objStoreList.attr("data-pager-mode") == "hide" || objStoreList.attr("data-pager-mode") == "init") {
                // Действия при нажатии на кнопку в режиме скрытия или инициализации
                currentVisibleCount = thisClass.options.visibleCount;

                $(this).show();
                objStoreList.find('.item').removeClass('flag-show');
                objStoreList.attr("data-pager-mode", "load");
                $(this).text(BX.message("ORDER_2_SHOW_MORE_PHARMACIES"));
            } else {
                // Действия при нажатии на кнопку в режиме загрузки
                currentVisibleCount += thisClass.options.loadCount;
            }


            // Отображение первых N аптек в зависимости от атрибута data-visible-count
            objStoreList.attr("data-visible-count", currentVisibleCount);
            objStoreList.find(`.item:not(.m-hidden):lt(${currentVisibleCount})`).addClass('flag-show');


            // Отображены все доступные элементы, изменить у кнопки режим работы - "hide", и текст "Свернуть" 
            if (currentVisibleCount >= objStoreList.find(".item:not(.m-hidden)").length) {
                objStoreList.attr("data-pager-mode", "hide");
                $(this).text(BX.message("ORDER_2_HIDE_PHARMACIES"));

                // Скрыть кнопку если все доступные аптеки помещаются в список без необходимости их скрывать или показывать 
                if (currentVisibleCount >= thisClass.options.visibleCount) {
                    $(this).hide();
                }
            }
        });
    }

    reset() {
        if (this.element == null) {
            console.error("storesPager is uninitialized!");
            return;
        }

        let thisClass = this,
            objStoreList = this.storesController.getObjStoreList();

        objStoreList.attr("data-pager-mode", "init");
        this.element.click();
    }

    forceRun() {
        this.element.click();
    }
}
