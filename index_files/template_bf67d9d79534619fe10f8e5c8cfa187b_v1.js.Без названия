                                    Object.prototype.toString.call(
                                        initialValue
                                    ) &&
                                    (initialValue = importDate(
                                        initialValue,
                                        opts
                                    )),
                                initialValue
                            );
                        },
                        insertMode: !1,
                        shiftPositions: !1,
                        keepStatic: !1,
                        inputmode: "numeric",
                    },
                });
