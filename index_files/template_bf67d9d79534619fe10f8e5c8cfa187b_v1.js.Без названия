; /* Start:"a:4:{s:4:"full";s:52:"/local/templates/.default/js/sp_lib.js?1595309857752";s:6:"source";s:38:"/local/templates/.default/js/sp_lib.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
if (sp_global === undefined) {
    var sp_global = {};
} //

sp_global.flagProcessing = false;

sp_global.fLog = function (msg, label) {
    if (label !== undefined) {
        var type = typeof(msg);
        if (type === 'string' || type === 'number' || type === 'boolean' || msg === null) {
            msg = label + ': ' + msg;
        } else {
            console.log(label +' ('+ type + ') vvvvv');
        }
    }
    console.log(msg);
} //

sp_global.fShowMessage = function (msg) {
    alert(msg);
} // function

sp_global.fSetProcessing = function (flag) {
    if (flag && sp_global.flagProcessing) {
        return false;
    }

    sp_global.flagProcessing = flag;
    $('.layer-processing').toggle(flag);
    
    return true;
} // function

/* End */
;
; /* Start:"a:4:{s:4:"full";s:55:"/local/templates/v22/assets/js/main.js?1712142182109098";s:6:"source";s:38:"/local/templates/v22/assets/js/main.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
window.sp_global.v22 = {

    fShowMessage: window.sp_global.fShowMessage,

    fLog: function(msg, label) {
        window.sp_global.fLog(msg, label);
    },

    fSetProcessing: function(flag) {
        var result = window.sp_global.fSetProcessing(flag);
        $('.header .main-icon-processing').toggle(window.sp_global.flagProcessing);

        this.fLog('fSetProcessing ' + ((flag) ? 1 : 0) + ((!result) ? ' error' : ''));

        return result;
    },

    fIsProcessing: function() {
        return window.sp_global.flagProcessing;
    },

    // ###

    urlAjax:  '/local/bitrix_ajax/v22.php',
    dataMain: {},
    dataMisc: {},
    
    // ###

    fAjax: function(params) {
        /*  window.sp_global.v22.fAjax({
                url:          '/local/bitrix_ajax/mobile2.php',
                data:         data,
                dataType:     json,     // json | html | ...
                showErrorMsg: standard, // Только для dataType=json # standard ("Ошибка") | error_msg (значение response.error_msg) | false (не показывать)
                checkContent: false,    // Только для dataType=html

                fSuccess: function(response) {
                }
            })
        */
        var that = this;

        if (params.dataType === undefined) {
            params.dataType = 'json';
        }
        if (params.showErrorMsg === undefined) {
            params.showErrorMsg = 'standard';
        }
        if (params.checkContent === undefined) {
            params.checkContent = false;
        }

        $.ajax({
            url:      params.url,
            type:     'POST',
            dataType: params.dataType,
            data:     params.data,

            success: function (response) {

                if (!response) {
                    that.fSetProcessing(false);
                    that.fShowMessage('Ошибка 100');
                } else {
                    //that.fLog(response);
                    if (params.dataType != 'json') {
                        if (params.checkContent) {
                            var objTMP = $('<div>' + response + '</div>');
                            if (!objTMP.find('.js-flag-right-content').length) {
                                that.fSetProcessing(false);
                                that.fShowMessage('Ошибка 101');
                                return;
                            }
                        }
                        params.fSuccess(response);
                    } else {
                        if (!response.error_msg || !params.showErrorMsg) {
                            params.fSuccess(response);
                        } else {
                            that.fSetProcessing(false);
                            that.fLog(response);
                            if (params.showErrorMsg == 'standard') {
                                that.fShowMessage('Ошибка');
                            } else {
                                // (params.showErrorMsg == 'error_msg')
                                that.fShowMessage(response.error_msg);
                            }
                        }
                    }
                } //
            },
            error: function () {
                that.fSetProcessing(false);
                that.fShowMessage('Нет соединения с сервером (код ошибки: 11)'); // # ajax error #
            }
        });
    }, // function

    fValidatePhone: function(value) {
        // regexp cкопировано из jquery.validationEngine-ru.js
        return (value.search(/^(8|\+7)?(\s?\-?)(\(?[9]\d{2}\)?)(\s?\-?)(\d{3})(\s?\-?)(\d{2})(\s?\-?)(\d{2})\s*$/) != -1);
    }, // function

    fValidateEmail: function(value) {
        return (value.search(/.+@.+/) != -1);
    }, // function

    fValidateCardNumber: function(value) {
        value = value.replace(/\s/g, '');
        return (value.search(/^\d{13}$/) != -1);
    }, // function

    fFormatNum: function(n) {
        // https://blog.abelotech.com/posts/number-currency-formatting-javascript/

        n = Number(n);
        return n.toFixed(2).toString().replace(/(\d)(?=(\d{3})+(?!\d))/g, '$1 ').replace('.', ',');
    }, // function

    fShowGlobalMsg: function (obj, msg) {
        if (msg != undefined) {
            obj.html(msg);
        }

        obj.fadeIn(500, function(){
            setTimeout(function(){
                obj.fadeOut(500);
            }, 1000);
        });
    }, // function

}; // object

window.sp_global.v22.fAuthShowForm = function(params) {
    // v22.fAuthShowForm({form: 'authorization'})

    var fShowMessage = window.sp_global.fShowMessage;

    function fLog(msg, label) {
        window.sp_global.fLog(msg, label);
    }

    var form = params.form;

    var v22 = window.sp_global.v22,
        urlSendForm = '/local/bitrix_ajax/v22.php',

        objMain,
        selector,
        fAfterGetForm;
    
    switch (form) {
        case 'authorization':
            selector      = '.modal-authorization';
            fAfterGetForm = fAuthorization;
            break;

        case 'registration':
            selector      = '.modal-registration';
            fAfterGetForm = fRegistration;
            break;

        case 'password_recovery':
            selector      = '.modal-password-recovery';
            fAfterGetForm = fPasswordRecovery;
            break;
                
        default:
            return;
    } //

    objMain = $(selector);

    $.fancybox.close();

    if (objMain.length) {
        $.fancybox.open(objMain);

    } else {
        if (!v22.fSetProcessing(true)) {
            return;
        }
        fLog('getForm');

        v22.fAjax({
            url:          v22.dataMain.urlPrefix + '/misc/auth_modal.php',
            data:         {form: form},
            dataType:     'html',
            checkContent: true,
            fSuccess: function(response) {
                $('body').append(response);
                objMain = $(selector);
                fAfterGetForm();
                v22.fSetProcessing(false);
                $.fancybox.open(objMain);
            }
        });
    } //

    function fAuthorization() {
        var objErrorMsg = objMain.find('.error-msg'),
            arObjField  = {};

        objMain.find('.js-field').each(function(){
            arObjField[ $(this).attr('name') ] = $(this);

            $(this).focusin(function(){
                $(this).removeClass('error');
            });
        });

        arObjField.PHONE.inputmask({
            mask:                 '+7 (999) 999 99 99',
            //clearMaskOnLostFocus: false,
        })

        objMain.find('form').submit(function(event){
            event.preventDefault();

            if (v22.fIsProcessing() || !fCheckForm() || !v22.fSetProcessing(true)) {
                return;
            }

            var data = {
                    oper_first: 'sarn',
                    oper:       'authorize',
                    sessid:     objMain.find('#sessid').val(),
                };

            for (var fieldName in arObjField) {
                data[ fieldName ] = arObjField[ fieldName ].val();
            }
            
            fLog(data, 'data');
            
            if (0) {
                v22.fSetProcessing(false);
                return;
            }
    
            v22.fAjax({
                url:          urlSendForm,
                data:         data,
                showErrorMsg: false,
                fSuccess: function(response) {
                    fLog(response);

                    if (response.error_msg) {
                        var flag = false;

                        if (response.field_invalid) {
                            for (var fieldName in arObjField) {
                                if (response.field_invalid[ fieldName ]) {
                                    arObjField[ fieldName ].addClass('error');
                                    flag = true;
                                }
                            }
                        } //

                        if (response.showError || !flag) {
                            objErrorMsg.html(response.error_msg).show();
                        }

                        v22.fSetProcessing(false);

                    } else {
                        ym(34347420, 'reachGoal', 'authorize');
                        
                        window.location.reload();
                    } //
                } //
            });
        }); // function

        function fCheckForm() {
            //return true;

            var result = true,
                value;
    
            objErrorMsg.html('').hide();
            objMain.find('.error').removeClass('error');

            value = arObjField.PHONE.val().trim();
            if (!v22.fValidatePhone(value)) {
                arObjField.PHONE.addClass('error');
                result = false;
            }
            
            value = arObjField.PW.val().trim();
            if (!value.length) {
                arObjField.PW.addClass('error');
                result = false;
            }

            return result;
        } // function

        fCommonBtnHandlers();
    } // fAuthorization

    function fRegistration() {
        var objErrorMsg = objMain.find('.error-msg'),
            arObjField  = {};

        objMain.find('.js-field').each(function(){
            arObjField[ $(this).attr('name') ] = $(this);

            $(this).focusin(function(){
                $(this).removeClass('error');
            });
        });

        arObjField.PHONE.inputmask({
            mask:                 '+7 (999) 999 99 99',
            clearMaskOnLostFocus: false,
        });

        objMain.find('.js-btn-send').click(function(){
            if (v22.fIsProcessing() || !fCheckForm() || !v22.fSetProcessing(true)) {
                return;
            }

            var data = {
                    oper_first: 'sarn',
                    oper:       'register',
                    sessid:     objMain.find('#sessid').val(),
                    'smart-token': $('#magicItem').find('input[name=smart-token]').val(),
                };

            for (var fieldName in arObjField) {
                data[ fieldName ] = arObjField[ fieldName ].val();
            }
            
            fLog(data, 'data');
            
            if (0) {
                v22.fSetProcessing(false);
                return;
            }
                
            v22.fAjax({
                url:          urlSendForm,
                data:         data,
                showErrorMsg: false,
                fSuccess: function(response) {
                    fLog(response);

                    if (response.error_msg) {
                        var flag = false;

                        if (response.field_invalid) {
                            for (var fieldName in arObjField) {
                                if (response.field_invalid[ fieldName ]) {
                                    arObjField[ fieldName ].addClass('error');
                                    flag = true;
                                }
                            }
                        } //

                        if (response.showError || !flag) {
                            objErrorMsg.html(response.error_msg).show();
                        }

                    } else {
                        ym(34347420, 'reachGoal', 'register')

                        objMain.find('.js-step-1').hide();
                        objMain.find('.js-step-2').show().removeClass('m-hidden');
                    } //

                    v22.fSetProcessing(false);
                } //
            });
        }); // function

        function fCheckForm() {
            //return true;

            var result = true,
                value;
    
            objErrorMsg.html('').hide();
            objMain.find('.error').removeClass('error');

            value = arObjField.PHONE.val().trim();
            if (!v22.fValidatePhone(value)) {
                arObjField.PHONE.addClass('error');
                result = false;
            }

            return result;
        } // function

        fCommonBtnHandlers();
    } // fRegistration

    function fPasswordRecovery() {
        var objErrorMsg = objMain.find('.error-msg'),
            arObjField  = {};

        objMain.find('.js-field').each(function(){
            arObjField[ $(this).attr('name') ] = $(this);

            $(this).focusin(function(){
                $(this).removeClass('error');
            });
        });

        arObjField.PHONE.inputmask({
            mask:                 '+7 (999) 999 99 99',
            //clearMaskOnLostFocus: false,
        })

        objMain.find('.js-btn-send').click(function(){
            if (v22.fIsProcessing() || !fCheckForm(1) || !v22.fSetProcessing(true)) {
                return;
            }

            var data = {
                    oper_first: 'sarn',
                    oper:       'passwordRecoverySendCode',
                    sessid:     objMain.find('#sessid').val(),
                    PHONE:      arObjField.PHONE.val(),
                };

            fLog(data, 'data');
            
            if (0) {
                v22.fSetProcessing(false);
                return;
            }
    
            v22.fAjax({
                url:          urlSendForm,
                data:         data,
                showErrorMsg: false,
                fSuccess: function(response) {
                    fLog(response);

                    if (response.error_msg) {
                        var flag = false;

                        if (response.field_invalid) {
                            if (response.field_invalid.PHONE) {
                                arObjField.PHONE.addClass('error');
                                flag = true;
                            }
                        } //

                        if (response.showError || !flag) {
                            objErrorMsg.html(response.error_msg).show();
                        }

                    } else {
                        ym(34347420, 'reachGoal', 'authorize');

                        objMain.find('.js-step-1').hide();
                        objMain.find('.js-step-2').show().removeClass('m-hidden');
                    } //

                    v22.fSetProcessing(false);
                } //
            });
        }); // function

        objMain.find('.js-btn-send-2').click(function(){
            if (v22.fIsProcessing() || !fCheckForm(2) || !v22.fSetProcessing(true)) {
                return;
            }

            var data = {
                    oper_first: 'sarn',
                    oper:       'passwordRecoveryChangePassword',
                    sessid:     objMain.find('#sessid').val(),
                };

            for (var fieldName in arObjField) {
                data[ fieldName ] = arObjField[ fieldName ].val();
            }
    
            fLog(data, 'data');
            
            if (0) {
                v22.fSetProcessing(false);
                return;
            }
    
            v22.fAjax({
                url:          urlSendForm,
                data:         data,
                showErrorMsg: false,
                fSuccess: function(response) {
                    fLog(response);

                    if (response.error_msg) {
                        var flag = false;

                        if (response.field_invalid) {
                            var ar = ['CODE', 'PW_NEW', 'PW_NEW_2'],
                                i,
                                fieldName;
            
                            for (i=0; i<3; i++) {
                                fieldName = ar[i];
                                if (response.field_invalid[ fieldName ]) {
                                    arObjField[ fieldName ].addClass('error');
                                    flag = true;
                                }
                            }
                        } //

                        if (response.showError || !flag) {
                            objErrorMsg.html(response.error_msg).show();
                        }

                        v22.fSetProcessing(false);

                    } else {
                        window.location.reload();
                    } //
                } //
            });
        }); // function

        function fCheckForm(step) {
            //return true;

            var result = true,
                value,
                value_2;

            objErrorMsg.html('').hide();
            objMain.find('.error').removeClass('error');

            if (step == 1) {

                value = arObjField.PHONE.val();
                if (!v22.fValidatePhone(value)) {
                    arObjField.PHONE.addClass('error');
                    result = false;
                }

            } else {
                // step 2
               
                var ar = ['CODE', 'PW_NEW', 'PW_NEW_2'],
                    i,
                    fieldName;

                for (i=0; i<3; i++) {
                    fieldName = ar[i];

                    value = arObjField[ fieldName ].val();
                    if (!value.length) {
                        arObjField[ fieldName ].addClass('error');
                        result = false;
                    }
                } //

                if (result) {
                    value   = arObjField.PW_NEW.val();
                    value_2 = arObjField.PW_NEW_2.val();

                    if (value != value_2) {
                        arObjField.PW_NEW.addClass('error');
                        arObjField.PW_NEW_2.addClass('error');
                        result = false;
                        objErrorMsg.html( objErrorMsg.data('text-pw_not_equal') ).show();
                    }
                }
            } //

            return result;
        } // function

        fCommonBtnHandlers();
    } // fPasswordRecovery

    function fCommonBtnHandlers() {
        objMain.find('.js-btn-authorization').click(function(){
            v22.fAuthShowForm({form: 'authorization'});
        });
        objMain.find('.js-btn-registration').click(function(){
            v22.fAuthShowForm({form: 'registration'});
        });
        objMain.find('.js-btn-password-recovery').click(function(){
            v22.fAuthShowForm({form: 'password_recovery'});
        });
    } //

} // function

window.sp_global.v22.objSelectCity = {

    fLog: function(msg, label) {
        window.sp_global.fLog(msg, label);
    }, // function

    fInit: function() {
        var that = this;
        
        this.objMain = $('.js-block-city-selection');

        $('.js-btn-city-selection').click(function(){
            if (that.objMain.html()) {
                that.fShow();
            } else {
                that.fGetContent();
            }
        });
    }, // function

    fShow: function() {
        this.fLog('fShow');
        $.fancybox.open(this.objMain, {
            touch: false,
            autoFocus: false,
            //width: 'auto',
            //height: '100%',
        });
    }, // function

    fGetContent: function() {

        this.fLog('fGetContent');

        var v22 = window.sp_global.v22,
            that = this;

        if (!v22.fSetProcessing(true)) {
            return;
        }

        v22.fAjax({
            url:          v22.dataMain.urlPrefix + '/misc/city_selection.php',
            data:         {},
            dataType:     'html',
            checkContent: true,
            fSuccess: function(response) {
                v22.fSetProcessing(false);
                that.objMain.html(response);
                that.objMain.find('.js-scroll-container').scrollbar();
                that.fBindHandlers();
                that.fShow();
            }
        });

    }, // function

    fBindHandlers: function() {
        var v22     = window.sp_global.v22,
            that    = this,
            objMain = this.objMain;

        objMain.find('.js-nav-item').click(function(){

            objMain.find('.js-nav-item').removeClass('active');
            $(this).addClass('active');

            objMain.find('.js-tab').removeClass('active');
            objMain.find('.js-tab[data-id='+ $(this).data('id') +']').addClass('active');
        });

        objMain.find('.js-city').click(function(){
            var obj = $(this);

            if (obj.hasClass('active')) {
                return;
            }

            var data = {
                oper:   'setCity',
                cityId: obj.data('id'),
            };

            that.fLog(data, 'data');

            if (!v22.fSetProcessing(true)) {
                return;
            }
    
            v22.fAjax({
                url:      '/local/bitrix_ajax/v22.php',
                data:     data,
                dataType: 'json',
                fSuccess: function(response) {
                    //that.fLog(response);
                    //v22.fSetProcessing(false);

                    objMain.find('.js-city').removeClass('active');
                    obj.addClass('active');

                    window.location.reload();
                }
            });
        });

    }, // function

} // object

window.sp_global.v22.fInitSelectCityMobile = function() {

    function fLog(msg, label) {
        window.sp_global.fLog(msg, label);
    }

    var v22 = window.sp_global.v22,
        objMain = $('#mmenu .js-city-list');

    objMain.find('.js-city').click(function(){
        var obj = $(this);

        if (obj.hasClass('main-menu__city--selected')) {
            return;
        }

        var data = {
            oper:   'setCity',
            cityId: obj.data('id'),
        };

        fLog(data, 'data');

        if (!v22.fSetProcessing(true)) {
            return;
        }

        v22.fAjax({
            url:      '/local/bitrix_ajax/v22.php',
            data:     data,
            dataType: 'json',
            fSuccess: function(response) {
                //that.fLog(response);
                //v22.fSetProcessing(false);

                objMain.find('.js-city').removeClass('main-menu__city--selected');
                obj.addClass('main-menu__city--selected');

                window.location.reload();
            }
        });
    });

} // function

window.sp_global.v22.balloonTemplate = {
    
    balloonTemplate: null,

    fGet: function() {
        var that = this;

        if (!this.balloonTemplate) {
            /*  2022-02-18 копия https://yandex.ru/dev/maps/jsbox/2.1/balloon_autopan
                Удалены комментарии
                В MyBalloonLayout заменены
                    .popover -> .map-balloon
                    .close -> .m-close
                В MyBalloonLayout и MyBalloonContentLayout изменен html

                Есть обращения к "MyBalloonLayout.superclass". MyBalloonLayout заменен на balloonTemplate.balloonLayout

                https://yandex.ru/dev/maps/jsapi/doc/2.1/ref/reference/layout.templateBased.Base.html
                https://yandex.ru/dev/maps/jsapi/doc/2.1/ref/reference/Balloon.html
            */
            this.balloonTemplate = {
                balloonLayout: ymaps.templateLayoutFactory.createClass(
                    '<div class="map-balloon">' +
                    '<div class="m-close">&times;</div>' +
                    '<div class="arrow"></div>' +
                    '<div class="inner">' +
                    '$[[options.contentLayout observeSize minWidth=280 maxWidth=280 maxHeight=200]]' +
                    '</div>' +
                    '</div>',
                    {
                        build: function () {
                            this.constructor.superclass.build.call(this);
                            this._$element = $('.map-balloon', this.getParentElement()); // .popover -> .map-balloon
                            this.applyElementOffset();
                            this._$element.find('.m-close')
                                .on('click', $.proxy(this.onCloseClick, this)); // .close -> .m-close
                        },
                        clear: function () {
                            this._$element.find('.m-close')
                                .off('click'); // .close -> .m-close
                            this.constructor.superclass.clear.call(this);
                        },
                        onSublayoutSizeChange: function () {
                            that.balloonTemplate.balloonLayout.superclass.onSublayoutSizeChange.apply(this, arguments); // MyBalloonLayout -> that.balloonTemplate.balloonLayout
                            if(!this._isElement(this._$element)) {
                                return;
                            }
                            this.applyElementOffset();
                            this.events.fire('shapechange');
                        },
                        applyElementOffset: function () {
                            this._$element.css({
                                left: -(this._$element[0].offsetWidth / 2),
                                top: -(this._$element[0].offsetHeight + this._$element.find('.arrow')[0].offsetHeight)
                            });
                        },
                        onCloseClick: function (e) {
                            e.preventDefault();
                            this.events.fire('userclose');
                        },
                        getShape: function () {
                            if(!this._isElement(this._$element)) {
                                return that.balloonTemplate.balloonLayout.superclass.getShape.call(this); // MyBalloonLayout -> that.balloonTemplate.balloonLayout
                            }
                            var position = this._$element.position();
                            return new ymaps.shape.Rectangle(new ymaps.geometry.pixel.Rectangle([
                                [position.left, position.top], [
                                    position.left + this._$element[0].offsetWidth,
                                    position.top + this._$element[0].offsetHeight + this._$element.find('.arrow')[0].offsetHeight
                                ]
                            ]));
                        },
                        _isElement: function (element) {
                            return element && element[0] && element.find('.arrow')[0];
                        }
                    }
                ),
                balloonContentLayout: ymaps.templateLayoutFactory.createClass(
                    '<div class="baloon-content">$[properties.balloonContent]</div>'
                ),
                balloonPanelContentLayout: ymaps.templateLayoutFactory.createClass(
                    '<div class="map-balloon-panel-content">$[properties.balloonContent]</div>'
                ),
            };
        } //
        
        return this.balloonTemplate;
    } // function

} // object

window.sp_global.v22.fInitSelectStore = function(params) {
    /*  fInitSelectStore({
            source:  'headerModal', // headerModal | menuMobile | pageStores | pageCatalogDetail | orderDelivery
            objMain: $('.js-block-store-selection--header-modal'),
            
            onSelectStore: function(){},
        })
    */
    var fShowMessage = window.sp_global.fShowMessage;

    function fLog(msg, label) {
        window.sp_global.fLog(msg, label);
    }

    function fShowModal() {
        fLog('fShowModal');
        $.fancybox.open(objMain, {
            touch: false,
            autoFocus: false,
        });
    } // function

    function fGetContent() {

        fLog('fGetContent');

        if (!v22.fSetProcessing(true)) {
            return;
        }

        var data = {};

        if (source == 'headerModal') {
            data.flagModalDesktop = 1;
        } else if (source == 'menuMobile') {
            data.flagMobile = 1;
        }

        v22.fAjax({
            url:          urlAjax,
            data:         data,
            dataType:     'html',
            checkContent: true,
            fSuccess: function(response) {
                v22.fSetProcessing(false);
                objMain.html(response);
                fBindHandlers();

                if (source == 'headerModal') {
                    fShowModal();

                } else if (source == 'menuMobile') {
                    var h = window.innerHeight - 40 - 50;
                    h -= Math.ceil( $('.modal-store .block-top').outerHeight() );
                    $('.modal-store .block-fixed-height').css('height', h + 'px').css('max-height', 'unset');
                } //

                objMain.find('.js-scroll-container-tmp').addClass('js-scroll-container').scrollbar();

                ymaps.ready(function(){
                    fMapInit();
                    fShowSelected();
                });
            }
        });

    } // function

    function fBindHandlers() {

        objMain.find('.js-nav-item').click(function(){
            var tabId = $(this).data('id');

            objMain.find('.js-nav-item').removeClass('active');
            $(this).addClass('active');

            objMain.find('.js-tab').removeClass('active');
            objMain.find('.js-tab.js-tab-id-'+ tabId).addClass('active');

            if (tabId == 1 && scrollStoreList) {
                fScrollStoreList(scrollStoreList);
                scrollStoreList = 0;
            }
        });

        function fSetStoreActive(objStore) {
            var objStoreActive = objMain.find('.js-store.active');
            if (objStoreActive.length) {
                fSetStoreActive_hlp(objStoreActive, false);
            }
            fSetStoreActive_hlp(objStore, true);
        } //

        function fSetStoreActive_hlp(objStore, flag) {
            objStore.toggleClass('active', flag);
            objStore.find('.js-map-balloon-content .map-balloon-content').toggleClass('active', flag);

            let ballonObj = {
                name: $(this).find('.delivery__name').text().trim(),
                address: $(this).find('.delivery__address').text().trim(),
                schedule: $(this).find('.delivery__schedule').text().trim(),
                imagePath: $(this).find('.delivery__image').attr('src'),
            };

            let ballonHtml = createBalloonHtml(ballonObj);
            console.log(44444);

            placemarkList[ objStore.data('id') ].properties.set(
                'balloonContent',
                ballonHtml
            );
        } //

        objMain.find('.js-store-list').on('click', '.pharmacy-list__content', function(){
            var objStore = $(this).closest('.js-store');

            if (objStore.hasClass('active')) {
                return;
            }

            if (source == 'orderDelivery') {
                fSetStoreActive(objStore);
                params.onSelectStore();
                return;
            }

            if (!v22.fSetProcessing(true)) {
                return;
            }

            var data = {
                oper:    'setStore',
                storeId: objStore.data('id'),
            };

            fLog(data, 'data');

            v22.fAjax({
                url:      '/local/bitrix_ajax/v22.php',
                data:     data,
                dataType: 'json',
                fSuccess: function(response) {
                    //fLog(response);
                    //v22.fSetProcessing(false);

                    fSetStoreActive(objStore);

                    window.location.reload();
                }
            });
        });

        objMain.find('.js-store-list').on('click', '.btn-show-on-map', function(event){
            event.stopPropagation();

            var objStore  = $(this).closest('.js-store'),
                latitude  = objStore.data('latitude'),
                longitude = objStore.data('longitude');
            
            objMain.find('.js-nav-item[data-id=2]').click();

            ymapsMap.setCenter([latitude, longitude], 16);

            placemarkList[ objStore.data('id') ].balloon.open();
        });

        objMain.find('.js-store-list').on('click', '.js-favorite', function(){

            if (!v22.fSetProcessing(true)) {
                return;
            }

            var obj = $(this),
                value = (obj.hasClass('active')) ? 0 : 1,

                data = {
                    oper:    'setFavoriteStore',
                    storeId: obj.closest('.js-store').data('id'),
                    value:   value,
                };

            fLog(data, 'data');

            v22.fAjax({
                url:      '/local/bitrix_ajax/v22.php',
                data:     data,
                dataType: 'json',
                fSuccess: function(response) {
                    //fLog(response);
                    v22.fSetProcessing(false);
                    obj.toggleClass('active', value);
                }
            });
        });

        objMain.find('.search-menu__input').on('input', function(){
            var value = $(this).val().trim();

            if (searchValue == value) {
                return;
            }

            searchValue = value;
            fSearch();
        });

        objMain.find('.js-all-day-stores').change(function(){
            objMain.find('.js-all-day-stores-mobile').prop('checked', $(this).prop('checked'));
            fSearch();
        });

        objMain.find('.js-all-day-stores-mobile').change(function(){
            objMain.find('.js-all-day-stores').prop('checked', $(this).prop('checked'));
            fSearch();
        });

    } // function

    function fSearch() {

        var data = {
            all_day_store: objMain.find('.js-all-day-stores').is(':checked') ? 1 : 0,
            search_store:  searchValue,
            flagStoreList: 1,
        };

        if (source == 'orderDelivery') {
            data.storeId = 0;

            var obj = objMain.find('.js-store-list .js-store.active');
            if (obj.length) {
                data.storeId = obj.data('id');
            }
        }

        fLog(data, 'data');

        v22.fAjax({
            url:      urlAjax,
            data:     data,
            dataType: 'html',

            fSuccess: function(response) {
                var objResponse = $('<div>' + response + '</div>'),
                    objContent  = objResponse.find('.js-store-list');

                if (!objContent.length) {
                    fShowMessage('Ошибка 101');
                } else {
                    objMain.find('.js-store-list').html( objContent.html() );
                    fMapRefresh();
                }
            }
        });
    } // function

    function fShowSelected() {
        var objStore = objMain.find('.js-store-list .js-store.active');

        if (!objStore.length) {
            return;
        }

        var latitude  = objStore.data('latitude'),
            longitude = objStore.data('longitude'),
            offset    = objStore.data('offset');

        ymapsMap.setCenter([latitude, longitude], 16);

        placemarkList[ objStore.data('id') ].balloon.open();

        fScrollStoreList(offset);
    } // function

    // ### map (

    var objMap,
        ymapsMap,
        MyBalloonLayout,
        MyBalloonContentLayout,
        MyBalloonPanelContentLayout,
        placemarkList   = {},
        scrollStoreList = 0;

    function fMapInit() {

        objMap = objMain.find('.map');

        var latitude  = objMap.data('latitude'),
            longitude = objMap.data('longitude');

        ymapsMap = new ymaps.Map(
            objMap[0],
            {
                center:   [latitude, longitude],
                zoom:     12,
                controls: [],
            }, {
                searchControlProvider: 'yandex#search'
            }
        );

        fMapRefresh();

        objMap.on('click', '.map-balloon-content .info', function(){
            var storeId = $(this).closest('.map-balloon-content').data('id');
            objMain.find('.js-store-list .js-store[data-id=' + storeId + ']').find('.btn-select-store').click();
        });

    } // function

    function fMapRefresh() {
        if (!ymapsMap) {
            return;
        }

        ymapsMap.geoObjects.removeAll();

        var placemarkOptions = {
                iconLayout:      'default#image',
                iconImageHref:   '/local/templates/.default/components/sp-artgroup/catalog.store.list/.default/images/marker.png',
                iconImageSize:   [32, 40],
                iconImageOffset: [-16, -38],
                balloonShadow:   false,

                //balloonPanelMaxMapArea: 0, // Запрет балун-панели
            };

        if (1) {
            var balloonTemplate = v22.balloonTemplate.fGet();

            placemarkOptions.balloonLayout             = balloonTemplate.balloonLayout;
            placemarkOptions.balloonContentLayout      = balloonTemplate.balloonContentLayout;
            placemarkOptions.balloonPanelContentLayout = balloonTemplate.balloonPanelContentLayout;
        } //

        if (window.innerWidth < 390) {
            placemarkOptions.balloonAutoPanMargin = 10; // https://yandex.ru/dev/maps/jsapi/doc/2.1/ref/reference/Balloon.html
        }

        var offset = 0;
        
        objMain.find('.js-store-list .js-store').each(function(){
            
            let ballonObj = {
                name: $(this).find('.delivery__name').text().trim(),
                address: $(this).find('.delivery__address').text().trim(),
                schedule: $(this).find('.delivery__schedule').text().trim(),
                imagePath: $(this).find('.delivery__image').attr('src'),
            };

            let ballonHtml = createBalloonHtml(ballonObj);
            console.log($(this));

            var objStore       = $(this),
                latitude       = objStore.data('latitude'),
                longitude      = objStore.data('longitude'),
                balloonContent = ballonHtml,
                
                placemark = new ymaps.Placemark(
                    [latitude, longitude],
                    {
                        balloonContent: balloonContent,
                    },
                    placemarkOptions
                ),

                offsetThis = offset;
            
            objStore.data('offset', offset);

            offset += objStore.outerHeight(true);

            placemark.events.add('click', function(){
                if (0) {
                    fLog(objStore.data('id'), 'id');
                    fLog(objStore.find('.pharmacy_list_card_address').html(), 'address');
                }

                if (window.innerWidth < 768) {
                    scrollStoreList = offsetThis;
                } else {
                    fScrollStoreList(offsetThis);
                }
            });

            ymapsMap.geoObjects.add(placemark);

            placemarkList[ objStore.data('id') ] = placemark;
        });

    } // function

    function fScrollStoreList(offset) {
        objMain.find('.js-scroll-container').animate({
            scrollTop: offset,
        }, 1000);
    } // function

    // ### )

    // ##########

    if (!params) {
        params = {};
    }
    var source = params.source;

    var v22 = window.sp_global.v22,

        urlAjax     = v22.dataMain.urlPrefix + '/misc/store_selection.php',

        objMain     = params.objMain,
        searchValue = '';

    switch (source) {
        case 'headerModal':
            $('.js-btn-store-selection').click(function(){
                if (objMain.html()) {
                    fShowModal();
                } else {
                    fGetContent();
                }
            });
            break;
        case 'menuMobile':
            if (0) {
                $('#mmenu .js-mm-pick-pharmacy a').click(function(){
                    fLog('select store mobile');
                    if (!objMain.html()) {
                        fGetContent();
                    }
                });
            } else {
                v22.dataMisc.mmenu.API.bind('openPanel:before', function(panel){
                    if (panel.id == 'mmenu-store') {
                        fLog('select store mobile');
                        if (!objMain.html()) {
                            fGetContent();
                        }
                    }
                });
            }
            break;
        case 'pageStores':
        case 'pageCatalogDetail':
        case 'orderDelivery':
            fBindHandlers();
            objMain.find('.js-scroll-container-tmp').addClass('js-scroll-container').scrollbar(); // \js\components\template\template.js
            ymaps.ready(function(){
                fMapInit();
                fShowSelected();
            });
            break;
        default:
            return;
    } //

} // function

window.sp_global.v22.fBtnSpecialOrderClick = function(params) {
    
    function fLog(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    }

    var v22     = window.sp_global.v22,
        urlAjax = '/local/components/sp-artgroup/special_order.form/ajax_v22.php';

    if (!v22.fSetProcessing(true)) {
        return;
    }

    var data = {
            oper: 'getForm',
        };

    if (!params) {
        params = {};
    }

    if (params.product_id !== undefined) {
        data.product_id = params.product_id;

    } else if (params.order_id !== undefined) {
        data.order_id = params.order_id;

    } else if (params.product_str !== undefined) {
        data.product_str = params.product_str;
    }

    fLog(data, 'fBtnSpecialOrderClick');

    v22.fAjax({
        url:      urlAjax,
        data:     data,
        dataType: 'html',
        fSuccess: function(response) {
            //fLog(response);
            v22.fSetProcessing(false);
            $.fancybox.open(response);
            fBindHandlers();
        }
    });

    function fBindHandlers() {

        var objMain                = $('.form-special-order'),
            objError               = objMain.find('.js-step-1 .error-msg'),
            objBlockSelectQuantity = objMain.find('.js-quantity'),

            objDeliveryTime        = objMain.find('.js-delivery-time'),
            objPrice               = objMain.find('.product-price'),
            objPriceMsg            = objMain.find('.product-price-msg'),
            objPriceMsg_2          = objMain.find('.product-price-msg-2'),
            deliveryTimeDefault    = objDeliveryTime.html(),

            arObjField = {},

            dataProductQuantity = false,
            strTMP              = objMain.find('.js-data-product-quantity').html(),

            nothing;
        
        if (strTMP) {
            try {
                dataProductQuantity = JSON.parse(strTMP);
            } catch (e) {
                fLog('dataProductQuantity wrong');
            }
        } //
        fLog(dataProductQuantity, 'dataProductQuantity');
    
        function fCreateOrder() {

            if (v22.fIsProcessing() || !fCheckForm() || !v22.fSetProcessing(true)) {
                return;
            }

            var data = {
                    oper: 'createOrder',
                };

            for (var fieldName in arObjField) {
                data[ fieldName ] = arObjField[ fieldName ].val();
            }

            if (0) {
                //data.store_id = 3333;
                //data.quantity = 0;
                data.phone = 89009009090;
            }

            fLog(data, 'fBtnSpecialOrderClick');

            v22.fAjax({
                url:          urlAjax,
                data:         data,
                showErrorMsg: false,

                fSuccess: function(response) {
                    fLog(response);

                    if (response.error_msg) {
                        var flag = false;

                        if (response.field_invalid) {
                            if (response.field_invalid.store_id) {
                                arObjField.store_id.addClass('error');
                                flag = true;
                            }
                            if (response.field_invalid.username) {
                                arObjField.username.addClass('error');
                                flag = true;
                            }
                            if (response.field_invalid.phone) {
                                arObjField.phone.addClass('error');
                                flag = true;
                            }
                        }

                        if (!flag) {
                            objError.html(response.error_msg).removeClass('m-hidden');
                        }
                        
                    } else {
                        objMain.find('.js-step-1').hide();
                        objMain.find('.js-step-2').html(response.msg).removeClass('m-hidden');
                    }

                    v22.fSetProcessing(false);
                },
            });

        } // function

        function fCheckForm() {
            //return true;

            var result = true,
                value;

            objError.html('').addClass('m-hidden');
            objMain.find('.error').removeClass('error');

            // store_id
            value = arObjField.store_id.val();
            if (!value) {
                arObjField.store_id.addClass('error');
                result = false;
            }
            
            // username
            value = arObjField.username.val().trim();
            if (!value.length) {
                arObjField.username.addClass('error');
                result = false;
            }

            // phone
            value = arObjField.phone.val().trim();
            if (!v22.fValidatePhone(value)) {
                arObjField.phone.addClass('error');
                result = false;
            }

            // product
            if (arObjField.product) {
                value = arObjField.product.val().trim();
                if (!value.length) {
                    arObjField.product.addClass('error');
                    result = false;
                }
            }

            return result;
        } // function

        function fObjBlockSelectQuantity_Change (k) {
            var objInput = objBlockSelectQuantity.find('.js-value'),
                value    = parseInt(objInput.val());

            if (isNaN(value)) {
                value = 1;
            } else {
                value += k;
                value = Math.max(1, value);
            }

            objInput.val(value);

            if (dataProductQuantity) {
                var quantityMax = 0,
                    deliveryTimeStr,
                    i;

                for (i=0; i<dataProductQuantity.length; i++) {

                    deliveryTimeStr = dataProductQuantity[ i ].deliveryTimeStr;
                    quantityMax    += dataProductQuantity[ i ].quantity;

                    if (value <= quantityMax) {
                        break;
                    }
                } //

                var flag = (value > quantityMax);
                objDeliveryTime.html( (flag) ? deliveryTimeDefault : deliveryTimeStr );
                objPrice.toggle(!flag);
                objPriceMsg.toggle(!flag);
                objPriceMsg_2.toggle(flag);
            } //

        } // function

        objMain.find('.js-field').each(function(){
            arObjField[ $(this).attr('name') ] = $(this);
        });

        objMain.find('.js-btn-create-order').click(function(){
            fCreateOrder();
        });

        // ### quantity (
        if (dataProductQuantity) {
            fObjBlockSelectQuantity_Change(0); // Установим срок доставки и цену
        }

        objBlockSelectQuantity.find('.js-value').change(function(){
            fObjBlockSelectQuantity_Change(0);
        });

        objBlockSelectQuantity.find('.js-plus').click(function(){
            fObjBlockSelectQuantity_Change(1);
        });

        objBlockSelectQuantity.find('.js-minus').click(function(){
            fObjBlockSelectQuantity_Change(-1);
        });
        // ### )

        arObjField.username.add(arObjField.phone).focusin(function(){
            $(this).removeClass('error');
        });

        if (arObjField.product) {
            arObjField.product.focusin(function(){
                $(this).removeClass('error');
            });
        }

        arObjField.store_id.click(function(){
            $(this).removeClass('error');
        });

        arObjField.phone.inputmask({ mask: '+7 (999) 999-99-99' });

    } // function

} // function

window.sp_global.v22.fBtnSubscriptionClick = function(params) {
    
    function fLog(msg, label) {
        window.sp_global.fLog(msg, label);
    }

    var v22     = window.sp_global.v22,
        urlAjax = '/local/components/sp-artgroup/subscription.form/ajax_v22.php';

    if (!v22.fSetProcessing(true)) {
        return;
    }

    var objMain,
        objEmail,
        objError,
        
        product_id = (params.product_id !== undefined) ? params.product_id : null,
        data = {
            oper:       'getForm',
            product_id: product_id,
        };

    fLog(data, 'fBtnSubscriptionClick');

    v22.fAjax({
        url:      urlAjax,
        data:     data,
        dataType: 'html',
        fSuccess: function(response) {
            //fLog(response);

            v22.fSetProcessing(false);

            $.fancybox.open(response, {touch: false, autoFocus: false});

            objMain  = $('.form-subscription');
            objEmail = objMain.find('.js-email');
            objError = objMain.find('.js-step-1 .error-msg');

            objMain.find('.js-btn-subscribe').click(function(){
                fSubscribe();
            });

            objEmail.focusin(function(){
                $(this).removeClass('error');
            });
        }
    });

    function fSubscribe() {

        if (v22.fIsProcessing() || !fCheckForm() || !v22.fSetProcessing(true)) {
            return;
        }

        var email = (objEmail.length) ? objEmail.val() : '',
            
            data = {
                oper:       'subscribe',
                product_id: product_id,
                email:      email,
            };

        fLog(data, 'fBtnSubscriptionClick');

        v22.fAjax({
            url:          urlAjax,
            data:         data,
            showErrorMsg: false,

            fSuccess: function(response) {
                fLog(response);

                if (response.error_msg) {
                    if (response.field_invalid.email && objEmail.length) {
                        objEmail.addClass('error');
                    } else {
                        objError.html(response.error_msg);
                    }
                } else {
                    objMain.find('.js-step-1').hide();
                    objMain.find('.js-step-2').removeClass('m-hidden');
                }

                v22.fSetProcessing(false);
            },
        });

    } // function

    function fCheckForm() {
        //return true;

        var result = true;

        objError.html('');
        objEmail.removeClass('error');
    
        if (objEmail.length) {
            var value = objEmail.val();
            if (!v22.fValidateEmail(value)) {
                objEmail.addClass('error');
                result = false;
            } 
        }

        return result;
    } // function

} // function

window.sp_global.v22.favoriteProduct = {

    arId: [],

    fInit: function(arId) {
        this.arId = arId;
        this.msgHtml = $('.js-favorite-msg').first().html();
        this.fApply( $('body') );
    }, // function

    fApply: function(objMain) {

        function fLog(msg, label) {
            //window.sp_global.v22.fLog(msg, label);
        }
    
        var that = this,
            v22  = window.sp_global.v22;

        fLog(this.arId, 'fApply');

        objMain.find('.h2o_add_favor').each(function(){
            var objBtn = $(this),
                id     = objBtn.data('id').toString(),
                flag   = that.arId.includes(id);

            if (flag) {
                fLog(flag, id);
            }

            that.fSetFlag(objBtn, flag);

            if (objBtn.data('hasHandler')) {
                fLog('h2o_add_favor: hasHandler ' + id);
                return;
            }

            objBtn.data('hasHandler', 1);

            objBtn.click(function(){
                fLog('h2o_add_favor');

                if (!v22.fSetProcessing(true)) {
                    return;
                }
        
                v22.fAjax({
                    url: '/local/bitrix_ajax/v22.php',
                    data: {
                        oper: 'setProductFavorite',

                        'AJAX_CALL_FAVORITES_ADD':  'Y',
                        'h2o_add_favorites':        'Y',
                        'H2O_FAVORITES_ELEMENT_ID': id,
                    },
                    fSuccess: function(response) {
                        fLog(response);

                        v22.fSetProcessing(false);

                        if (response.notAuthorized) {
                            v22.fAuthShowForm({form: 'authorization'});
                            return;
                        }

                        if (response.ADD) {
                            that.fSetFlag(objBtn, true);
                            
                            if (!that.arId.includes(id)) {
                                that.arId.push(id);
                            }

                            v22.fShowGlobalMsg( objBtn.find('.msg-success') );

                        } else if (response.DELETE) {
                            that.fSetFlag(objBtn, false);
                            
                            var index = that.arId.indexOf(id);
                            if (index !== -1) {
                                that.arId.splice(index, 1);
                            }

                        } else {
                            window.sp_global.fShowMessage('Ошибка'); // Не должно
                        }

                        fLog(that.arId, 'favoriteProduct');
                    }
                });
            });

            if (that.msgHtml !== undefined) {
                objBtn.prepend( that.msgHtml );
                
                var obj = objBtn.find('.msg'),
                    x   = -((obj.outerWidth() - objBtn.width()) / 2);
                obj.css('right', x + 'px');
                
                obj = objBtn.find('.msg-success'),
                x   = -((obj.outerWidth() - objBtn.width()) / 2);
                obj.css('right', x + 'px');
            } //
        });
    }, // function

    fSetFlag: function(obj, flag) {
        obj.toggleClass('active', flag);
    }, // function

}; // object

window.sp_global.v22.fShowCatalogStores = function(params) {
    /*  window.sp_global.v22.fShowCatalogStores({
            storeList:       storeList,
            objBlockProduct: objItem.find('.js-block-product-for-store-list'),
            objBtnBuy:       objItem.find('.js-btn-buy-container'),
            productId:       productId,

            storeList:       storeList,
            flagPageOrder:   true,
        });
    */

    function fLog(msg, label) {
        window.sp_global.fLog(msg, label);
    }

    if (window.sp_global.v22.btnBuy.timer.id) {
        fLog('wait');
        return;
    }

    var v22 = window.sp_global.v22,
        objMain          = $('.catalog-store-list'),
        objItems         = objMain.find('.js-items'),
        objItemsOriginal = objMain.find('.js-items-original'),
        
        storeList = params.storeList;

    //fLog(storeList, 'storeList');

    if (!objItemsOriginal.html()) {
        objItemsOriginal.html( objItems.html() );
    }

    objItems.html('');

    var objItemsTMP = $('<div>' + objItemsOriginal.html() + '</div>');

    objItemsTMP.find('.item').each(function(){
        var obj     = $(this),
            storeId = obj.data('id');
        
        if (storeList.includes(storeId)) {
            obj.detach().appendTo(objItems);
        }
    });

    objItemsTMP.find('.item').removeClass('available');
    objItems.append( objItemsTMP.html() );

    if (!params.flagPageOrder) {
        objMain.find('.js-block-product').html( params.objBlockProduct.html() );

        var objTMP = objMain.find('.js-block-product .js-btn-buy-container-modal');
        objTMP.html( params.objBtnBuy.html() );

        v22.btnBuy.fAddHandler_BtnBuy({
            objBtnBuyContainer: objTMP,
            productId:          params.productId,
            flagModal:          true,
        });
    } //

    // ### map (

    if (!v22.dataMisc.fShowCatalogStores) {
        v22.dataMisc.fShowCatalogStores = {
            ymapsMap: null,
        };
    } //

    ymaps.ready(function(){
        if (!v22.dataMisc.fShowCatalogStores.ymapsMap) {

            var objMap    = objMain.find('.map'),
                latitude  = objMap.data('latitude'),
                longitude = objMap.data('longitude');

            v22.dataMisc.fShowCatalogStores.ymapsMap = new ymaps.Map(
                objMap[0],
                {
                    center:   [latitude, longitude],
                    zoom:     12,
                    controls: [],
                }, {
                    searchControlProvider: 'yandex#search'
                }
            );
        } //

        var ymapsMap = v22.dataMisc.fShowCatalogStores.ymapsMap,
            placemarkOptions = {
                iconLayout:      'default#image',
                iconImageHref:   '/local/templates/.default/components/sp-artgroup/catalog.store.list/.default/images/marker.png',
                iconImageSize:   [32, 40],
                iconImageOffset: [-16, -38],
                balloonShadow:   false,
            };

        if (1) {
            var balloonTemplate = v22.balloonTemplate.fGet();

            placemarkOptions.balloonLayout             = balloonTemplate.balloonLayout;
            placemarkOptions.balloonContentLayout      = balloonTemplate.balloonContentLayout;
            placemarkOptions.balloonPanelContentLayout = balloonTemplate.balloonPanelContentLayout;
        } //

        ymapsMap.geoObjects.removeAll();

        objItems.find('.item').each(function(){

            if (!$(this).hasClass('available')) {
                return;
            }

            let ballonObj = {
                name: $(this).find('.delivery__name').text().trim(),
                address: $(this).find('.delivery__address').text().trim(),
                schedule: $(this).find('.delivery__schedule').text().trim(),
                imagePath: $(this).find('.delivery__image').attr('src'),
            };

            let ballonHtml = createBalloonHtml(ballonObj);
            console.log(222222222);

            var objStore       = $(this),
                latitude       = objStore.data('latitude'),
                longitude      = objStore.data('longitude'),
                balloonContent = ballonHtml,

                placemark = new ymaps.Placemark(
                    [latitude, longitude],
                    {
                        balloonContent: balloonContent,
                    },
                    placemarkOptions
                );
            
            ymapsMap.geoObjects.add(placemark);
        });

    });
    // ### )

    $.fancybox.open(objMain);
} // function

window.sp_global.v22.fSetBasketQuantity = function(quantity) {
    $('.js-basket-quantity').html(quantity);
    $('.header .block-basket, .main-menu__fixed-menu .block-basket').toggleClass('m-empty', !Boolean(quantity) );
} // function

window.sp_global.v22.btnBuy = {

    lastObjectId: 0,
    items: {},

    timer: {
        id:       null,
        objectId: 0,    // Данный элемент можно редактировать (изменить количество и удалить) пока запрос не отправлен
        delay:    1000,
    },

    fChangeQuantity: function (params) {
        // fChangeQuantity({productId, quantityMax, quantity, objectId})
        
        //window.sp_global.v22.fLog(params, 'fChangeQuantity params');

        var item = this.items[ params.productId ];

        item.data = {
            quantityMax: params.quantityMax,
            quantity:    params.quantity,
        };

        for (var objectId in item.list_fRefresh) {
            if (objectId != params.objectId) {
                item.list_fRefresh[ objectId ]();
            }
        }
    }, // function

    fAddHandler_BtnBuy: function (params) {
        /*  window.sp_global.v22.btnBuy.fAddHandler_BtnBuy({
                objBtnBuyContainer: ,
                quantityMax: ,
                quantity: ,
                productId: ,
                searchString: ,

                objBtnBuyContainer: ,
                productId:          productId,
                flagModal:          true,
            });
        */

        function fLog(msg, label) {
            window.sp_global.v22.fLog(msg, label);
        } //

        var that    = this,
            v22     = window.sp_global.v22,
            urlAjax = '/local/bitrix_ajax/v22.php',
            
            objMain     = params.objBtnBuyContainer,
            objValue    = objMain.find('.js-value'),
            objErrorMsg = objMain.find('.global-error-msg'),
            objErrorMsg = objMain.find('.global-error-msg'),
            
            textMaxQuantity = objErrorMsg.html(),
            textNotInStore  = objErrorMsg.data('msg-not-in-store'),
            productId = params.productId,
            is_wholesale = params.is_wholesale,
            
            timer    = this.timer,
            objectId = ++this.lastObjectId;


        if (params.flagModal) {
            if (!this.items[ productId ]) {
                return; // Не ожидается
            }
            params.quantityMax = this.items[ productId ].data.quantityMax;
            params.quantity    = this.items[ productId ].data.quantity;
        } //
    
        var quantityMax     = params.quantityMax,
            quantityInitial = params.quantity,
            quantityPrev    = quantityInitial;

        function fRefresh() {
            var data = that.items[ productId ].data;

            quantityMax     = data.quantityMax;
            quantityInitial = data.quantity;
            quantityPrev    = quantityInitial;

            fUpdateHtml(data.quantity);
        } //

        if (!params.flagModal) {
            if (!this.items[ productId ]) {
                this.items[ productId ] = {
                    data: {
                        quantityMax: quantityMax,
                        quantity:    quantityInitial,
                    },
                    list_fRefresh: {},
                };
            }
            this.items[ productId ]['list_fRefresh'][ objectId ] = fRefresh;
        }

        function fUpdateHtml(quantity) {
            var flag = Boolean(quantity);

            objMain.find('.js-container-1').toggleClass('active', !flag);
            objMain.find('.js-container-2').toggleClass('active', flag);
    
            objValue.val(quantity);
        } //

        // # (
        if (quantityInitial) {
            fUpdateHtml(quantityInitial);
        }

        function fQuantityChange(k) {

            if (v22.fIsProcessing() || (timer.id && timer.objectId != objectId)) {
                fLog('Processing or timer');
                objValue.val(quantityPrev);
                return;
            }

            var value = parseInt(objValue.val());
    
            if (isNaN(value)) {
                value = 1;
            } else {
                value += k;
                value = Math.max(1, value);
            }
    
            if (value > quantityMax) {
                value = quantityMax;
                v22.fShowGlobalMsg(objErrorMsg, textMaxQuantity);
            }
    
            objValue.val(value);

            // ###

            if (value == quantityPrev) {
                fLog('value == quantityPrev');
                return;
            }
            quantityPrev = value;

            if (timer.id) {
                clearTimeout(timer.id);
                timer.id = null;
                fLog('stop timer');
            }
            
            if (quantityInitial == value) {
                fLog('quantityInitial == value');
                return;
            }

            // # Заводим таймер (
            fLog('start timer; value=' + value + ' objectId=' + objectId);

            timer.objectId = objectId;

            timer.id = setTimeout(function(){
                timer.id = null;

                if (!v22.fSetProcessing(true)) {
                    return;
                }

                var data = {
                        oper:        'basketAdd',
                        productId:   productId,
                        quantity:    value,
                        is_wholesale: is_wholesale
                    };
                
                if (params.searchString) {
                    data.searchString = params.searchString; 
                }

                fLog(data);

                v22.fAjax({
                    url:  urlAjax,
                    data: data,
                    fSuccess: function(response) {
                        fLog(response);
                        v22.fSetProcessing(false);
    
                        console.log(textMaxQuantity);

                        if (response.msg == 'not enough quantity') {
                            v22.fShowGlobalMsg(objErrorMsg, textMaxQuantity);
                        
                        } else if (response.msg == 'not in store') {
                            v22.fShowGlobalMsg(objErrorMsg, textNotInStore);
                        } //

                        quantityInitial = response.quantity;
                        quantityPrev    = quantityInitial;
                        quantityMax     = response.quantityMax;

                        that.fChangeQuantity({
                            productId:   productId,
                            quantityMax: response.quantityMax,
                            quantity:    response.quantity,
                            objectId:    objectId,
                        });

                        fUpdateHtml(response.quantity);
                        v22.fSetBasketQuantity(response.basketCount);

                        v22.fOrderRefresh();
                    } //
                });

            }, timer.delay);
            // # )

        } // function

        objValue.change(function(){
            fQuantityChange(0);
        });
        objMain.find('.js-btn-plus').click(function(){
            fQuantityChange(1);
        });
        objMain.find('.js-btn-minus').click(function(){
            fQuantityChange(-1);
        });
        // # )

        objMain.find('.js-btn-buy').click(function(){
            if (!quantityMax) {
                v22.fShowGlobalMsg(objErrorMsg, textNotInStore);

            } else if (!v22.fIsProcessing() && !timer.id) {
                fQuantityChange(1);
                fUpdateHtml(1);
            }
        });

        objMain.find('.js-btn-remove').click(function(){

            if (v22.fIsProcessing() || (timer.id && timer.objectId != objectId)) {
                fLog('Processing or timer');
                return;
            }

            if (timer.id) {
                clearTimeout(timer.id);
                timer.id = null;
                fLog('stop timer');
            }

            if (!v22.fSetProcessing(true)) {
                return;
            }

            var data = {
                    oper:      'basketRemove',
                    productId: productId,
                };

            fLog(data);
            
            v22.fAjax({
                url:  urlAjax,
                data: data,
                fSuccess: function(response) {
                    fLog(response);
                    v22.fSetProcessing(false);

                    quantityInitial = 0;
                    quantityPrev    = quantityInitial;

                    that.fChangeQuantity({
                        productId:   productId,
                        quantityMax: quantityMax,
                        quantity:    0,
                        objectId:    objectId,
                    });

                    fUpdateHtml(0);
                    v22.fSetBasketQuantity(response.basketCount);
                } //
            });

        });

    }, // function

}; // object

window.sp_global.v22.fShowCatalogDelivery = function(params) {
    /*  window.sp_global.v22.fShowCatalogDelivery({
            objBlockProduct: objItem.find('.js-block-product-for-store-list'),
            objBtnBuy:       objItem.find('.js-btn-buy-container'),
            productId:       productId,
        });
    */

    function fLog(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    }

    if (window.sp_global.v22.btnBuy.timer.id) {
        fLog('wait');
        return;
    }

    var v22 = window.sp_global.v22,
        objMain = $('.modal-catalog-delivery');

    objMain.find('.js-block-product').html( params.objBlockProduct.html() );

    var objTMP = objMain.find('.js-block-product .js-btn-buy-container-modal');
    objTMP.html( params.objBtnBuy.html() );
        
    v22.btnBuy.fAddHandler_BtnBuy({
        objBtnBuyContainer: objTMP,
        productId:          params.productId,
        flagModal:          true,
    });

    $.fancybox.open(objMain);
} // function

window.sp_global.v22.fInitMenuCatalog = function() {

    function fLog(msg, label) {
        //window.sp_global.fLog(msg, label);
    }

    var objBtnMenu     = $('.js-btn-menu-catalog'),
        objMenu        = $('.js-menu-catalog'),
        objMenuContent = objMenu.find('.content');

    // # Открыть/закрыть меню (
    function fOpenClose(flag) {
        objBtnMenu.toggleClass('main-menu__button--active', flag);
        objMenu.toggle(flag);
        $('body').toggleClass('template--no-overflow', flag); // overflow: hidden;

        if (flag) {
            $(document).on('mousedown.menuCatalog', fMousedown); // "menuCatalog" - namespace для удаления обработчика
            $(window).on('resize.menuCatalog', fResize);
        } else {
            $(document).off('mousedown.menuCatalog', fMousedown);
            $(window).off('resize.menuCatalog', fResize);
        }
    } //

    function fMousedown(e) {
        fLog('fMousedown');

        if (!objBtnMenu.is(e.target)
            && !objBtnMenu.has(e.target).length
            && !objMenuContent.is(e.target)
            && !objMenuContent.has(e.target).length
        ) {
            fLog('fMousedown: close');
            fOpenClose(false);
        }
    } //

    // При уменьшении ширины окна < 768 меню закрывается
    function fResize() {
        //fLog('fResize' + window.innerWidth);

        if (window.innerWidth < 768) {
            fLog('fResize: close');
            fOpenClose(false);
        }
    } //
    
    objBtnMenu.click(function(){
        var flag = !objBtnMenu.hasClass('main-menu__button--active');
        fOpenClose(flag);
    });
    // # )

    // # Открыть меню второго уровня (
    var arObjItemL1 = objMenu.find('.main-menu__item'),
        arObjMenuL2 = objMenu.find('.main-menu__content-tab');

    arObjItemL1.mouseenter(function(){

        arObjItemL1.removeClass('main-menu__item--hover');
        $(this).addClass('main-menu__item--hover');

        arObjMenuL2.removeClass('main-menu__content-tab--active');
        arObjMenuL2.filter('.js-main-menu-tab-' + $(this).data('open-tab')).addClass('main-menu__content-tab--active');
    });
    // # )

} // function

window.sp_global.v22.fInitMenuMobile = function() {

    function fLog(msg, label) {
        window.sp_global.fLog(msg, label);
    }

    var v22 = window.sp_global.v22;

    // Заголовок "Выбор региона", "Выбор города", "Выбор аптеки"
    function fOpenPanel(panel) {
        var objTitle = $('#mmenu .mm-navbars_top .mm-navbar__title'),
            title    = $(panel).find('.js-panel').data('menu-title');

        objTitle.find('.main-menu__region-select').remove();

        //fLog('panel.id ' + panel.id);

        if (flagBackIsClose && panel.id == 'mmenu-root') {
            flagBackIsClose = false;
            api.close();
            fLog('flagBackIsClose: close');
        }

        if (title) {
            fLog('panel title: ' + title);
            objTitle.prepend('<div class="main-menu__region-select">' + title + '</div>');
        } else {
            //console.log(panel);
        }
    } //

    v22.dataMisc.mmenu = new Mmenu('#mmenu',
        {
            hooks: {
                'openPanel:before': fOpenPanel,
            },
            extensions: [
                'position-front',
                'shadow-panels',
                'fx-panels-slide-100',
                'border-none',
                'fullscreen',
            ],
            navbars: [
                {
                    position: 'top',
                    content: ['prev', 'title', 'close'],
                },
            ],
        },
        {
            language: 'ru',
        },
    );

    var api             = v22.dataMisc.mmenu.API,
        flagBackIsClose = false;

    $('.main-menu__fixed-menu .js-btn-dialog-select-city').click(function(event){
        event.preventDefault();

        flagBackIsClose = true;

        $.fancybox.close();
        if (1) {
            api.openPanel( document.getElementById('mmenu-region') );
            api.open();
        } else {
            $('.main-menu__button')[0].click();
            $('.js-mm-pick-region a')[0].click();
        }
    });

    $('.main-menu__fixed-menu .js-btn-dialog-select-store').click(function(event){
        event.preventDefault();

        flagBackIsClose = true;

        $.fancybox.close();
        if (1) {
            api.openPanel( document.getElementById('mmenu-store') );
            api.open();
        } else {
            $('.main-menu__button')[0].click();
            $('.js-mm-pick-pharmacy a')[0].click();
        }
    });

} // function

window.sp_global.v22.komboxFilter = {

    flagMobileReady: false,

    fInit: function() {

        this.objMain = $('.kombox-filter');

        this.fRefreshShowAll();

        this.objMain.find('.js-btn-show-more').click(function(){
            $(this).closest('.m-checkbox-list').addClass('show-all');
            $(this).hide();
        });

        this.objMain.find('.filter_reset_btn').click(function(){
            var objList = $(this).closest('.m-checkbox-list'),
                objInput;

            objList.find('.lvl2.kombox-checked').each(function(){
                objInput = $(this).find('input');
                objInput.prop('checked', false);
            });
            if (objInput) {
                objInput.prop('checked', true).click();
            }

            $(this).hide();
        });

        this.fInitBtnMobile();
    },

    fRefreshShowAll: function() {
        this.objMain.find('.m-checkbox-list').each(function(){
            var objList = $(this),
                n   = objList.find('.lvl2.kombox-checked').length,
                n_2 = 0;

            objList.find('.filter_reset_btn').toggle( Boolean(n) );

            if (objList.hasClass('show-all')) {
                return;
            }

            objList.find('.lvl2').each(function(){
                var objItem  = $(this),
                    flagHide = false;

                if (objItem.hasClass('kombox-disabled') && !objItem.hasClass('kombox-checked')) {
                    return;
                }

                if (!objItem.hasClass('kombox-checked')) {
                    n++;
                    if (n > 4) {
                        flagHide = true;
                        n_2++;
                    }
                }
                objItem.toggleClass('block-hidden', flagHide);
            });

            objList.find('.js-btn-show-more').toggle( Boolean(n_2) );
            if (n_2) {
                objList.find('.js-btn-show-more .js-value').text(n_2);
            }
        });
    },

    fInitBtnMobile: function() {

        function fLog(msg, label) {
            //window.sp_global.v22.fLog(msg, label);
        }
    
        var that = this,
            objBtn     = $('.js-btn-filter-mobile'),
            objContent = $('.js-filter-mobile'),
            flagOpen   = false;

        if (window.innerWidth > 767) {
            this.fInitKombox();
        } else {
            $('.filter-wrap__categories .categories').detach().appendTo( $('.js-filter-mobile-categories') );
            $('.kombox-filter').detach().appendTo( $('.js-filter-mobile') );
            $('.kombox-filter .filter').removeClass('filter');
        } //

        // # Открыть / закрыть (
        function fOpenClose(flag) {
            flagOpen = flag;
            objContent.toggle(flag);

            if (flag && !that.flagMobileReady) {
                that.fInitKombox();
                that.flagMobileReady = true;
            }

            if (flag) {
                $(document).on('mousedown.filterMobile', fMousedown); // "filterMobile" - namespace для удаления обработчика
            } else {
                $(document).off('mousedown.filterMobile', fMousedown);
            }
        } //

        function fMousedown(e) {
            fLog('fMousedown');

            if (!objBtn.is(e.target)
                && !objBtn.has(e.target).length
                && !objContent.is(e.target)
                && !objContent.has(e.target).length
            ) {
                fLog('fMousedown: close');
                fOpenClose(false);
            }
        } //

        objBtn.click(function(){
            fOpenClose(!flagOpen);
        });
        // # )
    },

}, // object

window.sp_global.v22.objSearch = {

    fShowMessage: window.sp_global.fShowMessage,

    fLog: function(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    },

    timer: {
        id:           null,
        searchString: null,
        processing:   false,
        repeat:       false,
        cancel:       false,
        delay:        500, // 500
    },

    minLen:          3,
    fieldValue_prev: null,
    cache:           {},
    hasHtml:         false,

    fInit: function() {
        var that = this,
            objMain     = $('.header__search'),
            objBtnClear = objMain.find('.btn-clear');

        this.objField          = objMain.find('.search-menu__input');
        this.objIconProcessing = objMain.find('.icon-processing');
        this.objSearchResult   = objMain.find('.search-menu__dropdown');

        function fToggleBtnClear() {
            objBtnClear.toggle( Boolean(that.objField.val().length) );
        }

        fToggleBtnClear();

        this.objField.keyup(function(){

            fToggleBtnClear();

            var fieldValue = that.objField.val().trim(),
                timer      = that.timer;

            if (fieldValue === that.fieldValue_prev) {
                // Изменений нет
                return;
            }
            that.fieldValue_prev = fieldValue;

            timer.searchString = fieldValue;

            that.hasHtml = false;
            that.fObjSearchResultShow(false);

            if (fieldValue.length < that.minLen) {
                if (timer.id) {
                    if (timer.processing) {
                        timer.repeat = false;
                        timer.cancel = true;
                    } else {
                        clearTimeout(timer.id);
                        timer.id = null;
                    }
                }
                that.fLog('fieldValue.length short');

                return;
            }

            if (timer.id) {
                if (timer.processing) {
                    // Выполняется обработка. По окончании нужно повторить
                    timer.repeat = true;
                    timer.cancel = false;
                    that.fLog('set repeat: ' + timer.searchString);
                    return;
                }

                clearTimeout(timer.id);
                timer.id = null;
                that.fLog('clearTimeout');
            }

            that.fSearch();
        });

        objMain.find('form').submit(function(event){
            event.preventDefault();

            var value = that.objField.val().trim();

            if (value.length) {
                var url = $(this).attr('action') + encodeURIComponent(value);

                that.fLog('search "' + value + '": ' + url);
                window.location.assign(url);
            }
        });

        objMain.find('.btn-search').click(function(){
            objMain.find('form').submit();
        });

        objBtnClear.click(function(){
            that.objField.val('');
            fToggleBtnClear();
        });

        this.fObjSearchResultSetHeight();
    }, // function

    fSearch: function() {
        var that  = this,
            timer = this.timer,
            html  = this.cache[ timer.searchString ];

        timer.processing = false;
        timer.repeat     = false;
        timer.cancel     = false;

        if (html !== undefined) {
            this.fLog('get from cache: ' + timer.searchString);

            if (!html) {
                this.fLog('not found');
            } else {
                this.fLog('show result');
                this.fObjSearchResultSetContent(html);
            }

            return;
        } //

        this.fLog('setTimeout');

        timer.id = setTimeout(function(){
            that.fSearch_hlp();
        }, timer.delay);
    }, // function

    fSearch_hlp: function () {
        var that         = this,
            timer        = this.timer,
            searchString = timer.searchString,
            
            error = {
                count: 0,
                msg:   '',
                date:  new Date().getTime(),
            };

        timer.processing = true;

        this.fLog('processing start');

        that.objIconProcessing.show();

        var data = {
            oper:      'search',
            ajax_call: 'y',
            q:         searchString,
            l:         that.minLen,
        };

        if(window.location.href.indexOf('vterminal') !== -1) {
            data['is_terminal'] = 'Y';
        }

        this.fLog(data, 'data');

        function fAjax() {

            $.ajax({
                url:      '/local/bitrix_ajax/v22.php',
                type:     'POST',
                dataType: 'html',
                data:     data,

                success: function (response) {
                    
                    that.fAjaxStop();

                    var html = '';

                    //that.fLog(response.substring(0, 100) + '#...');

                    if (!response) {
                        that.fShowMessage('Ошибка 100');
                    } else {
                        var objTMP = $('<div></div>');
                        objTMP.html(response);

                        var objData = objTMP.find('.js-search-result-data');
                        
                        if (!objData.length) {
                            that.fShowMessage('Ошибка 101');
                        } else {
                            if (objData.html() == 'not_found') {
                                that.fLog('not found: ' + searchString);
                                that.cache[ searchString ] = false;
                            } else {
                                that.fLog('found: ' + searchString);
                                objData.remove();
                                html = objTMP.html();
                                that.cache[ searchString ] = html;
                            }
                        }
                    } //
                    
                    if (timer.cancel) {
                        that.fLog('processing cancel');
                        return;
                    }
                    if (timer.repeat) {
                        that.fLog('repeat...');
                        that.fSearch(); // delay again
                        return;
                    }

                    if (html) {
                        that.fLog('show result');
                        that.fObjSearchResultSetContent(html);
                    }

                    that.fLog('processing stop');
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    //that.fLog(jqXHR, 'jqXHR');
                    that.fLog(jqXHR.status, 'jqXHR.status');
                    //that.fLog(textStatus, 'textStatus');
                    //that.fLog(errorThrown, 'errorThrown');

                    error.count++;
                    data.error_count = error.count;

                    if (error.count <= 5) {
                        var date = new Date().getTime(),
                            str = ((date - error.date) / 1000).toFixed(2);

                        error.msg += jqXHR.status + '(' + str + ')' + '#';

                        data.error_msg = error.msg;
                    }

                    setTimeout(function(){
                        if (1) {
                            that.fLog(data.error_count, 'error_count');
                            that.fLog(data.error_msg, 'error_msg');
                        }
            
                        error.date = new Date().getTime();
                        fAjax();
                    }, 1000);
                }
            });
        } // function

        fAjax();

    }, // function

    fAjaxStop: function() {
        this.timer.id = null;
        this.objIconProcessing.hide();
    }, // function

    fObjSearchResultSetContent: function (content) {
        this.hasHtml = true;

        this.objSearchResult.html(content);
        this.fBindHandlers();

        if (this.objField.is(':focus')) {
            this.fObjSearchResultShow(true);
        }
    }, // function

    fObjSearchResultShow: function (flag) {
        var that = this;

        function fMousedown(event) {
            var target = $(event.target);

            //that.fLog('fMousedown');

            if (target.is(that.objField) || target.is(that.objSearchResult)) {
                return;
            }

            if (!that.objSearchResult.has(target).length) {
                that.fLog('fMousedown: close');
                that.fObjSearchResultShow(false);
            }
        }
    
        $('.header__search .search-menu-wrapper').toggleClass('search-menu--active', flag);

        $('html, body').css({
            overflow: (flag) ? 'hidden' : 'auto',
        });

        if (flag) {
            $(document).on('mousedown.search', fMousedown); // "search" - namespace для удаления обработчика
        } else {
            $(document).off('mousedown.search');
        }
    }, // function

    fBindHandlers: function () {
        var v22     = window.sp_global.v22,
            that    = this,
            objMain = this.objSearchResult,
            
            basketProductIdAndQuantity = v22.dataMain.basketProductIdAndQuantity;

        objMain.find('.search-menu__dropdown-item').each(function(){
            var objItem   = $(this),
                productId = objItem.data('id');
            
            // ### button buy (
            var obj = objItem.find('.js-btn-buy-container'),
                quantityMax = objItem.data('quantity_max'),
                is_wholesale = objItem.data('is-wholesale'),
                quantity    = (basketProductIdAndQuantity[ productId ]) ? basketProductIdAndQuantity[ productId ] : 0;

            if (obj.length) {
                v22.btnBuy.fAddHandler_BtnBuy({
                    objBtnBuyContainer: obj,
                    quantityMax:        quantityMax,
                    quantity:           quantity,
                    productId:          productId,
                    is_wholesale:       is_wholesale,
                });
            } //
            // ### )

            objItem.find('.js-btn-subscription').click(function(){
                that.fObjSearchResultShow(false);
                v22.fBtnSubscriptionClick({ product_id: productId });
            });

           /*  objItem.find('.js-btn-wholesale-order').click(function(){
                v22.fBtnWholeSaleOrderClick();
            }); */
    
            objItem.find('.js-btn-special-order').click(function(){
                that.fObjSearchResultShow(false);
                v22.fBtnSpecialOrderClick({ product_id: productId });
            });
        });

    }, // function

    fObjSearchResultSetHeight: function () {
        var h = window.innerHeight - $('.header').outerHeight();
        this.objSearchResult.css('max-height', h + 'px');
    }, // function

} // object

window.sp_global.v22.fOrderRefresh = function() {
    if ($('.page-order').length) {
        window.location.reload();
    }
} // function

window.sp_global.v22.fInitNewsSubscription = function() {

    function fLog(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    }

    var v22 = window.sp_global.v22,

        objMain    = $('.news-subscription'),
        objModal   = $('.modal-news-subscription-result'),
        arObjField = {};

    objMain.find('.js-field').each(function(){
        arObjField[ $(this).attr('name') ] = $(this);

        $(this).focusin(function(){
            $(this).removeClass('error');
        });
    });

    objMain.find('.js-btn-send').click(function(){

        if (v22.fIsProcessing() || !fCheckForm() || !v22.fSetProcessing(true)) {
            return;
        }

        var data = {
                oper:  'subscribeNews',
                email: arObjField.email.val().trim(),
                city:  arObjField.city.val(),
            };

        fLog(data, 'data');

        if (0) {
            v22.fSetProcessing(false);
            return;
        }

        v22.fAjax({
            url:          v22.urlAjax,
            data:         data,
            showErrorMsg: false,
            fSuccess: function(response) {
                fLog(response);

                var flagError = Boolean(response.error_msg);

                objModal.find('.msg-success').toggleClass('m-hidden', flagError);
                objModal.find('.msg-error').html(response.error_msg).toggleClass('m-hidden', !flagError);
                
                if (!flagError) {
                    objMain.remove();
                }
                v22.fSetProcessing(false);

                $.fancybox.open(objModal);
            } //
        });

    });

    function fCheckForm() {
        //return true;

        var result = true,
            value;

        objMain.find('.error').removeClass('error');

        value = arObjField.email.val().trim();
        if (!v22.fValidateEmail(value)) {
            arObjField.email.addClass('error');
            result = false;
        }

        value = arObjField.city.val();
        if (!value.length) {
            arObjField.city.addClass('error');
            result = false;
        }

        return result;
    } // function

} // function

window.sp_global.v22.fInitHeaderFixed = function() {
    var objHeader = $('.header'),
        flag,
        flagPrev;

    function fScroll() {
        flag = ($(window).scrollTop() >= 100);
        if (flagPrev !== flag) {
            flagPrev = flag;
            objHeader.toggleClass('fixed', flag);
            //window.sp_global.v22.fLog(flag, 'flag ' + objHeader.length);
        }
    }

    $(window).scroll(function(){
        fScroll();
    });

    fScroll();
} // function

window.sp_global.v22.generateDeliveryProductPopap = function(obj) {

    let dataObj = {
        img: obj.find(`[data-popap="img"]`).attr('src'),
        name: obj.find(`[data-popap="name"]`).text().trim(),
        link: obj.find(`[data-popap="link"]`).attr('href'),
        manufacturer: obj.find(`[data-popap="manufacturer"]`).text().trim(),
    };

    if(dataObj.link == '' || dataObj.link == undefined)
    {
        dataObj.link = 'javascript:void(0);';
    }
    if(dataObj.manufacturer == '' || dataObj.manufacturer == undefined)
    {
        dataObj.manufacturer = '';
    }

    return `
        <div class="js-block-product-for-store-list" hidden>
            <div class="hor-card hor-card--md col-12">
                <div class="row mx-0 align-content-stretch">

                    <div class="hor-card__image-row col-12 col-md-auto d-flex justify-content-center">
                        <div class="hor-card__image-cont hor-card__image-cont--md">
                            <a href="${dataObj.link}">
                                <img src="${dataObj.img}" />
                            </a>
                        </div>
                    </div>

                    <div class="hor-card__content hor-card__content--md col-12 col-md">
                        <a 
                            class="hor-card__title hor-card__title--mb text-center text-md-left"
                            href="${dataObj.link}"
                        >
                            ${dataObj.name}
                        </a>
                        <div class="hor-card__text template--hideon-sm">
                            ${dataObj.manufacturer}
                        </div>
                    </div>

                    <div class="hor-card__control-col hor-card__control-col--md col-12 col-md-auto">
                        <div class="hor-card__price --- global-spp-store"></div>
                        <div class="hor-card__buttons-col hor-card__buttons-col--md --- js-btn-buy-container-modal"></div>
                    </div>

                </div>
            </div>
        </div>
    `;
},

$(document).ready(function(){
    window.sp_global.v22.fInitHeaderFixed();
});

window.sp_global.v22.fInitDialogOpen = function() {
    
    $('.js-diolog-open').click(function(event){
        event.preventDefault();
        $.fancybox.close();
        $.fancybox.open({
            src: $(this).data('open-target'),
        });
    });

} // function

window.sp_global.v22.fFeedbackShowForm = function() {
    // v22.fFeedbackShowForm()

    function fLog(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    }

    var v22 = window.sp_global.v22,
        url = v22.dataMain.urlPrefix + '/misc/feedback_modal.php',

        selector = '.modal-feedback',
        objMain  = $(selector);
    
    $.fancybox.close();

    if (0 || objMain.data('flagSent')) {
        fLog('flagSent');
        objMain.remove();
        objMain = '';
    }
    
    if (objMain.length) {
        $.fancybox.open(objMain);

    } else {
        if (!v22.fSetProcessing(true)) {
            return;
        }
        fLog('getForm');

        v22.fAjax({
            url:          url,
            data:         {},
            dataType:     'html',
            checkContent: true,
            fSuccess: function(response) {
                $('body').append(response);
                objMain = $(selector);
                fBindHandlers();
                v22.fSetProcessing(false);
                $.fancybox.open(objMain);
            }
        });
    } //

    function fBindHandlers() {
        var objForm     = objMain.find('.js-form'),
            objErrorMsg = objMain.find('.error-msg'),
            arObjField  = {};

        objMain.find('.js-field').each(function(){
            arObjField[ $(this).attr('name') ] = $(this);

            $(this).focusin(function(){
                $(this).removeClass('error');
            });
        });

        objMain.find('.js-input-file').change(function(){
            var input = this;
    
            if (input.files && input.files[0]) {
                objMain.find('.js-file-name').html(input.files[0]['name']).show();
            }
        });
    
        objForm.submit(function(event){
            event.preventDefault();

            if (v22.fIsProcessing() || !fCheckForm() || !v22.fSetProcessing(true)) {
                return;
            }

            var formData = new FormData(objForm[0]);

            formData.append('oper', 'save');

            $.ajax({
                url:         url,
                type:        'POST',
                dataType:    'json',
                data:        formData,
                processData: false,
                contentType: false,
    
                success: function (response) {
                    v22.fSetProcessing(false);
    
                    if (!response) {
                        fShowMessage('Ошибка 100');
                    } else {
                        fLog(response);
    
                        if (response.error_msg) {
                            var flag = false;
    
                            if (response.field_invalid) {
                                for (var fieldName in response.field_invalid) {
                                    if (arObjField[ fieldName ]) {
                                        arObjField[ fieldName ].addClass('error');
                                        flag = true;
                                    }
                                }
                            } //
    
                            if (response.showError || !flag) {
                                objErrorMsg.html(response.error_msg).show();
                            }
    
                        } else {
                            objForm.hide();
                            objMain.find('.step-2').show();
                            objMain.data('flagSent', 1);
                        } //
    
                        if (response.debug_info && response.debug_info.text) {
                            objMain.find('.debug_info').html(response.debug_info.text).show();
                        }
                    } //
                },
                error: function () {
                    v22.fSetProcessing(false);
                    fShowMessage('Нет соединения с сервером (код ошибки: 31)'); // # ajax error #
                }
            });
        }); // function

        function fCheckForm() {
            //return true;

            var result = true,
                value;
    
            objErrorMsg.html('').hide();
            objMain.find('.error').removeClass('error');

            value = arObjField.fio.val().trim();
            if (!value.length) {
                arObjField.fio.addClass('error');
                result = false;
            }

            value = arObjField.email.val().trim();
            if (!v22.fValidateEmail(value)) {
                arObjField.email.addClass('error');
                result = false;
            }

            value = arObjField.text.val().trim();
            if (!value.length) {
                arObjField.text.addClass('error');
                result = false;
            }

            if (arObjField.captcha_word) {
                value = arObjField.captcha_word.val().trim();
                if (!value.length) {
                    arObjField.captcha_word.addClass('error');
                    result = false;
                }
            }

            return result;
        } // function

    } //

} // function

window.sp_global.v22.fProposeTopicSchoolOfBeautifyAndHealthShowForm = function() {
    // v22.fProposeTopicSchoolOfBeautifyAndHealthShowForm()

    function fLog(msg, label) {
        window.sp_global.v22.fLog(msg, label);
    }

    var v22 = window.sp_global.v22,
        url = v22.dataMain.urlPrefix + '/misc/propose_topic_school_of_beautify_and_health_modal.php',

        selector = '.js-modal-propose-topic-school-of-beautify-and-health',
        objMain  = $(selector);
    
    $.fancybox.close();

    if (0 || objMain.data('flagSent')) {
        fLog('flagSent');
        objMain.remove();
        objMain = '';
    }
    
    if (objMain.length) {
        $.fancybox.open(objMain);

    } else {
        if (!v22.fSetProcessing(true)) {
            return;
        }
        fLog('getForm');

        v22.fAjax({
            url:          url,
            data:         {},
            dataType:     'html',
            checkContent: true,
            fSuccess: function(response) {
                $('body').append(response);
                objMain = $(selector);
                fBindHandlers();
                v22.fSetProcessing(false);
                $.fancybox.open(objMain);
            }
        });
    } //

    function fBindHandlers() {
        var objForm     = objMain.find('.js-form'),
            objErrorMsg = objMain.find('.error-msg'),
            arObjField  = {};

        objMain.find('.js-field').each(function(){
            arObjField[ $(this).attr('name') ] = $(this);

            $(this).focusin(function(){
                $(this).removeClass('error');
            });
        });

        objMain.find('.js-input-file').change(function(){
            var input = this;
    
            if (input.files && input.files[0]) {
                objMain.find('.js-file-name').html(input.files[0]['name']).show();
            }
        });
    
        objForm.submit(function(event){
            event.preventDefault();

            if (v22.fIsProcessing() || !fCheckForm() || !v22.fSetProcessing(true)) {
                return;
            }

            var formData = new FormData(objForm[0]);

            formData.append('oper', 'save');

            $.ajax({
                url:         url,
                type:        'POST',
                dataType:    'json',
                data:        formData,
                processData: false,
                contentType: false,
    
                success: function (response) {
                    v22.fSetProcessing(false);
    
                    if (!response) {
                        fShowMessage('Ошибка 100');
                    } else {
                        fLog(response);
    
                        if (response.error_msg) {
                            var flag = false;
    
                            if (response.field_invalid) {
                                for (var fieldName in response.field_invalid) {
                                    if (arObjField[ fieldName ]) {
                                        arObjField[ fieldName ].addClass('error');
                                        flag = true;
                                    }
                                }
                            } //
    
                            if (response.showError || !flag) {
                                objErrorMsg.html(response.error_msg).show();
                            }
    
                        } else {
                            objForm.hide();
                            objMain.find('.step-2').show();
                            objMain.data('flagSent', 1);
                        } //
    
                        if (response.debug_info && response.debug_info.text) {
                            objMain.find('.debug_info').html(response.debug_info.text).show();
                        }
                    } //
                },
                error: function () {
                    v22.fSetProcessing(false);
                    fShowMessage('Нет соединения с сервером (код ошибки: 31)'); // # ajax error #
                }
            });
        }); // function

        function fCheckForm() {
            //return true;

            var result = true,
                value;
    
            objErrorMsg.html('').hide();
            objMain.find('.error').removeClass('error');

            value = arObjField.text.val().trim();
            if (!value.length) {
                arObjField.text.addClass('error');
                result = false;
            }

            if (arObjField.captcha_word) {
                value = arObjField.captcha_word.val().trim();
                if (!value.length) {
                    arObjField.captcha_word.addClass('error');
                    result = false;
                }
            }

            return result;
        } // function

    } //

} // function

/* Для рекламы */
window.sp_global.v22.fAdvertLabel = function() {
    $('.glbl-advt-label').mouseenter(function(){
      var objDetail = $(this).next('.glbl-advt-label-detail');

      $('.glbl-advt-label-detail').removeClass('active'); // Удаляем класс 'active' у всех блоков .glbl-advt-label-detail

      objDetail.addClass('active'); // Добавляем класс 'active' только к objDetail этого баннера
    });

    $('.glbl-advt-label').mouseleave(function(){
      var objDetail = $(this).next('.glbl-advt-label-detail'); // Получаем objDetail, связанный с этим баннером
      objDetail.removeClass('active');
  });
} // function
/* Для рекламы */

$(document).ready(function(){

    var fShowMessage = window.sp_global.fShowMessage;

    function fLog(msg, label) {
        window.sp_global.fLog(msg, label);
    }

    var v22 = window.sp_global.v22;

    if (1) {
        var obj = $('.js-data-main');

        try {
            if (!obj.length) throw 'js-data-main length';
            v22.dataMain = JSON.parse( obj.html() );

        } catch (e) {
            fShowMessage('Ошибка: 102 # main.js');
            return;
        }
        //fLog(v22.dataMain, 'v22.dataMain');
    } //

    v22.fInitMenuCatalog();
    v22.fInitMenuMobile();

    v22.objSelectCity.fInit();
    v22.fInitSelectCityMobile();
    v22.fInitSelectStore({source: 'headerModal', objMain: $('.js-block-store-selection--header-modal')});
    //$('.js-btn-store-selection').click();
    v22.fInitSelectStore({source: 'menuMobile', objMain: $('.js-block-store-selection--menu-mobile')});

    v22.objSearch.fInit();

    // Авторизация
    $('.js-btn-authorization').click(function(){
        v22.fAuthShowForm({form: 'authorization'});
    });
    // Регистрация
    $('.js-btn-registration').click(function(){
        v22.fAuthShowForm({form: 'registration'});
    });

    v22.fInitDialogOpen();

    // Открыть форму "Оставить отзыв" при нажатий на кнопку
    $('.js-btn-feedback').click(function(event){
        event.preventDefault();
        v22.fFeedbackShowForm();
    });

    v22.fAdvertLabel();

    // Открыть форму "Предложить тему" (Школа здоровья и красоты) при нажатий на кнопку
    $('.js-btn-propose-topic-school-of-beautify-and-health').click(function(event){
        event.preventDefault();
        v22.fProposeTopicSchoolOfBeautifyAndHealthShowForm();
    });
    
    if (0) {
        v22.favoriteProduct.fInit([
            '63431',
            '64942',
            '66673',
        ]);
    } //

    $('body').on('click', '.js-btn-wholesale-order', function() {
        $.fancybox.open(
            $('.sp_form__block'),
            {touch: false, autoFocus: false}
        );
    });
});

$.fancybox.defaults.touch     = false;
$.fancybox.defaults.autoFocus = false;
$.fancybox.defaults.baseClass = 'diolog-wrapper';

function spSetMessages(messagesObj)
{
	try
	{
		if(Object.keys(messagesObj).length <= 0)
		{
			return;
		}
	
		Object.keys(messagesObj).forEach(key => {
			let textWrapper = $(`[data-sp-message=${key}]`);

			if(textWrapper.length > 0)
			{
				textWrapper.html(messagesObj[key])
			}
		});
	}
	catch(error)
	{
		console.log(error);
	}
}

function createBalloonHtml(obj)
{
    return `
        <div class="map-balloon-content">
            <div class="image" style="background-image: url('${obj.imagePath}')"></div>
            <div class="info m-overflow-wrap">
                <div>
                    <div class="address">${obj.address}</div>
                    <div class="name">${obj.name}</div>
                    <div class="schedule">${obj.schedule}</div>
                </div>
            </div>
        </div>
    `;
}
