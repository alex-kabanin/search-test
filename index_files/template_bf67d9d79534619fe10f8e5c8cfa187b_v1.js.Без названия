                    for (
                        regexMask &&
                        ((opts.optionalmarker[0] = void 0),
                        (opts.optionalmarker[1] = void 0));
                        (match = regexMask
                            ? regexTokenizer.exec(mask)
                            : tokenizer.exec(mask));

                    ) {
                        if (((m = match[0]), regexMask))
                            switch (m.charAt(0)) {
                                case "?":
                                    m = "{0,1}";
                                    break;

                                case "+":
                                case "*":
                                    m = "{" + m + "}";
                                    break;

