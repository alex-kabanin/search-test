                                    closeGroup();
                                    break;

                                case opts.optionalmarker[0]:
                                    openenings.push(new MaskToken(!1, !0));
                                    break;

                                case opts.groupmarker[0]:
                                    openenings.push(new MaskToken(!0));
                                    break;

